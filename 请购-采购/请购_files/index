                    type: Number,
                    default: 0,
                    required: !0,
                    validator: function(e) {
                        return e >= 0 && e <= 100
                    }
                },
                status: {
                    type: String
                },
                strokeWidth: {
                    type: Number,
                    default: 6
                },
