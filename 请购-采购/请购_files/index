        //        }
        //    })
    },
    _messagesList: $("#messagesDiv"),
    getMessages: function () {
        this._messagesList.html("");
        var _this = this;
        if (parseInt(this._taskSize) <= 0) return;
        var size = this._taskSize;
        var status = $("[ulType='messages']").find("[class='select']").attr("liType");
        if (!status)
            status = "0";
        if (status == "0") {
            index = this._messageNotReadIndex;
        }
        else {
            index = this._messageAllIndex;
        }
        //$.get("../Main/Messages?Status=" + status + "&PageIndex=" + index + "&PageSize=" + size, { mt: Math.uuid() }, function (data) {
        //    _this._messagesList.html("");
        //    if (data && data.Messages) {
        //        _this._messageListForClick = data.Messages;
        //        var indexItems = 0;
        //        if (data.Messages.length != 0) {
        //            $.each(data.Messages, function (i, item) {
        //                var avatar = _this.getAvatarImages(item.SenderUserPhoto, item.SenderUserName)
        //                var li = "<li onclick='javascript:ServiceManager.openMessageShowDialog(" + i + ")' liindex=" + i + ">" + avatar + "<p class='list-con-news' title='" + item.Content + "'>" + item.Title + "</p><p class='list-con-name'>" + item.SenderUserName + "</p><p class='list-con-time'>" + item.SendDateTime + "</p><span class='list-con-detail'>查看详情</span></li>";
        //                _this._messagesList.append(li);
        //                indexItems = ((index - 1) * size) + 1 + i;;
        //            })
        //        } else {
        //            _this._messagesList.append("<li class='list-num-0'>" + ResourceDataManager.get("NoReadMessage") + "</li>");
        //        }
        //        $("#messageTotalItems").text(data.Total);
        //        $("#messageItemsIndex").text(((index - 1) * size + 1) + " - " + indexItems);
        //        PageTools.bindPage("messagesPageTool", index, size, data.Total);
        //    }
        //})

    },
    openMessageShowDialog: function (index) {
        var _this = this;
        index = parseInt(index);
        if (this._messageListForClick) {
            var mm = this._messageListForClick[index];
            _this.openMSgInfo(mm);

        }
    },
    openMSgInfo: function (msgInfo) {
        var _this = this;
        $.post("../Main/ViewMessage", { MessageStr: $.base64encode($.utf16to8(JSON.stringify(msgInfo))), mt: Math.uuid() }, function (data) {
            if (data) {
                var items = data.split(',');
                if (items.length >= 6) {
                    var fea = {
                        url: "../.." + items[0],
                        title: items[1],
                        width: items[3],
                        height: items[2],
                        isHideStatusbar: items[4] == "True" ? true : false,
                        isModeless: items[5] == "True" ? true : false
                    };
                    DocumentManager.showDialog(fea, callBackFunc);//===by zzq 改用zlayer弹出框
                    function callBackFunc() {
                        if ($("[ulType='messages']").find("[class='select']").attr("liType") == "0") {
                            $("#messagesDiv > li[liindex=" + index + "]").remove();
                            _this.getMessages();
                        }
                    }
                }
            }
        });
    },
    getCalendars: function () {
        if (parseInt(this._newsSize) > 0) {
            if ($("#calendarsDiv > li").length == 0) {
                $.get("../Main/Calendars?MaxAmount=" + this._newsSize, { mt: Math.uuid() }, function (data) {
                    $("#calendarsDiv").html("");
                    if (data && data.Calendars) {
                        $.each(data.Calendars, function (i, item) {
                            var li = "<li class='text-ellipsis' onclick='javascript:ServiceManager.openCalendarsItem('" + item.ID + "')'><a href='javascript:ServiceManager.openCalendarsItem('" + item.ID + "')' title='" + item.Title + "'>" + item.Title + "</a></span><span class='text-muted m-l-xs'>" + item.DTStart + "</span></li>";
                            $("#calendarsDiv").append(li);
                        })

                    }
                })
            }
        }
    },
    _taskList: $("#tasksDiv"),
    getTasks: function () {
        this._taskList.html("");
        var _this = this;
        var index = 1;
        var size = this._taskSize;
        if (parseInt(this._taskSize) <= 0) return;
        var status = $("[ulType='tasks']").find("[class='select']").attr("liType");
        if (!status)
            status = "0";
        if (status == "0") {
            index = _this._taskNotFinishIndex;
        }
        else {
            index = _this._taskAllIndex;
        }
        //$.get("../Main/Tasks?Status=" + status + "&PageIndex=" + index + "&PageSize=" + size, { mt: Math.uuid() }, function (data) {
        //    if (data && data.Tasks) {
        //        _this._taskListForClick = data.Tasks;
        //        var indexItems = 0;
        //        if (data.Tasks.length != 0) {
        //            $.each(data.Tasks, function (i, item) {
        //                var avatar = _this.getAvatarImages(item.SenderUserPhoto, item.SenderName);
        //                var li = "<li onclick='javascript:ServiceManager.openTaskShowDialog(" + i + ")' liindex=" + i + ">" + avatar + "<p class='list-con-news' title='" + item.Title + "'>" + item.Title + "</p><p class='list-con-name'>" + item.SenderName + "</p><p class='list-con-time'>" + item.CreatedOn + "</p><span class='list-con-detail'>查看详情</span></li>";
        //                _this._taskList.append(li);
        //                indexItems = ((index - 1) * size) + 1 + i;
        //            })
        //        } else {
        //            _this._taskList.append('<li class="list-num-0">' + ResourceDataManager.get("NoReadTask") + '</li>');
        //        }
        //        $("#taskTotalItems").text(data.Total);
        //        $("#taskItemsIndex").text(((index - 1) * size + 1) + "-" + indexItems);
        //        PageTools.bindPage("taskPageTool", index, size, data.Total);
        //    }
        //})
    },
    getAvatarImages:function(imageID,SenderUserName){
            var imgSrc="";
            if(imageID !="" && imageID){
                imgSrc='../../gridfs/id/'+imageID+'.jpg';
            }
            else if(SenderUserName=="Enterprise Administrator"){ //EA管理员头像
                imgSrc='../../css/images/ea-avatar.png';
            }else{
                imgSrc='../../css/images/avatar.png';
            }
            var avatar='<img code="'+SenderUserName+'" class="index-avatar"  src="'+imgSrc+'">'; 
            return avatar;
    },
    changeEnterprise: function (id) {
        $.get("../Main/EnterprisesChange", { EnterpriseID: id, mt: Math.uuid() }, function (data) {
            if (data == true) {
                window.location.reload();
            }
        })
    },
    changeOrg: function (id, name) {
        $.get("../Main/OrgChange", { OrgID: id, OrgName: name, mt: Math.uuid() }, function (data) {
            if (data == true) {
                window.location.reload();
            }
        })
    },
    changeLanguages: function (id) {
        $.get("../Main/LanguagesChange", { Language: id, mt: Math.uuid() }, function (data) {
            if (data == true) {
                window.location.reload();
            }
        })
    },
    changeThemes: function (id) {
        $.get("../Main/ThemesChange", { Theme: id, mt: Math.uuid() }, function (data) {
            if (data == true) {
                window.location.reload();
            }
            else {
                alert(ResourceDataManager.get("CantChangeTheme"));
            }
        })
    },
    getUnifyInfo: function () {
        var that=this;
        $.get("../Main/CSUnifyInfo", { mt: Math.uuid() }, function (data) {
            if (data) {
                try {
                    var dd = data.replace(/\\/g, ""); //去JSON字符串中的的 "\\"  -- weiyin -- 2016.07.04

                    dd = JSON.parse(dd);
                    if (dd) {
                        if (dd.MsgNum) {
                            var messageCountSpan = $("#messageCountSpan");
                            that.setDataOriginalTitle(messageCountSpan, dd.MsgNum);
                        }
                        if (dd.TaskNum) {
                            var taskCountSpan = $("#taskCountSpan");
                            that.setDataOriginalTitle(taskCountSpan, dd.TaskNum);
                        }
                        if (dd.CalendarNum) {
                            var calendarCountSpan = $("#calendarCountSpan");
                            that.setDataOriginalTitle(calendarCountSpan, dd.CalendarNum);
                        }
                        if (dd.BatchApprovalNum) {
                            var batchApprovaCountSpan = $("#batchApprovaCountSpan");
                            that.setDataOriginalTitle(batchApprovaCountSpan, dd.BatchApprovalNum);
                        }
                        //if (dd.BatchTracingNum) {
                        //    var workflowCountSpan = $("#workflowCountSpan");
                        //    that.setDataOriginalTitle(workflowCountSpan, dd.BatchTracingNum);
                        //}
                        if (dd.PostMsg) MsgBox.init(dd.PostMsg);
                    }
                }
                catch (e)
                {
                    console.log("getUnifyInfo方法返回了错误json格式");
                }
            }
        })
    },
    toReadMessage:function(){
        var that=this;
        /*$.get("../Main/CSUnifyInfo", { mt: Math.uuid() }, function (data) {
            if (data) {
                try {
                    var dd = data.replace(/\\/g, ""); //去JSON字符串中的的 "\\"  -- weiyin -- 2016.07.04
                    dd = JSON.parse(dd);
                    if (dd) {
                        if (dd.PostMsg) MsgBox.init(dd.PostMsg);
                    }
                }
                catch (e)
                {
                    console.log("getUnifyInfo方法返回了错误json格式");
                }
            }
        })*/
        var back_time = $("#UnreadMessageDuration").val();
        var time_cell = 120000;
        var getInfo = function(){
            $.get("../Main/GetUnReadMessage", { mt: Math.uuid() }, function (data) {
                if (data && data.data) {
                    MsgBox.init($.parseJSON(data.data),false);
                }
            })
        };
        getInfo();
        if(back_time&&back_time!=0){
            time_cell = back_time;
        }
        window.setInterval(function(){
            getInfo();
        },time_cell); 
    },
    setDataOriginalTitle:function(item,num){
            var title=item.attr("data-title");
            if(title){
               var dataOriginalTitle= title+"( "+num+" )";
                item.attr("data-original-title",dataOriginalTitle);
            }

    },
    getSearchCategorys: function () {
        //获取搜索分类列表
        $.get("../main/SearchCategorys", { mt: Math.uuid() }, function (data) {

            if (data && data.SearchCategorys) {
                $("#searchCategorysDiv").html("");
                $.each(data.SearchCategorys, function (i, item) {
                    $("#searchCategorysDiv").append('<li code="' + item.Key + '" text="' + item.Value + '">' + item.Value + '</li>')
                })
                var defalut = $("#searchCategorysDiv li")[0];
                $("#searchCategorysDiv").next(".selectDiv").text($(defalut).attr("text")).attr("code", $(defalut).attr("code"));
            }


        });
    },
    openCalendarsItem: function (id) {
        var url = "../../ufsoft/simple.aspx?lnk=d95dc158-9535-4cf9-9499-2bcef747f300&ShowType=ShowModal&ParentTaskID=e4e3eab3-7fd8-4dc7-b419-3f7d53169af7&Mode=Browse&ID=" + id + "&From=Portal&ShowAtlasModalDialog=true&UBF_Title=%E6%97%A5%E5%8E%86%E4%BA%8B%E4%BB%B6";
