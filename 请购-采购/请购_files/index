    openTranslate:function(){
        $(window.document).find(".adminUl .list-help").hide();
        $(window.document).find(".adminUl li").removeClass("hover");
        var id = TabPanelManager.getActiveTab().id;
        var frame = window.top.frames[id + "Frame"];
        if (id !='toolbarPlugin1'){
           translateTool.on(); 
        }else{
            alert("当前界面不支持翻译功能");
        }
    },
    logout: function () {
        TabPanelManager.disponse(function () {
            window.top.ServiceManager.logoutNoCheck.apply(window.top,[]);
        });
    },
    logoutNoCheck: function () {
        //实在是没有办法才这么写，想用相对路径，但是如果界面中弹出了路径为/ufsoft/saveconform.aspx之后，由这个界面点击是、否之后再次进入
        //这个回调中的时候，里面的相对路径就是相对于/ufsoft/saveconform.aspx路径的了。用window.top.location.href="../main/logout"，最终会跳转到/main/logout，而不是/mvc/main/logout。
        //如果用window.top.location.href="../mvc/main/logout"会导致在没有弹出/ufsoft/saveconform.aspx文件的时候页面跳转到/mvc/mvc/main/logout。因为这时对于/mvc/main/index页面来说前面的相对路径就是这个位置。
        //这两种写法只能兼顾一个情况。
        //因为相对路径是相对于当前页面的，/ufsoft/saveconform.aspx弹出后，点击是或者否虽然页面会关闭，但是因为用的zlayer，为了保证js顺利执行，关闭页面会在js执行之后，所以js执行的关闭回调的当前页面还是/ufsoft/saveconform.aspx
        //50就没有这个问题，因为在执行到回调的时候页面是真正的关闭了（window.showModalDialog)。
        //所以用了一个比较变态的方式，如下
        try {
            window.top.location.href = window.top.location.href.toLocaleLowerCase().replace("main/index","main/logout");
        }
        catch (e) {

        }
    }
}
/**
*翻译功能的操作集合
*/
var translateTool = {
    _translateElement: null,
    _frame:null,
    _frameWindow: null,
    _frameDocument: null,
    _zIndex: 9999,
    _status:'off',
    _id:null,
    init: function () {
        var _this = this;
        window.addEventListener("keydown", function(e){
            var code = e.which || e.keyCode;
            if(code == 27 && _this._status == "on"){
                _this.off();
            }
        });
    },
    on: function () {
        var _this = this;
        var id = TabPanelManager.getActiveTab().id;
        var frame = window.top.frames[id + "Frame"];
        var _window = this._frameWindow = $F(frame).getFrameWindow();
        var _document = this._frameDocument = $F(frame).getFrameDocument();
        var _$ = _window.$;
        if(frame&&_$){ //判断页面是否加载出来
            if (this._status == "on"){
                this.off();//防止重复点击，效果累加
            }
            this.bindEventToFrame();
        }else{
           return; 
        }
        
        _window.addEventListener("keydown", function(e){
            var code = e.which || e.keyCode;
            if(code == 27){
                _this.off();
            }
        });
    },
    off: function () {
        $("#myTranslate").hide();
        $(".portal-header .translateBg1").remove(); //清除遮罩层
        var _$ = this._frameWindow.$;
        var _window = this._frameWindow;
        var _this = this;
        _$.each(_$("[ick]"), function (i, item) {
            var ick = $(item).attr("ick");
            if (ick != "Panel") {
                var _c = _window.$find(_$(item).attr("id"));
                if (_c)
                    _c.to_Defualt();
            }
        });
        //删除页签遮罩层start
        $.each($(".tabpanel_mover li"), function (i, item) {
            var translateBg = $(item).find(".translateBg");
            if(translateBg){
               translateBg.unbind("click");
               translateBg.remove();
            }
        });
        $(".tabpanel_select_content .translateBg").remove();
        //删除页签遮罩层end
        this._status = 'off';
        this._id = null;
    },
    show: function (_c,origin_id,sub_id,flag) {
        if(flag == true){
            var _id = TabPanelManager.getActiveTab().id;
            var frame = window.top.frames[_id + "Frame"];
            if (frame) {
                var frameWindow = this._frameWindow = $F(frame).getFrameWindow();
                var frameDocument = this._frameDocument = $F(frame).getFrameDocument();
                var TabPanel = window.top.TabPanelManager.getActiveTab();
                var formid = this._frameWindow.$("#FormIDForHidden").val();
            }
        } else {
            var TabPanelSelect = window.top.TabPanelManager._tabpanel;
            var TabPanelIndex = TabPanelSelect.getTabIndexById(origin_id);
            var TabPanel = TabPanelSelect.tabs[TabPanelIndex];
            var frame = window.top.frames[origin_id + "Frame"];
            var frameWindow = $F(frame).getFrameWindow();
            var frameDocument = $F(frame).getFrameDocument();
            //对报表做特殊处理
            var formid = frameWindow.$&&frameWindow.$("#FormIDForHidden")?frameWindow.$("#FormIDForHidden").val():'';
        }
        var _this=this;
        var url = TabPanel.url;
        this._id = origin_id;

        if($("[labelId]").length > 0){  //这个地方如果判断有则先remove然后再重新添加
            $("[labelId]").closest(".layui-layer").remove();
        }
        var url = '../../mvc/resource/index?formId='+formid+'&controlId='+sub_id;
        var fea = {
            url: url,
            title: "翻译",
            width: 598,
            height: 340,
            isHideStatusbar: false,
            isModeless: false
        };
        DocumentManager.showDialog(fea,function(options){
            //{isFine = true, current = model.LaugValue[CSContext.Current.UICulture]}
            if(options&&options.isFine){
               var dom = $(frameDocument).find("#"+origin_id);
               if(dom.length == 0){
                 dom = $(frameDocument).find("[forid="+origin_id+"]");//为grid表头做特殊处理
               }
               _c.set_translateName(dom,options.current);    
            }
        });
    },
    bindEventToFrame: function () {
        var _this = this;
        var _window = this._frameWindow;
        var _document = this._frameDocument;
        var _$ = _window.$;
        var _index = _this._zIndex + 1;
        if(_$){
            $(".tabpanel_select_content").append("<div class='translateBg' z-index='" + _index + "'></div>");
            $(".portal-header .logo-bar").after("<div class='translateBg1'></div>");
            $("#myTranslate").show();
            _$.each(_$("[ick]"), function (i, item) {
                var ick = $(item).attr("ick");
                if (ick != "Panel" && $(item).css("display") != "none" && ick != "Menu" && ick !="Wizard") {
                    var _c = _window.$find(_$(item).attr("id"));
                    if (_c)
                        _c.to_Translate(_index, function (origin_id,sub_id) {
                            //显示翻译弹窗
                            _this.show(_c,origin_id,sub_id,true);
                        });
                }
            });
            //添加页签遮罩层start
            $.each($(".tabpanel_mover li"), function (i, item) { //页签支持知识弹窗
                var id = $(item).attr('id');
                var name = $(item).find(".title").text();
                $(item).append("<div class='translateBg' z-index='" + _index + "'></div>");
                if(i ==0){
                    $(item).find(".translateBg").click(function (e) {
                        e.preventDefault();
                        e.stopPropagation();
                    })
                }else{
                    $(item).find(".translateBg").click(function (e) {
                        //_this.show(id,false);
                        e.preventDefault();
                        e.stopPropagation();
                    })
                }
            });
            this._status = 'on';
            //添加页签遮罩层end
        }else{
            this._status = 'off';
        }
            
    }
};
/**
* 快捷菜单的操作集合
* Mahx
*/
var QuickMenuManager = {
    _quickMenusList: null,
    _quickMenuClickTime: [],
    init: function () {
        this.getQuickMenu();
        this.bindEvent();
    },
    getQuickMenu: function () {
        var _this = this;
        $.get("../Main/QuickMenuInfo", { mt: Math.uuid() }, function (data) {
            if (data && data.QuickMenus) {
                _this._quickMenusList = data.QuickMenus;
                _this.bindQuickMenu();
            }
        });
    },
    bindEvent: function () {
        var _this = this;

        //为快捷菜单管理按钮绑定点击事件，实现一点击弹出快捷菜单管理层。
        $("#favoriteManager").click(function () {
            _this.quickMenuManagerDivBind();
        });

        //为添加快捷菜单按钮绑定点击事件，实现一点击弹出编辑层功能
        $("#btnAddFavorite").bind("click", function () {
            var id = TabPanelManager.getActiveTab().id;
            var frame = window.frames[id + "Frame"];
            $("#btnAddFavorite").attr('data-toggle', '');
            //针对于导航条上的报表中心图标屏蔽再次添加到导航报错的问题 - WY
            var src = TabPanelManager.getActiveTab().url;
            var repeatNames = TabPanelManager.getActiveTab().title[0].innerHTML;
            var reportFlag = true;
            if (repeatNames == '报表中心') {
                reportFlag = false;
            }
            //针对于导航条上的报表中心图标屏蔽再次添加到导航报错的问题END

            if (frame && reportFlag) {
                var _frameWindow = HtmlHelper.getFrameWindow(frame);
                var _frameDocument = HtmlHelper.getFrameDocument(frame);
                var url = '';
                if(_frameWindow.GetRealUrl4Fav){
                    url = _frameWindow.GetRealUrl4Fav(src);
                    if (url.split("/display.aspx?").length > 1) {
                        url = "display.aspx?" + url.split("/display.aspx?")[1];
                    }
                }else{  //判断是新增节点的情况
                    var srcArr = src.split("/mvc/")[1].split("?");
                    url = srcArr[0] + '?' + srcArr[1];
                }
                $("#addToFavoritesDiv #favoriteUrl").val(url);
                $("#addToFavoritesDiv #favoriteTitle").val(_frameDocument.title);
                $("#addToFavoritesDiv #favoriteName").val(_frameDocument.title);

                $("#btnAddFavorite").attr('data-toggle', 'modal');
                //$("#addToFavoritesDiv").show();
                //$("#addToFavoritesDiv").modal('show');
            }

        });
        //为快捷菜单管理层中的确定按钮绑定事件，实现在对快捷菜单进行集中管理后，点确定保存到数据库。
        $("#favoritesManagerDiv  [name='btnOk']").click(function () {
            var list = new Array();
            var hasError = false;
            var message = "";
            var s = "";
            $.each($("#favoritesManagerUl > tr"), function (i, item) {
                var menu = {};
                menu.ID = i;
                menu.Name = $(item).find('[type="text"]').val();
                menu.Title = $(item).find('[type="text"]').attr("title");
                if (menu.Name.length == 0) {
                    hasError = true;
                    message = ResourceDataManager.get("QuickMenu_Name_NotNull");
                    return false;
                }
                if (s.indexOf(menu.Name + ";") > -1) {
                    hasError = true;
                    message = ResourceDataManager.get("QuickMenu_Name_Exist") + menu.Name;
                    return false;
                }
                menu.Uri = $(item).attr("menuUri");
                menu.IsShow = $(item).find('[type="checkbox"]').is(":checked") ? "True" : "False";
                s += menu.Name + ";";
                list.push(menu);
            });
            if (!hasError) {
                _this._quickMenusList = list;
                $.post("../main/QuickMenuManager", { QuickMenusStr: JSON.stringify(_this._quickMenusList) }, function (data) {
                    if (data) {
                        _this.bindQuickMenu();
                    }
                });
                $("#favoritesManagerDiv").modal('hide'); //隐藏弹出框
            }
            else {
                alert(message);
            }
        });

        //对添加快捷菜单层的确定按钮添加事件，新增快捷菜单。
        $("#addToFavoritesDiv  [name='btnOk']").click(function () {
            var name = $("#addToFavoritesDiv #favoriteName").val();
            var url = $("#addToFavoritesDiv #favoriteUrl").val();
            var title = $("#addToFavoritesDiv #favoriteTitle").val();
            if (!name) {
                DocumentManager.promptShow(ResourceDataManager.get("QuickMenu_Name_NotNull"));
                return;
            }
            var isHave = false;
            for (var i = 0; i < _this._quickMenusList.length; i++) {
                var item = _this._quickMenusList[i];
                if (item.Name == name) {
                    isHave = true;
                    break;
                }

            }
            if (isHave) {
                DocumentManager.promptShow(ResourceDataManager.get("QuickMenu_Name_Exist2"));
                return;
            }

            $.post("../main/AddQuickMenu", {
                Title: title,
                Name: name,
                Uri: url
            }, function (data) {
                if (data.Message) {
                    alert(data.Message);
                    return;
                }
                else {
                   if(/msie/.test(navigator.userAgent.toLowerCase())){  //true为IE核心 -- weiyin -- 2016.12.22               
                        name = Encoder.htmlEncode(name);
                        title = Encoder.htmlEncode(title);
                    }else{
                        name = Encoder.htmlDecode(name);
                        title = Encoder.htmlDecode(title);
                    }
                    _this._quickMenusList.push({ ID: _this._quickMenusList.length, Title: title, Uri: url, Name: name, IsShow: "True" });
                    _this.bindQuickMenu();
                    _this.clearFavoritesDiv();
                    $("#addToFavoritesDiv").modal("hide");
                }
            });
        });
        //对添加快捷菜单层的取消按钮添加事件，隐藏添加快捷菜单层。
        $("#addToFavoritesDiv #btnClose").click(function () {
            _this.clearFavoritesDiv();
        });

        //初始化编辑层中的快捷菜单的拖拽事件
        _this.dragsortQuickMenu();

    },
    quickMenuManagerDivBind: function () {
        $("#favoritesManagerUl").html("");
        if (this._quickMenusList) {
            $.each(this._quickMenusList, function (i, item) {
                var li = '<tr menuId="' + i + '" menuUri="' + item.Uri + '"><td><input type="checkbox" ';
                if (item.IsShow == "True")
                    li += 'checked ';
                li += 'menuId="' + i + '"/></td><td>' + item.Title
                    + '</td><td><input type="text" input-select="1" class="form-control-sm" title="' + item.Title + '" menuId="' + i + '" value="' + item.Name + '" /></td>'
                    + '<td class="oprate"><a  href="javascript:;" name="favoriteDeleteBtn" menuId="' + i + '" ><i class="fa fa-times-circle-o"></i></a></td></tr>';
                $("#favoritesManagerUl").append(li);
            });
            //$("#favoritesManagerDiv").show();
            $("#favoritesManagerUl").find('tr:first').addClass('select');
            this.oddRow("#favoritesManagerUl");
        }
    },
    oddRow: function (_this) {
        $(_this).find("tr:odd").addClass("oddRow");
        $(_this).find("tr:even").removeClass("oddRow");
    },
    bindQuickMenu: function () {
        $("#moreQuickMenuLi").hide();
        var isFull = false;
        $("#moreQuickMenuLi dl").html("");
        $("#quickMenuUl > [type='quickMenuItem']").remove();
        var parentWidth = $(document).width() - 280;
        var liTotalWidth = 0;
        if (this._quickMenusList) {
            $.each(this._quickMenusList, function (i, item) {

                if (item.IsShow == "True") {
                    liTotalWidth += (item.Name.length) * 12 + 16;
                    if (liTotalWidth > parentWidth && !isFull) {
                        isFull = true;
                        $("#moreQuickMenuLi").show();
                    }
                    if(item.Uri.indexOf(".aspx?") > -1){ //常用节点
                        if (!isFull) {
                            var li = "<li title='" + item.Title + "' id='" + i + "' type='quickMenuItem'><a href='javascript:QuickMenuManager.beforeQuickMenuClick(\"../../erp/" + item.Uri + "\",\"" + item.Title + "\"," + i + ")'>" + item.Name + "</a></li>";
                            $("#moreQuickMenuLi").before(li);
                        }
                        else {
                            var li = "<dd title='" + item.Title + "' id='" + i + "'><a href='javascript:QuickMenuManager.beforeQuickMenuClick(\"../../erp/" + item.Uri + "\",\"" + item.Title + "\"," + i + ")'>" + item.Name + "</a></dd>";
                            $("#moreQuickMenuLi dl").append(li);
                        }
                    }else{  //自定义的节点类型
                        if (!isFull) {
                            var li = "<li title='" + item.Title + "' id='" + i + "' type='quickMenuItem'><a href='javascript:QuickMenuManager.beforeQuickMenuClick(\"../" + item.Uri + "\",\"" + item.Title + "\"," + i + ")'>" + item.Name + "</a></li>";
                            $("#moreQuickMenuLi").before(li);
                        }
                        else {
                            var li = "<dd title='" + item.Title + "' id='" + i + "'><a href='javascript:QuickMenuManager.beforeQuickMenuClick(\"../" + item.Uri + "\",\"" + item.Title + "\"," + i + ")'>" + item.Name + "</a></dd>";
                            $("#moreQuickMenuLi dl").append(li);
                        }
                    }

                }

            })
        }
    },
    beforeQuickMenuClick: function (uri, name, index) {
        var itemQ = null;
        $.each(this._quickMenuClickTime, function (i, item) {
            if (item["index"] == index) {
                itemQ = item;
                return false;
            }
        });
        var curTime = (new Date()).getTime();
        if (!itemQ || curTime - itemQ["time"] > 2000) {
            if (!itemQ) {
                this._quickMenuClickTime.push({ "index": index, "time": curTime });
            }
            else {
                itemQ["time"] = curTime;
            }
            TabPanelManager.addUrlTab(uri, name);
        }
    },
    dragsortQuickMenu: function () {
        $("#favoritesManagerUl").dragsort({
            dragSelector: 'li',
            dragSelectorExclude: 'input, button, a, div,span',
            dragBetween: true,
            placeHolderTemplate: ' <li></li>',
            dragEnd: function () {
                var dd = $(this);
            }

        });
    },
    clearFavoritesDiv: function () {
        $("#addToFavoritesDiv").modal("hide");
        $("#addToFavoritesDiv #favoriteTitle").val("");
        $("#addToFavoritesDiv #favoriteName").val("");
        $("#addToFavoritesDiv #favoriteUrl").val("");
    }
}

/**
* 快速分页的操作集合
* Mahx
*/
var PageTools = {
    init: function () {
        this.bindEvent();
    },
    bindEvent: function () {

        var _this = this;
        //对页面分页的上一条下一条第一条最后一条实现方法
        $("[pageId]").click(function () {
            if($(this).hasClass("page-gray"))return;
            var size = 6;
            var totalPage = parseInt($(this).parent().attr("totalPage"));
            var divId = $(this).parent().attr("id");
            var type = "-";
            switch ($(this).attr("pageId")) {
                case "prevPage":
                    type = "-";
                    break;
                case "nextPage":
                    type = "+";
                    break;
                case "firstPage":
                    type = "--";
                    break;
                case "lastPage":
                    type = "++";
                    break;
                default:

            }
            _this.pageListChange(divId, totalPage, type);
        });
        //对页面分页的上一条下一条第一条最后一条实现方法
        $("[pageType]").click(function () {
            var size = 6;
            var totalPage = parseInt($(this).parent().attr("totalPage"));
            var divId = $(this).parent().attr("id");
            var type = $(this).attr("pageId") == "prevPage" ? "-" : "+";
            _this.pageListChange(divId, totalPage, type);
        });
        //点击页面获取当前页面的内容
        $(".page-ul").on("click", "[pageIndex]", function () {

            switch ($(this).parent().attr("id")) {
                case "messagesPageTool":
                    var status = $("[ulType='messages']").find("[class='select']").attr("liType");
                    if (status == "0") {
                        ServiceManager.setMessageNotReadIndex(parseInt($(this).text()));
                    }
                    else {
                        ServiceManager.setMessageAllIndex(parseInt($(this).text()));
                    }
                    ServiceManager.getMessages();
                    break;
                case "taskPageTool":
                    var status = $("[ulType='tasks']").find("[class='select']").attr("liType");
                    if (status == "0") {
                        ServiceManager.setTaskNotFinishIndex(parseInt($(this).text()));
                    }
                    else {
                        ServiceManager.setTaskAllIndex(parseInt($(this).text()));
                    }
                    ServiceManager.getTasks();
                    break;
                default:

            }
        })

    },
    pageListChange: function (divId, totalPage, type) {
        var pageTool = $("#" + divId);
        var size = 5;
        var indexArray = new Array();
        var index = 0;
        switch (type) {
            case "-":
                index = parseInt(pageTool.find("[pageId='prevPage']").next().attr("pageIndex"));
                if (index <= 1)
                    return;
                else {
                    var first = (parseInt((index - 1) / size) - 1) * size;
                    for (var i = 1; i <= size; i++) {
                        if (first + i <= totalPage)
                            indexArray.push(first + i);
                    }
                }
                break;
            case "+":
                index = parseInt(pageTool.find("[pageId='nextPage']").prev().attr("pageIndex"));
                if (index >= totalPage)
                    return;
                else {
                    var first = (parseInt((index - 1) / size) + 1) * size;
                    for (var i = 1; i <= size; i++) {
                        if (first + i <= totalPage)
                            indexArray.push(first + i);
                    }
                }
                break;
            case "--":
                index = parseInt(pageTool.find("[pageId='prevPage']").next().attr("pageIndex"));
                if (index <= 1)
                    return;
                else {
                    var first = 0;
                    for (var i = 1; i <= size; i++) {
                        if (first + i <= totalPage)
                            indexArray.push(first + i);
                    }
                }
                break;
            case "++":
                index = parseInt(pageTool.find("[pageId='nextPage']").prev().attr("pageIndex"));

                var first = parseInt((totalPage - 1) / size) * size;
                for (var i = 1; i <= size; i++) {
                    if (first + i <= totalPage)
                        indexArray.push(first + i);
                }

                break;
            default:
                break;

        }
        pageTool.find("[pageIndex]").remove();
        $.each(indexArray, function (i, item) {
            var li = "<li pageIndex='" + item + "'>" + item + "</li>";
            pageTool.find("[pageId='nextPage']").before(li);
        });
        pageTool.find("[pageId='prevPage']").next("[pageIndex]").click();
    },
    pageInit:function(pageUl,pageIndex,totalPage){
        var pageNum=5;
        var selectItem=$("#"+pageUl);
        var totalPage=Math.ceil(totalPage);//向上取整
        var firstPage=selectItem.children('[pageid="firstPage"]');
        var prevPage=selectItem.children('[pageid="prevPage"]');
        var nextPage=selectItem.children('[pageid="nextPage"]');
        var lastPage=selectItem.children('[pageid="lastPage"]');
        if(totalPage <=pageNum){
               firstPage.hide();
               prevPage.hide();
               nextPage.hide();
               lastPage.hide();
        }else{
                var remainder=(Math.ceil(totalPage / pageNum) -1 ) * pageNum;
                
                 if(pageIndex > pageNum){ //当前页大于pageNum 5时 
                          firstPage.show().removeClass("page-gray"); //显示
                          prevPage.show().removeClass("page-gray"); //显示
                }else{   //当前页小于pageNum 5时 
                          firstPage.addClass("page-gray"); 
                          prevPage.addClass("page-gray"); 
                }
                if(pageIndex > remainder ){ 
                           nextPage.addClass("page-gray");
                           lastPage.addClass("page-gray");
                }else{
                           nextPage.show().removeClass("page-gray");
                           lastPage.show().removeClass("page-gray");
                }
        }

    },
    bindPage: function (divId, index, size, total) {
        var pageTool = $("#" + divId);
        pageTool.find("[pageIndex]").remove();
        var totalPage = total % size == 0 ? total / size : total / size + 1;
        pageTool.attr("totalPage", totalPage);
        var fisrt = parseInt((index - 1) / 5) * 5;
        for (var i = fisrt + 1; i <= fisrt + 5; i++) {
            if (i <= totalPage) {
                var li = "<li pageIndex='" + i + "'>" + i + "</li>";
                if (i == index)
                    li = "<li pageIndex='" + i + "' class='select'>" + i + "</li>";
                pageTool.find("[pageId='nextPage']").before(li);
            }
        }
        this.pageInit(divId,index,totalPage-1);
    }
}


/**
* 消息弹层的的操作集合
* Mahx
*/
var MsgBox = {
    _data: [],
    _msgBox: {},
    _notify:null,
    _flag: false,
    getData: function () {
        if (this._data) { //数据不为空
            this.createLayout();
        }
    },
    createLayout: function () {
        var data = this._data;

        if (data && data.length > 0) {
            var msgNum = data.length;
            var layout = "";
            this._msgBox.html("");
            layout += '<div class="thA">'
                            + '<span class="closeBtn"></span>'
                            + ResourceDataManager.get("NewMessageWarn").format('<em>' + msgNum + '</em>')
                            + '</div>'
                            + '<div class="tbA">';
            for (var i = 0; i < msgNum; i++) {
                var txt = data[i].Content.length > 75 ? (data[i].Content.substring(0, 75) + "......") : data[i].Content;
                layout += '<div class="msgBox" data-id="' + data[i].ID + '" data-index="' + i + '">'
                              + '<div class="iTit">' + data[i].Name + '</div>'
                              + '<div class="iContent">'
                              + ''
                              + txt + '[<span class="msgMore">' + ResourceDataManager.get("ViewDetails") + '</span>]'
                              + '</div>'
                              + '<div class="msgInfo">'
                              + '<p class="rTips">'
                              + '<span class="time">' + data[i].SendOn + '</span>'
                              + '<span class="msgFlagRead">' + ResourceDataManager.get("IKnow") + '</span>'
                              + '<span class="msgDelete">' + ResourceDataManager.get("Delete") + '</span>'
                              + '</p>'
                              + '<span class="iName" title="'+data[i].SendBy+'">' + data[i].SendBy + '</span>'
                              + '</div>'
                              + '</div>';
            }
            layout += '</div>';
            this._msgBox.append(layout);
            this.bindEvent();
            this.show();

        }
        else {
            this._msgBox.html("");
            this.hide();
        }
    },
    remove: function (id) {
        var index = 0;
        for (var i = 0; i < this._data.length; i++) {
            if (this._data[i].ID == id) {
                index = i;
                break;
            }
        }
        this._data.splice(index, 1);
        this.createLayout();
    },
    show: function () {
        this.layout();
        this._msgBox.show();
    },
    hide: function () {
        this._msgBox.hide();
    },
    bindEvent: function () {
        var data = this._data;
        var that = this;
        //我知道了
        that._msgBox.find(".msgFlagRead").on("click", function () {
            var id = $(this).parents(".msgBox").attr("data-id");
            $.post("../main/SetMessageReaded", { mt: Math.uuid(), ID: id }, function (dd) {
                if (dd) {
                    that.remove(id);
                }
            })
        })

        // 查看详情
        that._msgBox.find(".msgMore").on("click", function () {
            var _index = $(this).parents(".msgBox").attr("data-index");
            //收集数据
            // var ID = $(this).parents(".msgBox").attr("data-id");
            var ID = data[_index].ID;
            var Title = data[_index].Name;
            var SenderUserName = data[_index].SendBy;
            var SendDateTime = data[_index].SendOn;
            var SourceEntityID = data[_index].SourceEntityTypeID;
            var SourceEntityType = data[_index].SourceEntityType;
            var URI = data[_index].URI;
            var SourceOrg = data[_index].SourceOrg;
            var ImplementType = data[_index].ImplementType;
            var Content = data[_index].Content;

            var msgsData = {
                "ID": ID,
                "Title": Title,
                "SenderUserName": SenderUserName,
                "SendDateTime": SendDateTime,
                "SourceEntityID": SourceEntityID,
                "SourceEntityType": SourceEntityType,
                "URI": URI,
                "SourceOrg": SourceOrg,
                "ImplementType": ImplementType,
                "Content": Content
            }

            ServiceManager.openMSgInfo(msgsData);

            that.remove(ID);  //消失当前这条信息（已读）
            // $.ajax({
            //     url: "../../simple.aspx?lnk=49f34ff5-02d5-46cb-8709-3d9846b67e71",
            //     data: { Mode: "Browse", ID: id }
            // });
        })

        //删除
        that._msgBox.find(".msgDelete").on("click", function () {
            var id = $(this).parents(".msgBox").attr("data-id");
            $.post("../main/DeleteMessage", { mt: Math.uuid(), ID: id }, function (dd) {
                if (dd) {
                    that.remove(id);
                }
            })
        })

        // 关闭弹层
        $(".closeBtn").on("click", function () {
            that.hide();
        })

    },
    layout: function () {
        var _boxH = this._msgBox.find(".msgBox").height() + 35;
        if (this._data.length <= 1) {  //判断信息长度为1时的弹层的高度
            this._msgBox.css({ "height": _boxH });
        } else {  //判断信息长度大于1时的弹层的高度
            this._msgBox.find(".tbA").css({ "height": 278 });
        }
        var divTop, divLeft, divWidth, divHeight, docHeight, docWidth;
        var me = this._msgBox;
        divWidth = parseInt(me.outerWidth(), 10);
        divHeight = parseInt(me.outerHeight(), 10);
        //docHeight = window.screen.availHeight - 60;
        docHeight = document.body.offsetHeight - 5;
        var st = parseInt(document.body.scrollTop, 10) || parseInt(document.documentElement.scrollTop, 10);
        var sl = parseInt(document.body.scrollLeft, 10) || parseInt(document.documentElement.scrollLeft, 10);
        me.css("top", docHeight - divHeight + st - 2);

        // me.css("left", docWidth - divWidth + sl - 2);                           
    },
    init: function (list,flag) {
        var _this = this;
        this._msgBox = $("#rbBox");
        this._notify = $("#myErrorNotify");
        this._notifyCheck = $("#myErrorNotify>b");
        this._data = list;
        this.getData();
        if (flag) {
            _this.showNotify();
        }
    },
    showNotify:function(){
        var _this = this;
        if (window.checkMessages.toLowerCase()=="true") {
            $.get("../main/GetCheckMessages", { mt: Math.uuid() }, function (dd) {
                if (dd) {
                    for (var i = 0; i < dd.length; i++) {
                        _this.errorNotify(dd[i]);
                    }
                }
            })
        }
    },
    errorNotify: function (txt) {
        var _this = this;
        var html = '<li>'+
        '<b>×</b>'+
        '<i class="iconfont y-icon-warn"></i><p>'+txt+'</p></li>'
        _this._notify.show().find(".myPromptContent").append(html);

        if(_this._notify.find(".myPromptContent li").length <=1){
            _this._notify.removeClass("active");
            _this._notifyCheck.hide();
        }else{
            _this._notify.addClass("active");
            _this._notifyCheck.show();
        }
        _this._notify.show().find(".myPromptContent li b").click(function() {
            $(this).parent("li").remove();
            var info_length = _this._notify.show().find(".myPromptContent li").length;
            if( info_length == 0){
                _this._notify.hide();
            }
        })
    }
}

/**
* 我的消息的操作集合
* Mahx
*/
var MyMsg = {
    code: "code", // 编码
    Names: "Names", //名称
    FirstName: "FirstName",  //名
    LastName: "LastName", //姓
    betweenName: "betweenName", //中间名
    otherName: "otherName", //昵称
    newTitle: "newTitle", //头衔
    Department: "Department", //部门
    telNum: "telNum",  //电话
    MobilNum: "MobilNum", //移动电话
    EmailAddr: "EmailAddr", //邮箱地址
    msg: "msg",
    flag: false,
    proID: "proID",  //隐藏域ID
    PhoneID: "PhoneID",  //隐藏域PhoneID
    MobilID: "MobilID",  //隐藏域MobilID
    EmailID: "EmailID",  //隐藏域EmailID
    IsAdd: "IsAdd",//是否新增 
    getDate: function () {  //取接口中数据
        var _this = this;
        $.getJSON("../main/MyInfo", { mt: Math.uuid() }, function (data) {
            //console.log(data);
            $("#" + _this.code).val(data.Code);
            $("#" + _this.Names).val(data.Name);
            $("#" + _this.FirstName).val(data.PersonName_FirstName);
            $("#" + _this.LastName).val(data.PersonName_LastName);
            $("#" + _this.betweenName).val(data.PersonName_MiddleName);
            $("#" + _this.otherName).val(data.PersonName_NickName);
            $("#" + _this.newTitle).val(data.Title);
            $("#" + _this.Department).val(data.Department);
            $("#" + _this.telNum).val(data.PhoneNum);
            $("#" + _this.MobilNum).val(data.MobilNum);
            $("#" + _this.EmailAddr).val(data.EmailAddr);
            $("#" + _this.proID).val(data.ID);
            $("#" + _this.PhoneID).val(data.PhoneID);
            $("#" + _this.MobilID).val(data.MobilID);
            $("#" + _this.EmailID).val(data.EmailID);
            $("#" + _this.IsAdd).val(data.IsAdd);
             $("#msg").modal({
                show:true,
                backdrop:'static' //点击空白处不关闭
            });
             AvatarFaciesManager.GetInstance().hideAvatarBoxInit();//头像
        })
    },
    bindEvent: function () {
        var that = this;
        //点击取消按钮
        $(".btnSure").on('click', function (event) {
            that.btnSure();
        })
        //点击取消按钮
        $(".btnNo").on('click', function (event) {

            $(".of-h input:text").each(function () {
                $(this).val("");
            })

        })

    },
    validateData: function () {
        if ($("#" + this.code).val() == '') {
            alert(ResourceDataManager.get("Code_NotNull"));
            flag = false;
            return false;
        } else if ($("#" + this.telNum).val() == '') {
            alert(ResourceDataManager.get("TelePhone_NotNull"));
            flag = false;
            return false;
        } else if ($("#" + this.MobilNum).val() == '') {
            alert(ResourceDataManager.get("Phone_NotNull"));
            flag = false;
            return false;
        } else if ($("#" + this.EmailAddr).val() == '') {
            alert(ResourceDataManager.get("Email_NotNull"));
            flag = false;
            return false;
        } else {
            flag = true;
        }
    },
    btnSure: function () {  //确定按钮
        this.validateData();
        var msgData = '';
        var _this = this;
        if (flag) {  //此处必选字段都已填写
            // alert("sdsd");
            var code = $("#" + _this.code).val();
            var Names = $("#" + _this.Names).val();
            var FirstName = $("#" + _this.FirstName).val();
            var LastName = $("#" + _this.LastName).val();
            var betweenName = $("#" + _this.betweenName).val();
            var otherName = $("#" + _this.otherName).val();
            var newTitle = $("#" + _this.newTitle).val();
            var Department = $("#" + _this.Department).val();
            var sexItem;
            var telNum = $("#" + _this.telNum).val();
            var MobilNum = $("#" + _this.MobilNum).val();
            var EmailAddr = $("#" + _this.EmailAddr).val();
            var ID = $("#" + _this.proID).val();
            var PhoneID = $("#" + _this.PhoneID).val();
            var MobilID = $("#" + _this.MobilID).val();
            var EmailID = $("#" + _this.EmailID).val();
            var IsAdd = $("#" + _this.IsAdd).val();
            //获取单选对应的值
            $(".sex").find("input:radio").each(function (i) {
                if ($(this).prop("checked")) {
                    sexItem = $(this).attr("value");
                }
            });

            // msgData = "Code:" + code + ",Name:" + Names + ",PersonName_FirstName:+" + FirstName + ",PersonName_LastName:" + LastName +",PersonName_MiddleName:"+ betweenName +",PersonName_NickName:"+ otherName + ",Title:"+ newTitle + ",Department:"+ Department +",Gender:"+ sexItem +",PhoneNum:"+ telNum +",MobilNum:" + MobilNum +",EmailAddr:"+ EmailAddr + ",mt:"+Math.uuid();
            msgData = {
                "Code": code,
                "Name": Names,
                "PersonName_FirstName": FirstName,
                "PersonName_LastName": LastName,
                "PersonName_MiddleName": betweenName,
                "PersonName_NickName": otherName,
                "Title": newTitle,
                "Department": Department,
                "Gender": sexItem,
                "PhoneNum": telNum,
                "MobilNum": MobilNum,
                "EmailAddr": EmailAddr,
                "ID": ID,
                "PhoneID": PhoneID,
                "MobilID": MobilID,
                "EmailID": EmailID,
                "IsAdd": IsAdd
            };

            //批量提交数据
            $.post("../main/ModifyMyInfo", { MyInfoString: JSON.stringify(msgData), mt: Math.uuid() }, function (data) {
                $(".btnNo").click();
                $("#msg").modal("hide");
            })
        }
    },
    init: function () {  //初始化
        this.bindEvent();
    }
}

/**
* Winow与Document操作集合
* Mahx
*/

var DocumentManager = {
    init: function () {
        this.resizeHeight(); //--之前是注释的，放开是因为lqy测试要用，后续再注释起来--20180301
        this.eventBind();
        this.designStyle();
        this.bindEvents();
    },
     bindEvents:function(){
            this.OnResize();
    },
    OnResize:function(){
            var that=this;
            var timer = 0;
            $(window).resize(function(){
                  clearTimeout(timer);
                  timer = setTimeout(function() {
                         that.resizeHeight();
                  }, 200);
            });
    },
    designStyle: function(){
        //ea权限登录下拉列表新增自定义登录页选项 -- 20170607
        if($("#userNameLi").text().toLowerCase() == "enterprise administrator" || $("#userNameLi").text().toLowerCase().indexOf("enterprise administrator") > -1){
            var AddLink = "<a href='../../mvc/changestyle/index' target='_blank'>"+ ResourceDataManager.get('designStyle') +"</a>";
            $("#designStyle").append(AddLink).show();
        }
    },
    promptShow: function (m) {
        $('[prompt="true"]').slideDown('fast').html(m);
    },
    promptHide: function () {
        $('[prompt="true"]:visible').hide();
    },
    addMoreItems: function () {
        var _this = this;
        var $conLeft = $("#portal-con-left");
        //点击折叠时
        $conLeft.find("div[zd-toggle]").on('click', function () {
            var $conArea = $(this).parents('.con-area');
            $conArea.find(".fa").toggle();
            $conArea.find(".list-con").toggle();
            _this.resizeHeight();
        })

    },
    enterSumbit: function () {    //响应回车事件
        var event = arguments.callee.caller.arguments[0] || window.event; //消除浏览器差异  
        if (event.keyCode == 13) {
            $(".fa-search-2").click();
        }
    },
    allSearch: function () {
        var event = arguments.callee.caller.arguments[0] || window.event; //消除浏览器差异  
        if (event.keyCode == 13) {
            $(".header-search-btn").click();
        }
    },
    txtHide: function () {
        $(".search-input").keyup(function () {
            if ($(this).val() == '') {
                $(".result").addClass("hidden");
            }
            else {
                // $(".fa-search-2").click();
            }
        })
    },
    showDialog: function (features, func) {//===by zzq 改变首页弹出框方式
        //准备弹出，从opt中提取参数，有可能被改变
        //var isModeless = features.isModeless;
        var title = features.title;
        var width = features.width;
        var height = features.height;
        //var isHideStatusbar = features.isHideStatusbar; //弹出的Form是否显示状态条
        var url = features.url;
        if (url.indexOf("?") != -1) {
            if (url.indexOf("UBF_Title=") == -1) {
                url += "&UBF_Title=" + encodeURIComponent(title);
            }
        } else {
            url += "?UBF_Title=" + encodeURIComponent(title);
        }

        var hideStatusBar = features.isHideStatusbar;
        var isModelessDialog = features.isModeless;
        var winHeight = parseInt(height, 10);
        if (isNaN(winHeight)) {
            winHeight = 418;
        }
        /*if (!hideStatusBar) {
            winHeight += 18;
        }
        if (navigator.userAgent.indexOf("MSIE 7") > 0
        || navigator.userAgent.indexOf("MSIE 8") > 0) {
            winHeight += 11;
        }
        else {
            winHeight += 56;
        }*/
        var winWidth = parseInt(width, 10);
        if (isNaN(winWidth)) {
            winWidth = 590;
        }
        else {
            winWidth += 5;
        }
        winWidth += 12;
        if (title) {
            window.ufTitle = title;
        }
        window.hideStatusBar = hideStatusBar;
        var revt;
        _features = "edge: Raised; center: Yes; help: No; resizable:No; status: No;scrollBars:No;dialogWidth:" + winWidth + "px;dialogHeight:" + winHeight + "px";

        //$('#loadPage').show();
        //setTimeout("$('#loadPage').hide()", 1);
        var _args;
        if (isModelessDialog) {
            _args = null;
            //revt = window.showModalDialog(url, null, _features);
        } else {
            _args = window;
            //revt = window.showModalDialog(url, window, _features);
        }
        if (url.indexOf("../../") < 0) {
            url = "../../" + url;
        }
        zlayer.open({
            title: title,
            area: [winWidth + "px", winHeight + "px"],
            argument: _args,
            content: url,//以mvc参照，simple需要添加相对路径
            endHandle: function (data) {
                if (window.clearUIState) {
                    //alert(window.clearUIState);
                    if (window.$) {
                        $.get(window.clearUIState);
                    }
                    window.clearUIState = null;
                }
                window.Escaped = null;
                if (func)
                    func(data);
            }
        });
        return;
        //if (window.clearUIState) {
        //    //alert(window.clearUIState);
        //    if (window.$) {
        //        $.get(window.clearUIState);
        //    }
        //    window.clearUIState = null;
        //}
        //window.Escaped = null;
        //return revt;
        //window.showModalDialog(url, window, "dialogHeight:" + winWidth + "px; dialogWidth: " + winHeight + "px; edge: Raised; center: Yes; help: No; resizable:No; status: No;scrollBars:No;");
    },
    liceseKeyDown: function (frameWindow) {
        var win = frameWindow || window.top;
        if (win.event.keyCode == 116) {//截获F5刷新只刷新当前单据
            var id = TabPanelManager.getActiveTab().id;
            var frame = win.top.frames[id + "Frame"];
            if (frame) {
                // frame.location.reload();
                $F(frame).getFrameWindow().location.href = $F(frame).getFrameWindow().location.href;
            }
            else {
                DocumentManager.refreshIndex();
            }
            win.event.keyCode = 0;
            win.event.returnValue = false;
            win.event.cancelBubble = true;
            return false;
        }
        else if (win.event.keyCode == 118) {
            var id = TabPanelManager.getActiveTab().id;
            var frame = win.top.frames[id + "Frame"];
            if (frame) {
                var helpID = "";
                var _frameWindow = HtmlHelper.getFrameWindow(frame);
                if (_frameWindow.url2HelpID_Custom != undefined) {
                    helpID = _frameWindow.url2HelpID_Custom();
                } else {
                    helpID = _frameWindow.url2HelpID();
                }
                _frameWindow.FMCOpenHelp(helpID, null, null, null);
            }
            win.event.keyCode = 0;
            win.event.returnValue = false;
            win.event.cancelBubble = true;
            return false;
        }
    },
    showPasswordDiv: function () {
        $('#passwordModifyDiv').show();
    },
    clearPasswordDiv: function () {
        $("#passwordModifyDiv").modal("hide")
        $("#passwordModifyDiv #oldPassword").val("");
        $("#passwordModifyDiv #newPassword").val("");
        $("#passwordModifyDiv #comfirmPassword").val("");
    },

    showYHTPasswordDiv: function () {
        var needPwd = $("#isNeedPwd").val();
        if (needPwd == "False") {
            $("#yhtPasswordCheckDiv").modal("hide");
            $.get("../yht/AccessYHT", { mt: Math.uuid() }, function (data) {
                if (data) {
                    if (data.StatusCode == 200) {  //状态200
                        //if (data.IsShowYHT) {
                        //    $(".logo1").removeClass("hidden");
                        //}
                        if (data.U9YHTUrl) {
                            //$(".yht").removeClass("hidden");
                            //$(".yht").on('click', function () {
                            window.open(data.U9YHTUrl, "_blank");
                            //})
                        }
                    }
                }
            });
        } else {
            $('#yhtPasswordCheckDiv').show();  
            setTimeout(function() {
                $("#yhtPassword").focus();
             }, 200);
        }
    },
    clearYHTPasswordDiv: function () {
        $("#yhtPasswordCheckDiv").modal("hide");
        $("#yhtPasswordCheckDiv #yhtPassword").val("");
    },
    refreshIndex: function () {
        ServiceManager.init();
    },
    portalConLeft:"#portal-con-left",
    portalConRight:"#portal-con-right",
    cssListCon:".list-con",
    cssListConUl:".list-con-ul",
    resizeHeight: function () {
        //求list-con高度,并赋值,js修改为css3 calc表达式
        /*var listConHeight=this.getConHeight($(this.cssListCon).eq(0));
        $(this.cssListCon).outerHeight(listConHeight);*/
        //求list-con-ul高度，并赋值 ,js修改为css3 calc表达式
        var listConUlHeight=this.getConHeight($(this.cssListConUl+":first",this.portalConLeft));
        //$(this.cssListConUl).outerHeight(listConUlHeight);
        //左侧竖排显示个数
        //左侧行高
        var leftListLineHeight=74;
        //左侧横排显示的个数
        var leftLiNumForHorizontal=this.getLeftLiNumForHorizontal();
        var leftLiNumForVertical=parseInt(listConUlHeight / leftListLineHeight) * leftLiNumForHorizontal;
        ServiceManager.setTaskSize(leftLiNumForVertical);
        //给右侧的内容区域块计算高度
        //左侧竖排显示个数
        //右侧行高
        var rightListLineHeight = 26;  //行高 
        var rightLiNumOnVertical=parseInt(listConUlHeight / rightListLineHeight);
        ServiceManager.setMessageSize(rightLiNumOnVertical)
        ServiceManager.setNewsSize(rightLiNumOnVertical);
        //渲染任务
        ServiceManager.getTasks();
        //渲染信息
        ServiceManager.getMessages();
        
    },
    singleDivShow: function (objID) {
        //求list-con高度,并赋值
        var portalConLeft = $("#" + objID).closest("#portal-con-left");
        $("#" + objID).closest(".con-area").hide();
        var leftListCon = $("#" + objID).closest(".con-area").siblings(".con-area");
        leftListCon.css("height", "100%");
        //js高度计算修改为css3 calc表达式
        /*var listConHeight = this.getConHeight(leftListCon);
        $(portalConLeft).find(this.cssListCon).outerHeight(listConHeight);*/
        //求list-con-ul高度，并赋值
        listConUlHeight = this.getConHeight(leftListCon.find(this.cssListConUl));
         //js高度计算修改为css3 calc表达式
        //$(portalConLeft).find(this.cssListConUl).outerHeight(listConUlHeight);
        //左侧竖排显示个数
        //左侧行高
        var leftListLineHeight = 74;
        //左侧横排显示的个数
        var leftLiNumForHorizontal = this.getLeftLiNumForHorizontal();
        var leftLiNumForVertical = parseInt(listConUlHeight / leftListLineHeight) * leftLiNumForHorizontal;
        ServiceManager.setTaskSize(leftLiNumForVertical);

        if (objID == 'messagesDiv') {
            //渲染任务
            ServiceManager.getTasks();
        } else if (objID == 'tasksDiv') {
            //渲染信息
            ServiceManager.getMessages();
        }
        
    },
    //左侧横排显示的个数
    getLeftLiNumForHorizontal:function(){
         //左侧横排显示的个数 
         var leftLiNumForHorizontal=parseInt($(this.portalConLeft).width() / $(this.portalConRight).width());
         $(this.cssListConUl,this.portalConLeft).addClass("list-con-li-"+leftLiNumForHorizontal);
         return    leftLiNumForHorizontal;
    },
    getConHeight:function(item){
        var conHeight;
        if ($(item.siblings()).is(":hidden")) {
            conHeight = item.height() - 34;
        } else {
            conHeight = item.parent().height() - item.siblings().outerHeight(true);
        }
          return conHeight;
    },
    eventBind: function () {
        var _this = this;
        $(".search-input").on("input propertychange", function () {
            if ($(this).val().length >= 2) {
                $(".fa-search-2").click();
            }
        })

        $(".header-search-btn").click(function () {
            var text = $(".header-search-text").val();
            var type = $("#searchCategorysDiv").next(".selectDiv").attr("code");

            var url = "../../erp/display.aspx?lnk=CS.Search.SearchUI&D=" + type + "&sId=3071nid&T=&Key=" + encodeURIComponent(text);  //weiyin 添加企业搜索许可参数 -- 2016.06.27
            TabPanelManager.addUrlTab(url, '企业搜索');
        });


        //修改密码的确定按钮，将修改同步到数据库
        $("#passwordModifyDiv  [name='btnOk']").click(function () {
            var oldP = $("#passwordModifyDiv #oldPassword").val();
            var newP = $("#passwordModifyDiv #newPassword").val();
            var comfirmP = $("#passwordModifyDiv #comfirmPassword").val();
            if (newP != comfirmP) {
                _this.promptShow(ResourceDataManager.get("Password_NotEquels"));
                return;
            }
            $.post("../main/ModifyPassword", {
                OldPwd: oldP,
                NewPwd: newP, mt: Math.uuid()
            }, function (data) {
                if (data) {
                    _this.promptShow(data);
                }
                else {
                    _this.clearPasswordDiv();
                }
            });
        });
        $(".modal-content .form-group> input").focus(function (event) {
            _this.promptHide();
        });
        //修改密码的取消按钮
        $("#passwordModifyDiv  [name='btnCancel']").click(function () {
            _this.clearPasswordDiv();
        });

        //页面收缩，自动变化查询条数
        _this.addMoreItems();

        //根据友户通密码获取免登录token访问云合同
        function CheckYHTPwd() {
            var content = $("#yhtPasswordCheckDiv #yhtPassword").val();
            $.get("../yht/GetAccessToken", { pwd: content, mt: Math.uuid() }, function (data) {
                if (data) {
                    if (data.status === "1") {
                        _this.clearYHTPasswordDiv();
                        $("#isNeedPwd").val("False");
                        $("[data-target='#yhtPasswordCheckDiv']").removeAttr("data-target").removeAttr("data-toggle");
                        window.open(data.U9YHTUrl, "_blank");
                    } else {
                        _this.promptShow(data.msg);
                    }
                }
            });
        };

        $("#yhtPasswordCheckDiv  [name='btnOk']").click(function(){CheckYHTPwd()});

        $(".modal-content .form-group> input").keydown(function (event) {
            _this.promptHide();
            var event=event||window.event;
            event.preventDefault;
            if(event.keyCode==13)
            {
                CheckYHTPwd();
            }
        });
        //取消按钮
        $("#yhtPasswordCheckDiv  [name='btnCancel']").click(function () {
            _this.clearYHTPasswordDiv();
        });
    }

}


// 错误信息展示
var errorMsgShow = function(){
    this.hubW = null;
    this.hubH = null;
    this.errorType = null;
    this.errorKey = null;
    this.changePages = 1;
    this.changeStatus = true;
    this.currentZindex = null;
}
errorMsgShow.prototype.checkType = function(div, entityID, entityFullName,animShow){
    var _this = this;
    var errorSpan = document.createElement("span");
    var stack;
    this.errorTxtMsg(div,entityID,entityFullName);
    errorSpan.innerHTML = this.errorType;
    // errorSpan.innerHTML = div.cusAttr('msg');
    if($(errorSpan).find("p").length > 0){  
        stack = $(errorSpan).find("p").html();
        $(errorSpan).find("p").remove();
        $(errorSpan).find("span").remove(); 
    }
    _this.errorKey = $(errorSpan).text();
    var TabPanelId = window.top.TabPanelManager.getActiveTab().id;
    
    if($("[targetid="+ TabPanelId +"]").length > 0){
        var errorSame = null;
        if($("[targetid="+ TabPanelId +"]").find('.error-page')[0]){
            errorSame = $("[targetid="+ TabPanelId +"]").find('.error-page')[0].innerHTML;
        }
        if($("[targetid="+ TabPanelId +"]").find('.errorTxt')[0]){
            errorSame = $("[targetid="+ TabPanelId +"]").find('.errorTxt')[0].innerHTML;
        }
        var errorSpanText = this.errorType;
        errorSpanText = errorSpanText.substring(0,4+errorSpanText.indexOf('\<\/p\>'));
        var errorSameText = errorSame.substring(0,4+errorSame.indexOf('\<\/p\>'));
        if(errorSpanText == errorSameText){
            var errorMsgId = $("[targetid="+ TabPanelId +"]")[0].id;
            var errorBoxW = $("[targetid="+ TabPanelId +"]").attr("errorBoxW");
            var errorBoxH = $("[targetid="+ TabPanelId +"]").attr("errorBoxH");
            // var errorTitle = TabPanelManager.getActiveTab().title[0].innerHTML;
            _this.openDiv(errorMsgId, errorBoxW, errorBoxH, animShow);
        }else{
            _this.getData(_this.errorKey, animShow,stack);
        }
    }else{
        _this.getData(_this.errorKey, animShow,stack);
    }
}

errorMsgShow.prototype.getData = function (errorKey, animShow,stack) {
    var _this = this;
    var TabPanel = window.top.TabPanelManager.getActiveTab();
    var frame = window.frames[TabPanel.id + "Frame"];
    var url = TabPanel.url;  
    // var formId = HtmlHelper.getFrameWindow(frame).$("#FormIDForHidden").val();
    var formId = HtmlHelper.getFrameWindow(frame).document.getElementById("FormIDForHidden");
    var formIdVal = '';
    if (formId) {
        formIdVal = formId.value;
    }

    //u9hub错误层的载入前的loading...等待动画
    if(animShow){
        $("#errorLoad").css({
            top: '106px',
            left: parseInt($("#" + animShow.frameId).offset().left) + parseInt(animShow.left) + 25 + 'px'
        }).show();
    }else{
        var loadingStatus = '<div class="bgLoading" style="background:#ccc;opacity:0.3;position:fixed; left:0; top:0; width:100%;height:100%;z-index:999;"></div>';
        $('body').append(loadingStatus);
        $("#errorLoad").css({
            top: '50%',
            left: '50%',
            marginLeft:"-50px",
            marginTop:"-42px"
        }).show();
    }

    $.post('../error/SearchInfoByKey', { key: errorKey.slice(0, 200), page: 1, pagesize: 3, formId: formIdVal, url: encodeURI(url) ,stack:stack}, function (data) {
        //弹出大层
        $("#errorLoad").hide(100);
        $(".bgLoading").remove();
        _this.rendLayout(data, animShow);
    },null,function(){ //报错时候调用
        $(".bgLoading").remove();
    });
}

errorMsgShow.prototype.layOutTemplate = function(templateSelect,select,data){
    var myTemplate = Handlebars.compile($(templateSelect).html());
    $(select).html(myTemplate(data));
}

errorMsgShow.prototype.errorTxtMsg = function(targetElement, entityID, entityFullName){
    var _this = this;
    //错误弹层的错误提示信息
    if(targetElement.id == "errorMessage" || targetElement.id =="ErrorMessageDiv"){
        this.errorType = targetElement.innerHTML;
    }
    var IsGridAllMsg = targetElement.cusAttr("IsGridAllMsg");

    if ((targetElement.cusAttr('msg') != null) && (targetElement.cusAttr('msg').length > 0)) {
        if (entityFullName) {
            this.errorType = "<a href=\"javascript:void(0);\" onclick=\"var TabPanelId = window.top.TabPanelManager.getActiveTab().id;var frame = window.top.frames[TabPanelId + 'Frame'];$F(frame).getFrameWindow().ShowDetailMessage('" + entityID + "','" + entityFullName + "')\">" + targetElement.cusAttr('msg') + "</a>";
        } else if (IsGridAllMsg) {
            this.errorType = targetElement.cusAttr('msg'); 
        } else {
            this.errorType = targetElement.cusAttr('msg');
        }
    }

    if ((targetElement.title != null) && (targetElement.title.length > 0)) {   //JMF
        if (entityFullName) {
            this.errorType = "<a href=\"javascript:void(0);\" onclick=\"var TabPanelId = window.top.TabPanelManager.getActiveTab().id;var frame = window.top.frames[TabPanelId + 'Frame'];$F(frame).getFrameWindow().UF$UI$ErrorTooltip$showDetailMessage('" + entityID + "','" + entityFullName + "')\">" + targetElement.title + "</a>";
        } else {
            if(targetElement.title){
                this.errorType = targetElement.title;
            }
        }
    }
}


errorMsgShow.prototype.rendLayout = function (data, animShow,obj) {
    var _this = this;
    var TabPanelId = window.top.TabPanelManager.getActiveTab().id;
    var errorBoxId, errorBoxId1;
    if (obj) {  //存在
        errorBoxId1 = obj;
    } else {
        errorBoxId = 'errorBox_' + TabPanelId;
        errorBoxId1 = 'errorBoxPc_' + TabPanelId;
    }

    if(data['IsHubRun'] == false){
        if(document.getElementById(errorBoxId) == null){
            var msgDiv = document.createElement('div');
            msgDiv.id = errorBoxId;
            document.body.appendChild(msgDiv);
            msgDiv.setAttribute("targetId",TabPanelId);
        }

        this.layOutTemplate('#mobileShowTemplate','#'+errorBoxId,data);
        $("#"+errorBoxId).find(".error-page").html(this.errorType);
        
        if(data.QRCode){
            $("#"+errorBoxId).find(".errorQRCode").attr("src","data:image/png;base64, " + data.QRCode);
        }
        _this.hubW = "265px";
        _this.hubH = "265px";
        $("#"+errorBoxId).attr("errorBoxW",_this.hubW);
        $("#"+errorBoxId).attr("errorBoxH",_this.hubH);

        if($("#"+errorBoxId1).length > 0 && $("#"+errorBoxId1).html() == ''){  //没连通状态下，若大层div有创建则remove掉
            $("#"+errorBoxId1).remove();
        }

        this.openDiv(errorBoxId, _this.hubW, _this.hubH, animShow);
    }

    if(data['IsHubRun'] == true){
        if(document.getElementById(errorBoxId1) == null){
            var msgDiv1 = document.createElement('div');
            msgDiv1.id = errorBoxId1;
            document.body.appendChild(msgDiv1);
            msgDiv1.setAttribute("targetId",TabPanelId);
        }

        //连通状态下二维码弹层动画回掉结束出现大层
            $("#"+errorBoxId).closest('.layui-layer').find(".layui-layer-close").click();  //关闭之前的layer小层
            $("#" + errorBoxId).remove();//关闭的同时顺便删掉此节点
            _this.PcShow(data, errorBoxId1, animShow);  //首次加载大层
    }
}

errorMsgShow.prototype.bindEvent = function (obj) {
    var _this = this;
    $("#" + obj + " .yes-icon-good").each(function (i) { // 点赞
        $(this).on('click', function () {
            var upvoteId = $(this).attr('upvoteId');
            _this.upvote(this, upvoteId);
        })
    })

    //页面交互折叠
    $("#" + obj + " .pcHubShow").find(".errorContent").hide().eq(0).show();

    $("#" + obj + " .error-Box .errorTitle").each(function (i) {
        $(this).on('click', function () {
            $("#" + obj + " .pcHubShow").find(".errorContent").hide();
            $(this).closest(".error-Box").find(".errorContent").show(300);
        })
    })

    //点击换一批按钮，切换数据
    $("#" + obj).parent(".layui-layer-content").find('.changeNextMsg').on('click', function () {
        $(this).unbind('click');
        $(this).find(".loader-4").removeClass("hidden");
        $(this).removeClass("iconfont y-icon-changeOther");
        _this.changeStatus = false;
        _this.changePages++;
        //console.log(_this.changePages);
        _this.changeNextMsg(obj, _this.changePages);
    })
}
errorMsgShow.prototype.PcShow = function (data, errorBoxId1, animShow) {
    var _this = this;
    var errorInfo = document.createElement("span");
    errorInfo.innerHTML = _this.errorType;
    if($(errorInfo).find('.moreMsgBox').length>0){
       showMsg = $(errorInfo).find('.moreMsgBox').remove(); 
    }
    showMsg = $(errorInfo).html();
    _this.layOutTemplate('#pcShowTemplate', '#' + errorBoxId1, data);
    _this.layOutTemplate('#ErrorListTemplate', '#' + errorBoxId1 + ' .error-lists', data);  //错误列表展示
    
    $("#" + errorBoxId1).find(".errorTxt").html(showMsg);

    _this.hubW = "730px";
    _this.hubH = "500px";
    $("#" + errorBoxId1).attr("errorBoxW", _this.hubW);
    $("#" + errorBoxId1).attr("errorBoxH", _this.hubH);

    if (animShow) {
        _this.openDiv(errorBoxId1, _this.hubW, _this.hubH, animShow, 'move');  //大弹层展示
    } else {
        _this.openDiv(errorBoxId1, _this.hubW, _this.hubH, animShow);  //大弹层展示
    }
    
    _this.bindEvent(errorBoxId1);
    

    //此处动态计算大层中错误列表的高度
    var errorListBox = $("#" + errorBoxId1).closest(".layui-layer-content").height() - $("#" + errorBoxId1).find(".errorTxt").height() - 16 - 38 - 10 - 40 + 'px';
    console.log("ss:" + $("#" + errorBoxId1).find(".errorTxt").height());
    $("#" + errorBoxId1).find(".error-lists").css({ height: errorListBox });

    //tags的选取操作
    var tagName;
    if (data.Tags){  
        $("#" + errorBoxId1).find(".errorTags").attr("hoverTags", data.Tags);
    }
    $("#" + errorBoxId1).find(".errorTags em").on('click', function () {
        var tagsArr = [], tagsStr = null;
        if ($(this).closest(".errorTags").attr("hoverTags") != '') {
            tagsArr = $(this).closest(".errorTags").attr("hoverTags").split(',');
        }
        
        $(this).toggleClass("bgHover");
        if ($(this).hasClass("bgHover")) {
            tagName = $(this).text();
            if ($.inArray(tagName, tagsArr) < 0) {
                tagsArr.push(tagName);
            }
            tagsStr = tagsArr.join(',');
            // console.log(tagsStr);
            
        } else {
            tagName = $(this).text();
            for (var i = 0 ; i < tagsArr.length; i++) {
                if (tagsArr[i] == tagName) {
                    tagsArr.splice(i, 1);
                    tagsStr = tagsArr.join(',');
                    console.log(tagsStr);
                    break;
                }
            }
        }
        _this.changePages = 1; //赋初始值
        $("#" + errorBoxId1).find(".errorTags").attr("hoverTags", tagsStr);
        _this.tagsGetData(tagsStr, errorBoxId1);  //选择tag即时调取接口渲染数据
    })

}

errorMsgShow.prototype.tagsGetData = function (tagsStr,obj) {
    var _this = this;
    $.get('../error/SearchInfoByTags', { key: _this.errorKey.slice(0, 200), page:1, pagesize: 3, tags: tagsStr }, function (data) {
        _this.layOutTemplate('#ErrorListTemplate', '#' + obj + ' .error-lists', data);  //错误列表展示
        _this.bindEvent(obj);
    })

} 

errorMsgShow.prototype.openDiv = function (id, errorW, errorH, animShow,bigAnimShow) {
    var _this = this;
    var errorTitle = TabPanelManager.getActiveTab().title[0].innerHTML;
    var zIndexNum = [];
    $(window.top.parent.document).find("[type='iframe']").each(function(i){
        zIndexNum.push($(this).attr("pzindex"));
    })
    var MaxZindex = Math.max.apply(null, zIndexNum);  //取最大的z-index值
    _this.currentZindex = parseInt(MaxZindex) + 1;   //从新计算当前弹层的z-index值并赋值，目的保证是最外层展示
    // var currentZindex = parseInt($(window.top.parent.document).find("[type='iframe']").attr("pzindex")) + 1;
    window.zlayer.openDiv({
        content: $("#"+id),//用jquery查找出对应的层
        width: errorW,
        height: errorH,
        title: errorTitle+"错误信息"
    });
    //重新设置当前弹出层的left和top值
    if (animShow) {
        var floorBoxLeft = parseInt($("#"+animShow.frameId).offset().left) + parseInt(animShow.left) + 25 + 'px';
        var floorBoxTop = '106px';
        $("#" + id).closest('.layui-layer').css({
            left: floorBoxLeft,
            top: floorBoxTop
        })
    }
    
    $("#"+id).closest('.layui-layer').find(".layui-layer-title").attr('title',errorTitle+"错误信息"); 
    $("#" + id).closest('.layui-layer').css("z-index", _this.currentZindex);
   
    
}

errorMsgShow.prototype.AnimShow = function (obj) {
    $("#" + obj).closest('.layui-layer').removeClass("bounceInDown animated").addClass("bounceInDown animated").one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
        $(this).removeClass("bounceInDown animated");
    });
}

errorMsgShow.prototype.changeNextMsg = function (obj,pageNum) {
    var _this = this;
    var tagsStr = $("#" + obj).find(".errorTags").attr("hoverTags");
    //重新渲染界面数据
    $.get('../error/SearchInfoByTags', { key: _this.errorKey.slice(0, 200), page: pageNum, pagesize: 3, tags: tagsStr }, function (data) {
        //弹出大层
        if (data['IsHubRun'] == true) { //pc的错误界面展示
            if (data.Data.data.length > 0) { //有数据
                if (pageNum <= data.Data.pager['lastPage']) { //分页数计数不大于接口返回的lastPage，则更新最新的数据
                    //_this.rendLayout(data, undefined, obj);
                    _this.layOutTemplate('#ErrorListTemplate', '#' + obj + ' .error-lists', data);  //错误列表展示
                    if (pageNum == data.Data.pager['lastPage']) { //等于最后一页的时隐藏换一批按钮
                        $("#" + obj).parent(".layui-layer-content").find('.changeNextMsg').hide();
                    }
                } else {  //无数据则隐藏掉换一批按钮
                    $("#" + obj).parent(".layui-layer-content").find('.changeNextMsg').hide();
                }
            } else { //无数据
                _this.layOutTemplate('#ErrorListTemplate', '#' + obj + ' .error-lists', data);  //错误列表展示
                
            }
            _this.bindEvent(obj);  //重新绑定
        }

    });
   
}

// 点赞
errorMsgShow.prototype.upvote = function(obj,upvoteId){
    $.get('../error/ThumbsUp',{id : upvoteId},function(data){
        console.log(data);
        if(data){
            try{
                if(data['Result']){  //true
                    var num = parseInt($(obj).text());
                    num++;
                    $(obj).find('span').html(num);
                }
            }
            catch(e){
                alert("返回了错误json格式");
            }
        }

    })
}

var errorMsgShowBox = new errorMsgShow();

var helpTool = {
    _helpElement: null,
    _frame:null,
    _frameWindow: null,
    _frameDocument: null,
    _zIndex: 9999,
    _status:'off',
    _id:null,
    init: function () {

    },
    on: function () {
        if (this._status == "on")
            this.off();
        var id = TabPanelManager.getActiveTab().id;
        var frame = window.top.frames[id + "Frame"];
        if (frame) {
            this._frameWindow = $F(frame).getFrameWindow();
            this._frameDocument = $F(frame).getFrameDocument();
            this.bindEventToFrame();
            //this.showModalDiv();
            this._status = 'on';
            $("#knowledgeBtn").addClass("hightHelpColor");
        }
    },
    off: function () {
        $("#knowledgeBtn").removeClass("hightHelpColor");  //清除高亮
        $(".portal-header .controlBg1").remove(); //清除遮罩层
        var _$ = this._frameWindow.$;
        var _window = this._frameWindow;
        var _this = this;
        _$.each(_$("[ick]"), function (i, item) {
            var ick = $(item).attr("ick");
            if (ick != "Panel") {
                var _c = _window.$find(_$(item).attr("id"));
                if (_c)
                    _c.to_Defualt();
            }
            // _window.$find(_$(item).attr("id")).to_Help();
        });
        //删除页签遮罩层start
        $.each($(".tabpanel_mover li"), function (i, item) {
            var controlBg = $(item).find(".controlBg");
            if(controlBg){
               controlBg.unbind("click");
               controlBg.remove();
            }
        });
        $(".tabpanel_select_content .controlBg").remove();
        //删除页签遮罩层end
        this._status = 'off';
        this._id = null;
    },
    show: function (name,id,flag) {
        name = name.replace(/\n/g, "");
        name = name.replace(/\r/g, "");
        if(flag == true){

            var _id = TabPanelManager.getActiveTab().id;
            var frame = window.top.frames[_id + "Frame"];
            if (frame) {
                this._frameWindow = $F(frame).getFrameWindow();
                this._frameDocument = $F(frame).getFrameDocument();
                var TabPanel = window.top.TabPanelManager.getActiveTab();
                var formid = this._frameWindow.$("#FormIDForHidden").val();
            }
        } else {
            var TabPanelSelect = window.top.TabPanelManager._tabpanel;
            var TabPanelIndex = TabPanelSelect.getTabIndexById(id);
            var TabPanel = TabPanelSelect.tabs[TabPanelIndex];
            var frame = window.top.frames[id + "Frame"];
            var frameWindow = $F(frame).getFrameWindow();
            var frameDocument = $F(frame).getFrameDocument();
            //对报表做特殊处理
            var formid = frameWindow.$&&frameWindow.$("#FormIDForHidden")?frameWindow.$("#FormIDForHidden").val():'';
            var title = frameDocument.title || name;
        }
        this._id = id;
        var url = TabPanel.url;
        var _this=this;
        // if($("[labelId='"+ this._id +"']").length > 0){  //这个地方如果判断有则先remove然后再重新添加
        //     $("[labelId='"+ this._id +"']").closest(".layui-layer").remove();
        // }

        if($("[labelId]").length > 0){  //这个地方如果判断有则先remove然后再重新添加
            $("[labelId]").closest(".layui-layer").remove();
        }
        //controlId: controlid, fieldId: fieldid,
        $.get("../error/GetU9HubLoginParams",{mt: Math.uuid()},function(data){
            if(data){
                var parameter = data.data;
                if(!parameter['grant_type']){
                    parameter['grant_type'] = '';
                }
                if(!parameter['login']){
                    parameter['login'] = "auto";
                }
                
                //拼接字符串
                var strParameter = ''
                for(key in parameter){
                    if(parameter[key])
                    strParameter += key +'='+parameter[key] + '&';
                }
                if(formid&&url){
                    $.ajax({
                        url:"../error/GetTechTagsByInfo",
                        data:{formId: formid,  url: encodeURI(url),mt: Math.uuid()},
                        type:"POST",
                        success:function(data){
                            if (data && data.Tags) {
                                if(flag == true){
                                    var url = '//u9hub.yonyou.com/site/go?'+ strParameter + 'redirect=/a/h5/posts?tags='+ data.Tags + "," + _this._frameDocument.title+ "," + name;
                                }else{
                                    var url = '//u9hub.yonyou.com/site/go?'+ strParameter + 'redirect=/a/h5/posts?tags='+ data.Tags + "," + title;
                                }
                                window.zlayer.openModelessDiv({
                                    content: url,
                                    width: "600px",
                                    id:_this._id,
                                    height: "600px",
                                    title: "知识弹窗"
                                });
                                $("[labelId]").closest(".layui-layer").find(".layui-layer-min").remove();  //最小化按钮去掉
                                //关闭帮助状态
                                _this.off();
                            }
                        },
                        error:function(msg){
                            alert("无法连通u9hub获取帮助信息，请稍后再试");
                        }
                    })
                }
                
            }
        })
        
    },
    bindEventToFrame: function () {
        var _$ = this._frameWindow.$;
        var _window = this._frameWindow;
        var _this = this;
        var _index = _this._zIndex + 1;
        _$.each(_$("[ick]"), function (i, item) {
            var ick = $(item).attr("ick");
            if (ick != "Panel" && $(item).css("display") != "none" && ick != "Menu" && ick !="Wizard") {
                var _c = _window.$find(_$(item).attr("id"));
                if (_c)
                    _c.to_Help(_index, function (name,id) {
                        if (ick != "TabControl" && ick != "Toolbar") {
                            //var forid = $(e.target).attr("forid");
                            //var fieldid = $(e.target).attr("fieldid");
                            //显示知识弹窗
                            _this.show(name,id,true);
                        }
                        //关闭帮助状态
                        // _this.off();
                        //_$(forid).get_id();
                    });
            }
            // _window.$find(_$(item).attr("id")).to_Help();
        });
        //添加页签遮罩层start
        $.each($(".tabpanel_mover li"), function (i, item) { //页签支持知识弹窗
            var id = $(item).attr('id');
            var name = $(item).find(".title").text();
            $(item).append("<div class='controlBg' z-index='" + _index + "'></div>");
            if(i ==0){
                $(item).find(".controlBg").click(function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                })
            }else{
                $(item).find(".controlBg").click(function (e) {
                    _this.show(name,id,false);
                    e.preventDefault();
                    e.stopPropagation();
                })
            }
        });
        $(".tabpanel_select_content").append("<div class='controlBg' z-index='" + _index + "'></div>");
        //添加页签遮罩层end
    },
    showModalDiv: function () {
        this._frameWindow.$("body").append("<div class='iframgBg' z-index='" + this._zIndex + "'></div>");
    }
};


//U9Hub操作
var U9HubTool = (function(){
    host="http://u9hub.yonyou.com/";
    var getKey=function(){
        return $("#userNameLi").text()+"_"+$("#OrgId").val()+'_u9HubToken';
    }
    var init=function(){
        return new Promise(function(resolve, reject){
            $.get("../error/GetU9HubLoginParams",{mt: Math.uuid()},function(data){
                if(data.data){
                    var username=data.data.user_name;
                    if(data){
                        function getU9HubMsg(){
                            $.ajax({
                                url:host+"api/oauth/token",
                                data:data.data,
                                type:"POST",
                                success:function(data){
                                    if (data.data) {
                                        //获取用户token
                                        setSession(getKey(),data.data.access_token);
                                        resolve(data.data.access_token);
                                    }
                                },
                                error:function(msg){
                                    setTimeout(function(){
                                        getU9HubMsg();
                                    },180000);
                                }
                            })
                        }
                        getU9HubMsg();
                    }
                }
            })
        });
    };
    var todo = function(obj,token){
        $.ajax({
            url: host+obj.url,
            data: obj.data,
            beforeSend: function(request) {
                request.setRequestHeader("Accept", "application/json");
                request.setRequestHeader("Authorization", "Bearer "+token);
            },
            dataType: 'JSON',
            type: obj.type,
            success: function (data) {
                obj.success(data);
            },
            error: function (data) {
                obj.error(data);
            }
        });
    }
    var ajax=function(obj){
        if(getToken()){
            todo(obj,getToken());
        }else{
            init().then(function(value){
                todo(obj,value);
            });
        }
        
    };
    var getSession = function(key){
        return sessionStorage.getItem(key);
    }
    var setSession = function(key,value){
        sessionStorage.setItem(key,value);
    }
    var getToken=function(){
        //var username = getSession('username');
        return getSession(getKey()); 
    };
    return {
        get:function(obj){
            obj.type = 'get';
            ajax(obj);      
        },
        post:function(obj){
            obj.type='post';
            ajax(obj);   
        }
    }
})();
$(function () {
    //    ResourceDataManager = new $NewPortal$ResourceDataManager("Resources_" + $("#UICulture").val() + "_.js");
    ResourceDataManager = NewPortal.ResourceDataManager;
    TabPanelManager.init();
    //重置高度，来规定显示列表条数
    DocumentManager.init();
    ServiceManager.init();
    QuickMenuManager.init();
    PageTools.init();
    MyMsg.init();
    MsgBox.init('',true);
    translateTool.init();
});

;
$(function() {
   
    //quickTab 标签开始
    (function($) {
        $.quickTab = {
            table: '[quickTab="true"]',
            remove: '[name="favoriteDeleteBtn"]',
            tbody:"#favoritesManagerUl",
            inputSelectSign:'[input-select="1"]',
            //delete
            trRemove: function() { 
                $($.quickTab.table).on('click', $.quickTab.remove, function() {
                    $(this).closest("tr").remove();
                     $.quickTab.trOddRow();
                })
            },
            //隔行换色
            trOddRow: function() { 
                    $($.quickTab.tbody).find("tr:odd").addClass("oddRow");
                    $($.quickTab.tbody).find("tr:even").removeClass("oddRow");
            },
             //行位置上下移动
            move: function() {
               var _go;
                $('[up="1"]').on("click",function(){
                    up("1");
               });
                $('[double-up="1"]').on("click",function(){
                    up();
               });
                $('[down="1"]').on("click",function(){
                    down("1");
               });
                $('[double-down="1"]').on("click",function(){
                    down();
               });
                function up(m){
                    var _select= $($.quickTab.table).find('.select');
                    if(_select.index()==0)return false;
                    _first=$($.quickTab.tbody).find('tr:first');
                    m=="1"?_go= _select.prev() : _go= _first;
                    var _clone=_select.clone();
                    _select.remove();
                    _go.before(_clone);
                    $.quickTab.trOddRow();
                };
                function down(m){
                    var _length=$($.quickTab.tbody).find('tr').length;
                    var _select= $($.quickTab.table).find('.select');
                    if(_select.index()==_length-1)return false;
                    _last=$($.quickTab.tbody).find('tr:last');
                    m=="1"?_go= _select.next() : _go= _last;
                    var _clone=_select.clone();
                    _select.remove();
                    _go.after(_clone);
                    $.quickTab.trOddRow();
                }
            },
            //选中行
            addSelect:function(){ 
                    $($.quickTab.tbody).on('click', 'tr', function() {
                    $(this).addClass("select");
                    $(this).siblings().removeClass("select");
                })
            },
            //input选中
            inputSelect:function(){ 
                    $($.quickTab.tbody).delegate($.quickTab.inputSelectSign, {
                        'focus':function () {
                          $(this).addClass("input-select");
                        },
                       'blur':function () {
                            $(this).removeClass("input-select");
                        }
                 });
            },
            init: function() {
                $.quickTab.trRemove();
                $.quickTab.move();
                $.quickTab.addSelect();
                $.quickTab.inputSelect();
            }
        }
        $.quickTab.init();
    })(jQuery);
    //quickTab 标签结束

     //弹出框 input的hover、active下划线 开始
    (function($) {
        $.inputFocus = {
            objInput: '.modal-content .form-control',
            select:'select-control',
            inputSelect: function() {
                $(document).delegate($.inputFocus.objInput, {
                    'mouseover':function () {
                      $(this).addClass( $.inputFocus.select);
                     },
                      'focus':function () {
                         $(this).attr("sts","1");
                         $(this).addClass($.inputFocus.select);
                     },
                     'blur':function () {
                         $(this).attr("sts","");
                         $(this).removeClass($.inputFocus.select);
                     },
                     'mouseout':function () {
                        if($(this).attr("sts")=="1")return false;
                        $(this).removeClass($.inputFocus.select);
                     }
               });
            },
            init: function() {
                $.inputFocus.inputSelect();
            }
        }
        $.inputFocus.init();
    })(jQuery);
    //弹出框 input的hover、active下划线 结束
})
;


    IndexFaciesManager = function() {
        this.cssMaxWidth = "maxWidth";
        this.cssAdminList = ".adminUl > li";
        this.cssHover = "hover";
        this.markAdminListSubMenu = "[adminUlhover='1']";
        this.cssNavsRight=".navs-right";
        this.idMoreQuickMenuList="#moreQuickMenuLi";
        this.cssListSubMenu=".listSubMenu";
        this.timer=null;
    }
    IndexFaciesManager.prototype = {
        init: function() {
            this.MaxWidth();
            this.NavMorePosition();
            this.Event();
            this.TopNavs();
            this.Search();
            this.TabNavs();
            this.DropDownBtn();
            this.ScanCode();
            this.initTooltip();
            this.CAInit();
            this.DuDuCallInit(); 
            this.FixedModule(); 
            this.cnWrapperBindMoveOut();
            this.whatsNewInit();
        },
       whatsNewInit:function(){
                 var that=this;
                 $.get("../../mvc/newfuncintro/GetFuncDescDataCount", {"ReadType":"0",mt: Math.uuid()}, function(data){
                       
                            try {
                                    var totalCount=parseInt(data.count);
                                    if( totalCount  && window.localStorage){
                                            var completedData=that.getLocalStorageCompletedData();
                                            //已读的
                                            var isCompletedData=completedData.filter(function(value) {
                                                    if (!!value.isCompleted) {
                                                          return true;
                                                    }
                                            });
                                           if(totalCount  > isCompletedData.length){
                                                 $('[tag="new-add"]').addClass("new-add-expend"); 
                                           }
                                    }
                                
                            } catch (e) {
                                
                            }
                   });

        },
         getLocalStorageCompletedData: function() {
            if (window.localStorage) {
                    window.localStorage.removeItem("completedData_"+userName); //清除上一个痕迹，下一版去掉即可
                    var userName=$("#userNameLi").html();
                    var completedData = window.localStorage.getItem("whatsNewData_"+userName)
                    if (completedData  && typeof(completedData)=="string") {
                        if(completedData=="undefined"){
                            return [];
                        }
                        else{
                             return JSON.parse(completedData);
                        }
                    }
            } else {
                return [];
            }
        },
        initTooltip:function()
        { //提示
            $('[data-toggle="tooltip"]').tooltip();
            // 右侧导航
            $("#cn-wrapper  li i.fa").tooltip({
                  placement:"left",
                  container:".csstransformsInnner"
                  // ,delay:{ hide: 200 }
            });
        },
        //组织宽度限制
        MaxWidth: function() {
            var _this=this;
            $("." + _this.cssMaxWidth).each(function(index, item) {
                if ($(item).width() > 100) {
                    $(item).width("100px");
                }
            })
        },
        //顶部快键功能"更多"的位置
        NavMorePosition: function() {
            var navsRight = $(this.cssNavsRight).css("right");
            if (!navsRight) {
                navsRight = 0;
            }
            var resetWidth = $(this.cssNavsRight).outerWidth(true) + parseInt(navsRight);

            $(this.idMoreQuickMenuList).css("right", resetWidth);
        },
        Event: function() {
            var _this=this;
            //鼠标hover
            $(_this.cssAdminList).hover(function() {
                _this.AdminListMouseover(this);
            }, function() {
                _this.AdminListMouseout(this);
            });
            //子菜单鼠标移上去
           $(_this.markAdminListSubMenu).delegate('dd', 'mouseover', function() {
                 _this.AdminListSubMenuMouseover(this);
            });
            //子菜单鼠标移开
           $(_this.markAdminListSubMenu).delegate('dd', 'mouseout', function() {
                 _this.AdminListSubMenuMouseout(this);
            });
           //nav-tabs选项卡
          $('[nav-tabs="1"]').delegate('li', 'click', function() {
                 _this.NavClick(this);
            });
             $(".adminUl li").delegate('#orgsSearch', 'keyup', function() {
                                    var val = $(this).val();
                                    if (val == "") {
                                        $('#orgsDl dd').show();
                                    } else {
                                        $('#orgsDl dd').hide()
                                        .filter(":contains('" + (val) + "')")
                                        .show();
                                    }
             })
             //美观，阴影
            $(".con-area").hover(function() {
                $(this).find(".list-con").addClass("con-area-hover");
            }, function() {
                $(this).find(".list-con").removeClass("con-area-hover");
            });

            window.top.clientHeight=document.body.offsetHeight;//给弹出层用
        },
        
        //选项卡click事件
        NavClick: function(_eventThis) {
                var obj = {
                    li: "li",
                    select: "select"
                };
                _eventThis = $(_eventThis);
                if(_eventThis.hasClass(obj.select))return;
                _eventThis.addClass(obj.select);
                _eventThis.siblings(obj.li).removeClass(obj.select);
                //消息
                if (_eventThis.parent().attr("ulType") == "messages") {
                    ServiceManager.getMessages();
                //任务
                } else if (_eventThis.parent().attr("ulType") == "tasks") {
                    ServiceManager.getTasks();
                }
           
        },
        //鼠标移上去
        AdminListMouseover: function(_eventThis) {
            var _this=this;
            $(_eventThis).addClass(_this.cssHover);
            _this.AdminListSubMenuDisplay(_eventThis,"block");
        },
        //鼠标移开
        AdminListMouseout: function(_eventThis) {
            var _this=this;
            $(_eventThis).removeClass(_this.cssHover);
            _this.AdminListSubMenuDisplay(_eventThis,"none");
            
        },
        //子菜单显示
        AdminListSubMenuDisplay:function(_eventThis,display){
            var _this=this;
            if ($(_eventThis).find(_this.markAdminListSubMenu).length > 0) {
                //if(display=="block"){ServiceManager.getOrgs();}; 太慢
                 $(_eventThis).find(_this.markAdminListSubMenu).css("display",display);
                 if($(_eventThis).find("#orgsSearch").length > 0){
                         $("#orgsSearch").css("display",display);
                         if(display=="block"){
                              $("#orgsSearch").prop("value","").select();
                              $('#orgsDl dd').show();
                         }
                 }
            }
        },
        //子菜单鼠标移上去
        AdminListSubMenuMouseover:function(_eventThis){
            var _this=this;
            $(_eventThis).addClass(_this.cssHover);
                if ($(_eventThis).find(_this.cssListSubMenu).length > 0) {
                    switch ($(_eventThis).attr("type")) {
                        case "enterprise":
                            ServiceManager.getEnterprise();
                            break;
                        case "languages":
                            ServiceManager.getLauguages();
                            break;
                        case "themes":
                            ServiceManager.getThemes();
                            break;
                        default:
                            break;
                    }
                    $(_eventThis).find(_this.cssListSubMenu).show();
                }
        },
        //子菜单鼠标移开
        AdminListSubMenuMouseout:function(_eventThis){
            var _this=this;
            $(_eventThis).removeClass(_this.cssHover);
            $(_eventThis).find(_this.cssListSubMenu).hide();
        },
        cnWrapper:'#cn-wrapper',
        cnButton:"#cn-button",
        //右侧浮动
        FixedModule:function(){
            var that=this;
            this.rightCnWrapper();
            var cnButton=$(this.cnButton);
            //click事件
            cnButton.click(function(){
                   that.cnButtonOnClick();
            })
            //jquery-ui draggable 拖拽
            $(".csstransforms").draggable({
                cursor:"move",
                handle:that.cnButton,
                start:function(){
                         cnButton.unbind("click");
                        $(".csstransformsInnner").unbind('mouseleave');
                },
                stop:function(){
                    setTimeout(function(){
                         cnButton.bind("click",that.cnButtonOnClick);
                         that.cnWrapperBindMoveOut();
                    },200)
                }

            })
            $(this.cnWrapper+" li i.fa").mouseover(function(){
                //提示外观变动
                 $(".tooltip").addClass("tooltip-add");
            })
            
        },
       
        cnWrapperBindMoveOut:function(){
            var that=this;
            $(".csstransformsInnner").hover(function(){},function(e){
                if($(".cn-wrapper ").attr("class").indexOf("opened-nav") >-1){
                    that.cnButtonOnClick();
                }
                 
            })
        },
        //6个li时，正好180度，半圆显示
        rightCnWrapper:function(){
            /*var cnWrapperLi=$(this.cnWrapper).find("li:visible");
            //为了美观特殊处理
            if(cnWrapperLi.length==6){
                     $(".csstransforms").css("right","70px");
            }  */
        },
        open:false,
        cnButtonOnClick:function(){
             var that=IndexFaciesManager.prototype;
             var cssOpenedNav='opened-nav';
             var cssCnButtonAdd='cn-button-add';
             var cnButton=$(that.cnButton);
             var cnWrapper=$(that.cnWrapper);
           //右侧浮动的各个li 的位置
            var cnWrapperLi=cnWrapper.find("li:visible");
            var cnWrapperLiLength=cnWrapperLi.length;
            //li的角度 
            var rotateDifference=30 + 2; //角度+扇形间隔  ,也是角度差
            var moreThan180Fan=cnWrapperLiLength * rotateDifference -180;//扇形多于180之外的部分的角度
                  if(!that.open){
                        $(".csstransforms").children().addClass("csstransformsInnner-add");
                        ServiceManager.getUnifyInfo();//调用indexService.js里的获取数量接口
                                 cnWrapper.addClass(cssOpenedNav);
                                 cnButton.addClass(cssCnButtonAdd);
                                 //默认7，css作了处理
                                //if(cnWrapperLiLength !=7){
                                   that.addFixedModuleForLi(cnWrapperLi,moreThan180Fan,rotateDifference);
                                //}
                  }
                  else{
                       $(".csstransforms").children().removeClass("csstransformsInnner-add");
                        cnWrapper.removeClass(cssOpenedNav);
                        cnButton.removeClass(cssCnButtonAdd);
                        //默认7，css作了处理
                        //if(cnWrapperLiLength !=7){
                            that.removeFixedModuleForLi(cnWrapperLi);
                        //}
                  }
                  that.open = !that.open;
        },
        isIconAddCss:true, //icons样式只加一次
        //右侧浮动的各个li 的位置
        addFixedModuleForLi:function(cnWrapperLi,moreThan180Fan,rotateDifference){
                        var firstRotate=-90  -moreThan180Fan/2;
                        var that=this;
                        cnWrapperLi.each(function(index,item){
                             //icon只加一次
                             if(that.isIconAddCss){
                                  var iconDeg= -1 * (firstRotate-75); 
                                  $(item).find("i").css("transform","rotate("+iconDeg+"deg)");
                              }
                              $(item).css("transform","rotate("+firstRotate+"deg)   skew(60deg)");
                              firstRotate=firstRotate + rotateDifference;
                        })
                        //icon只加一次
                        that.isIconAddCss=false;
        },
        removeFixedModuleForLi:function(cnWrapperLi){
                 //右侧浮动的各个li 的位置
                  cnWrapperLi.css("transform","");
        },
        //顶部导航
        TopNavs:function(){
                var obj = {
                    navsUl: $('.navs-ul'),
                    navMore: '[nav-more="1"]',
                    dl: '.nav-more-dl',
                    select: "select",
                    hover: "hover",
                    li: 'li'
                };
                obj.navsUl.delegate(obj.navMore, 'click', function() {

                    _this = $(this);
                    _this.toggleClass(obj.select);
                    // _this.find(obj.dl).attr("tabIndex","-1");
                    _this.find(obj.dl).toggle();
                    // if(_this.find(obj.dl).is(":visible")){  //weiyina 
                    //     _this.find(obj.dl).focus();
                    // }
                    // //return false;
                })

                // $(obj.dl).on('blur',function(){  //失去焦点隐藏div层 -- 跨浏览器兼容20170522 -- weiyin
                //     setTimeout(function(){
                //         obj.navsUl.find(obj.navMore).removeClass(obj.select);
                //         obj.navsUl.find(obj.dl).hide()
                //     },400);
                // })

                $(obj.dl).hover(function(){},function(){
                    obj.navsUl.find(obj.navMore).removeClass(obj.select);
                    obj.navsUl.find(obj.dl).hide();
                })

                obj.navsUl.delegate(obj.li, {
                    'mouseover': function() {
                        $(this).addClass(obj.hover);
                    },
                    'mouseout': function() {
                        $(this).removeClass(obj.hover);
                    }
                });
        },
        //搜索分类
        Search:function(){
             var obj = {
                selectUl: ".selectUl",
                m_select: '[m_select="1"]',
                selectDiv: ".selectDiv",
                selectBg: "selectBg"
            };
            $(document).delegate(obj.m_select, 'click', function() {
                $(obj.selectUl).hide();
                var value = $(this).find(obj.selectDiv).attr("code");
                $(this).find("input.selectDiv").select();
                var selectUl = $(this).find(obj.selectUl);
                var thisCode = selectUl.find("li[code='" + value + "']");
                selectUl.show().find("li").show().removeClass(obj.selectBg);
                thisCode.addClass(obj.selectBg);
                if (selectUl.height() > 250) selectUl.height(250);
                selectUl.scrollTop(thisCode.index() * thisCode.height());
                return false;
            })
            $(obj.m_select).find('.selectUl').on({
                mouseover: function() {
                    $(this).addClass(obj.selectBg);
                    $(this).siblings().removeClass(obj.selectBg);

                    
                },
                mouseleave: function() {
                    $(this).removeClass(obj.selectBg);
                },
                click: function() {
                    var _text = $(this).attr("text");
                    var selectUl = $(this).parent();
                    selectUl.hide();
                    var _value = $(this).attr("code");
                    selectUl.siblings("div.selectDiv").html(_text);
                    selectUl.siblings("input.selectDiv").val(_text);
                    selectUl.siblings(obj.selectDiv).attr("code", _value);
                    selectUl.siblings(obj.selectDiv).attr("title", _text);
                    return false;
                }
            }, 'li');
            
            // $(document).on('click', function() { 
            //     // ：visible 判断是否显示  显示：true ，隐藏： false
            //     if($("#searchCategorysDiv").is(":visible")){
            //         $(obj.selectUl).hide();
            //     }
            // })
            // 主动隐藏当前弹层而不是靠被动点击document来判断是否为当前区域再判断隐藏
            $("#searchCategorysDiv").hover(function(){},function(){
                $(obj.selectUl).hide();
            })

        },
        TabNavs:function(){
            var obj = {
                    tabsLi: ".nav-tabs li",
                    icon: ".fa-times-circle",
                    select: "text-pink",
                    li: "li"
                };
                $(obj.tabsLi).hover(function() {
                    $(this).find(obj.icon).show();
                }, function() {
                    $(this).find(obj.icon).hide();
                })

                $(obj.tabsLi).find(obj.icon).on({
                    mouseover: function() {
                        $(this).addClass(obj.select);
                    },
                    mouseleave: function() {
                        $(this).removeClass(obj.select);
                    },
                    click: function() {
                        $(this).parent(obj.li).remove();
                        return false;
                    }
                });
        },
        DropDownBtn:function(){
            var _this = this;
                // 下拉菜单1
            $(".tabpanel_select_content .btn1").mouseenter(function() {
                $(this).addClass("btn1a");
                $(".lists").removeClass("hidden");
            }).mouseleave(function() {
                $(this).removeClass("btn1a");
                $(".lists").addClass("hidden");
            })

            // 下拉菜单2
            $(".tabpanel_select_content .btn2").mouseenter(function() {
                $(this).addClass("btn2a");
                _this.checkAuthorization();
                $(".lists1").removeClass("hidden");
            }).mouseleave(function() {
                $(this).removeClass("btn2a");
                $(".lists1").addClass("hidden");
            })
             //动态添加菜单
            $(".otherlists").on('click', 'a', function() {
                $(".otherlists a").removeClass("onlys");
                $(this).addClass("onlys");
            })
            /*
            $(document).click(function(event) { //点击空白区域隐藏下拉框
                var $tar = $(event.target);
                if ($tar.is(".tabpanel_select_content .btn1,.tabpanel_select_content .btn2")) {

                } else {
                    $(".lists1,.lists").addClass("hidden");
                    $(".tabpanel_select_content .btn1").removeClass("btn1a");
                    $(".tabpanel_select_content .btn2").removeClass("btn2a");
                }
            });*/
        },
        checkAuthorization:function(){
            var id = TabPanelManager._tabpanel.getActiveTab().id;
            var frame = window.frames[id + "Frame"];
            if(frame){
                var _frameDocument = HtmlHelper.getFrameDocument(frame);
                if(_frameDocument){
                    var liHave = $(_frameDocument).find("li[val='Edit:page']");
                    if(liHave.length == 0){
                        $(".lists1 a").addClass("notBg");
                        $(".lists1 a").last().removeClass("notBg");
                    }else{
                        $(".lists1 a").removeClass("notBg");
                    }
                }
            }else{
                $(".lists1 a").addClass("notBg");
            }
        },
        ScanCode:function(){
                //weiyina新增扫码功能2016.07.29
        $.event.special.valuechange = {

            teardown: function (namespaces) {
                $(this).unbind('.valuechange');
            },

            handler: function (e) {
                $.event.special.valuechange.triggerChanged($(this));
            },

            add: function (obj) {
                $(this).on('keyup.valuechange cut.valuechange paste.valuechange input.valuechange', obj.selector, $.event.special.valuechange.handler)
            },

            triggerChanged: function (element) {
                var current = element[0].contentEditable === 'true' ? element.html() : element.val()
                  , previous = typeof element.data('previous') === 'undefined' ? element[0].defaultValue : element.data('previous')
                if (current !== previous) {
                    element.trigger('valuechange', [element.data('previous')])
                    element.data('previous', current)
                }
            }
        }

        var textAr = document.getElementById("textCode");
        $("#enterMsg").bind('click',function(){
            enterMsgOnClick();
        });
        var enterMsgOnClick=function (event) {
            $("#textCode").val('');
            $("#enterBox").toggle(300);
            $("#textCode").focus();
            $("#enterMsg").toggleClass("hoverB");
        }
        $("#enterBox").on('click', function (event) {
            event.stopPropagation();
        })
        //Code=000MO-0187002
        $("#textCode").on('focus', function () {
            var txtVal = $(this).val();

            if (txtVal == '请扫码录入') {
                $("#textCode").val('');
            }
        }).on('blur', function () {
            if ($(this).val() == '') {
                $("#textCode").val('请扫码录入');
            }
        // }).on('valuechange', function (event) {
        }).on('keyup', function (event) {
            var txtVal = $(this).val();
            if(event.keyCode == "13" && txtVal){  // 回车执行查询
                $.ajax({
                    type: "GET",
                    url: "../main/GetDocUrlByBarCode",
                    data: { Code: txtVal, mt: Math.uuid() },
                    success: function (data) {
                        if (data.split(".aspx?").length > 1) {
                            var urlStr = "../.." + data+"&scanCode=wya";
                            TabPanelManager.addUrlTab(urlStr, '');
                        }else if(data){
                            alert("当前条码属于："+ data);
                        }else{
                            alert("码有误请核实后重新录入");
                        }
                        $("#textEnter").html('最近一次录入串:<br/>'+txtVal);
                        $("#textCode").val(''); //清空录入值
                        $("#textCode").focus();
                    },
                    error: function (msg) {
                        alert(msg);
                    }
                })
            }
        })

        //文本区拖拽
       // $("#textEnter").mydrag({ "_drag": "#enterBox" });
        //jquery-ui draggable
             $("#enterBoxWrap").draggable({
                cursor:"move",
                cancel:"#textCode",
                start:function(){
                      $("#enterMsg").unbind('click');
                },
                stop:function(){
                     setTimeout(function(){
                            $("#enterMsg").bind('click', enterMsgOnClick);
                     },200)
                      
                }
            })
        // 判断扫码平台是否启动
        window.onload = function(){
            $.ajax({
                type : "GET",
                // url : "../main/GetAppState",
                url : "../main/IsApplicationEnable",
                data : {mt: Math.uuid()},
                success : function(data){
                    if(data == "1"){  //启动
                        $("#enterMsg").show();
                        // $("#enterMsg").addClass("hoverB");
                        // $("#enterBox").show();
                        $("#textCode").focus();
                        //判断当前的扫码图标的位置，避免首次刷新导致上方导航图标和扫码图标重叠，影响用户体验 -- weiyin 2017.02.21
                        var lastLiPositon = $("#cn-button").offset().top; //导航图中最后一个图标的位置
                        var enterMsgPosition = $("#enterMsg").offset().top; //扫码图标位置
                        if(lastLiPositon >= enterMsgPosition || enterMsgPosition - lastLiPositon < 30){
                            $("#enterMsg").css({"top":lastLiPositon + 60});
                            $("#enterBox").css("top",lastLiPositon + 40);
                        }
                    }else{
                        $("#enterBox").hide();
                        $("#enterMsg").hide();
                        $("#enterMsg").removeClass("hoverB");
                    }
                },
                error : function(msg){
                    alert(msg);
                }
            })
        }
    },
    CAInit:function(){
         $.ajax({
                type: "POST",
                url: "../Login/GetCAScripts",
                async: false,
                data: {
                    enterpriseId: ""
                },
                success: function (data) {//渲染添加CA的相关html元素
                    if (data) {
                        data = eval("(" + data + ")");
                        $("body").append('<div id="_CA_"></div>');
                        if (data.caEnable === "true") {
                            $("#_CA_").append('<object id="CertTool" style="display:none;" codeBase="../../' + data.codeBase + '" classid="' + data.classId + '"></object>');
                        }
                        var caJs = document.createElement("script");
                        caJs.setAttribute("type", "text/javascript");
                        caJs.setAttribute("src", "../../CA/Scripts/ca.js?v=20180718");
                        document.getElementById("_CA_").appendChild(caJs);
                        var caClientJs = document.createElement("script");
                        caClientJs.setAttribute("type", "text/javascript");
                        caClientJs.setAttribute("src", "../../" + data.caClientJsSrc);
                        document.getElementById("_CA_").appendChild(caClientJs);
                    }
                }
            });
    },
    DuDuCallInit:function(){
        $.ajax({
                type: "GET",
                url: "../duducall/GetDuDuKey",
                async: false,
                data: {
                    mt: Math.uuid()
                },
                success: function (data) {
                if (data&&data.indexOf("<html") <=-1) {
                    data = JSON.parse(data);
                    //如果通信公司没有给企业配置账号和秘钥，duDuBtn按钮隐藏
                    if (data.appId == ""||data.appPwd==""){ 
                        $("#duDuBtn").hide();
                    } else{
                            $("#duDuBtn").show(); 
                            $.ajax({
                                    type: "GET",
                                    url: "../duducall/EnableDuduCall",
                                    async: false,
                                    data: {
                                        mt: Math.uuid()
                                    },
                                    success: function (data) {
                                        //如果用户存在联系对象，则移出disabled
                                        if(data == "1"){  
                                            $("#duDuBtn").removeAttr("disabled");
                                        }else{
                                            $("#duDuBtn").attr("disabled", "disabled");
                                        }
                                    }
                                });                                       
                            }
                    }else{
                        $("#duDuBtn").hide();
                    }
                }
            });
        this.DuDuEventBind();
    },
     DuDuEventBind:function(){
        var _this=this;
        $("#duDuBtn").on('click', function () {
                var id = TabPanelManager.getActiveTab().id;
                var frame = window.frames[id + "Frame"];
                //如果是frame，则说明是页面，弹出多方通话窗口
                if(frame){
                    $.ajax({
                            type: "GET",
                            url: "../duducall/DoAction",
                            data: { entityType: $(frame.document).find("#_mWorkflowTag_").val(), mt: Math.uuid() },
                            success: function (data) {
                               if(data){
                                    data = JSON.parse(data);
                                    window.top.duduShowDialog(data);
                                }else{
                                  alert("用户或联系对象不存在");
                                }
                            },
                            error: function (msg) {
                                alert(msg);
                            }
                    })
                }
            })
        }

    }
    IndexFaciesManager.GetInstance = function() {
        return new IndexFaciesManager();
    }

    AvatarFaciesManager=function(){
         this.delPhotoBtnID="#delPhotoBtn"; //删除头像按钮
         this.myPhotoID="#myPhoto";
         this.cropBoxID="#crop-box"//剪裁box
         this.cropBtnID="#cropBtn";//剪裁按钮
         this.selectUploadBtnText="#reUploadPhoto .upload-btn-text";//上传按钮的文字
         this.jcrop_api=null;
         this.cropDisplayID='#cropDisplay';
         this.uploadPromptID="#uploadPrompt";//上传提示
         this.uploadPromptTextID="#uploadPromptText";//上传提示文字
         this.targetID="#target";
    }
    AvatarFaciesManager.prototype = {
                
                //初始化头像
                init:function(){
                    this.showAvatar();//初始化 显示头像
                    this.hideAvatarBoxInit();  //初始化，1、剪裁框及剪裁按钮隐藏 2上传按钮文字初始化
                    //绑定事件
                    this.bindEvent();
                },
                //显示头像
                showAvatar:function(){
                       var that=this;
                        $.ajax({
                                type: "post",
                                url: "../photo/ShowPhoto",
                                data: {
                                    mt: Math.uuid()
                                },
                                success: function (data) {
                                   if (data) {
                                          try{
                                                 data = JSON.parse(data);
                                                $(that.delPhotoBtnID).show();  
                                                  //更新头像相关信息
                                                 that.updateAvatarInfo(data.ImageUrl,data.ImageId,data.FileName);

                                          } catch (e) {
                                                //alert("初始化头像失败");
                                                data=null;
                                            }          
                                    }else{
                                          //alert("初始化头像失败");
                                    }
                                }
                            });
                       
                },
                //注册头像相关按钮事件
                 bindEvent:function(){
                     var that=this;
                     //上传头像
                       $(this.myPhotoID).on("change",function(){ 
                              that.photoOnChange();
                       });
                       //单击"剪裁上传"按钮
                        $(this.cropBtnID).on('click', function () {
                              that.cropBtnOnclick();
                        });
                        //删除头像
                        $(that.delPhotoBtnID).on('click', function () {
                               that.deleteAvatar();
                        });
                        $(that.uploadPromptID+" .close").on('click',function(){
                              $(that.uploadPromptID).fadeOut();
                       })
                    },
                photoOnChange:function(){
                    var that=this;
                       $("#myPhotoForm").ajaxSubmit({
                                type: "post",
                                url: "../photo/Upload",
                                data: { mt: Math.uuid()},
                                success: function (data) {
                                 that.othersInit(); //解决上传非图片文件后，提示框隐藏和无法上传问题
                                if(data){
                                        try{
                                            data = JSON.parse(data);
                                            if(data.ImageUrl){
                                                that.showAvatarBoxInit(); //1、剪裁框及剪裁按钮显示 2上传按钮文字修改  
                                                $(that.targetID).attr("src",data.ImageUrl); //裁剪框的图片文件替换为最新的上传图片
                                                that.updateAvatarInfo(null,data.ImageId,data.FileName); //更新头像相关信息
                                                that.photoJcrop();//剪裁初始化
                                            }
                                        }catch(e){
                                            that.errPrompt(data);//错误信息提示
                                            data=null;
                                        }       
                                    }else{
                                         console.log("上传失败");
                                    }
                                },
                                error: function (msg) {
                                    console.log(msg);
                                }
                                });
                },
                //单击"剪裁上传"按钮
                cropBtnOnclick:function(){
                       var that=this;
                       $.ajax({
                                type: "post",
                                url: "../photo/Crop",
                                data: {"ImageId": $("#ImageId").val(),"FileName": $("#FileName").val(),"CropX": $("#CropX").val(),"CropY": $("#CropY").val(),"CropW": $("#CropW").val(), "CropH": $("#CropH").val()},
                                success: function (data) {
                                  if(data){
                                            try{
                                                data = JSON.parse(data);
                                                that.updateAvatarInfo(data.ImageUrl,data.ImageId,data.FileName); //更新头像相关信息
                                                that.updateIndexAvatar(data.ImageUrl);//更新首页相应头像
                                                that.hideAvatarBoxInit();  //1、剪裁框及剪裁按钮隐藏 2上传按钮文字初始化
                                                $(that.delPhotoBtnID).show(); //删除头像按钮 显示
                                                that.prompt("头像上传成功!");//提示框
                                                
                                            }catch(e){
                                                that.prompt("头像上传失败，请重新上传");//提示框
                                                data=null;
                                            }
                                    }else{
                                        that.prompt("头像上传失败,请重新上传!");//提示框
                                    }
                                },
                                error: function (msg) {
                                    console.log(msg);
                                }
                                });
                },
                //删除头像
                deleteAvatar:function(){
                     var that=this;
                      $.ajax({
                                type: "post",
                                url: "../photo/DelPhoto",
                                data: {"ImageId": $("#ImageId").val()},
                                success: function (data) {
                                   if(data&&data=="1"){                        
                                            that.hideAvatarBoxInit();   //1、剪裁框及剪裁按钮隐藏 2上传按钮文字初始化                       
                                            $(that.delPhotoBtnID).hide(); //删除头像按钮 隐藏
                                            that.updateAvatarInfo("../../avatar/images/avatar.png","",""); //初始化头像相关信息
                                            that.updateIndexAvatar("");//更新首页相应头像
                                            that.prompt("删除成功!");//提示框
                                    }else{
                                        that.prompt("删除失败!");//提示框
                                    }
                                },
                                error: function (msg) {
                                    console.log(msg);
                                }
                                });
                },
                //更新头像相关信息
                updateAvatarInfo:function(avatarSrc,avatarID,fileName){
                          if(avatarSrc){
                              $("#finnalPhoto").attr("src",avatarSrc);//我的信息中头像
                              $("#logoBarAvatar").attr("src",avatarSrc); // logo-bar上的用户头像
                          }
                          if(avatarID)$("#ImageId").attr("value",avatarID);
                          if(fileName)$("#FileName").attr("value",fileName);
                },
                //更新首页头像src
                updateIndexAvatar:function(imageUrl){
                            //var userCode=$("#code").prop("value");
                            var userCode=$("#userNameLi").text();
                            $('[code="'+userCode+'"]').attr("src",imageUrl);
                },
                //1、剪裁框及剪裁按钮显示 2上传按钮文字修改  
                showAvatarBoxInit:function(){
                          $(this.cropBoxID).show();//剪裁框显示
                          $(this.cropBtnID).show();   //剪裁按钮显示
                          $(this.selectUploadBtnText).text("重新选择"); //“上传头像”文字修改为”重新上传“     
                },
                hideAvatarBoxInit:function(){
                          $(this.cropBoxID).hide(); //剪裁框隐藏
                          $(this.cropBtnID).hide(); //剪裁按钮隐藏
                          $(this.selectUploadBtnText).text("上传头像");//”重新上传“  文字修改为 “上传头像” 
                          $(this.targetID).attr("src","");//裁剪框的图片清空
                          this.othersInit();
                },
                othersInit:function(){
                          $(this.myPhotoID).prop("value","");  //不能上传同一个头像bug
                          $(this.uploadPromptID).hide();//提示关闭
                },
                //提示
                prompt:function(text,notHidden){
                          var timeOut=null;
                          var selectUploadPrompt=$(this.uploadPromptID);
                          selectUploadPrompt.show().find(this.uploadPromptTextID).html(text);
                          if(!notHidden){
                              timeOut=setTimeout(function(){
                                     selectUploadPrompt.fadeOut();
                              },2000)
                          }
                },
                //错误提示
                errPrompt:function(str){
                     var errPromptText="头像上传失败！";
                     if(typeof(str)== "string"){
                             if(str.indexOf('error-message') > 0){
                                errPromptText+=str.substring(str.indexOf("<h1>")+4,str.indexOf("</h1>"));
                             }
                     }
                     this.prompt(errPromptText,true);
                },
                //jcrop
                photoJcrop:function(){
                           // Create a scope-wide variable to hold the Thumbnailer instance
                            var thumbnail;
                            var that=this;
                            //不destroy第二次添加images有问题
                            if(typeof that.jcrop_api !="undefined" && that.jcrop_api)that.jcrop_api.destroy();
                            $(that.targetID).Jcrop({
                                aspectRatio: 1,
                                bgOpacity: 0.3,
                                boxWidth:300,
                                setSelect:[200,200,300,300]
                            }, function () {
                                that.jcrop_api = this;
                                thumbnail = this.initComponent('Thumbnailer', { width: 100, height: 100 });
                                thumbnail.autoHide = true;
                                this.ui.preview = thumbnail;
                            });

                            // 有点神奇，直接在img对象上挂接事件是不行的，必须在上级的容器上！！
                            $(that.cropDisplayID).on('cropmove cropend', function (e, s, c) {
                                if (c.w > 0) {
                                    $('#CropX').val(c.x);
                                    $('#CropY').val(c.y);
                                    $('#CropW').val(c.w);
                                    $('#CropH').val(c.h);
                                }
                            });
                   }
    }
    AvatarFaciesManager.GetInstance = function() {
        return new AvatarFaciesManager();
    }

   
   
       

//右边侧栏 截屏（又称错误反馈） start
ErrorFeedback = function() {
       this.screenShotID="#screenShot";
       this.screenShotInputID="#screenShot";
       this.errorFeedbackID="#errorFeedback";
    }
ErrorFeedback.prototype = {
    init: function() {
          this.Events();
    },
    Events:function(){
           this.errorFeedbackOnClick();
           this.bodyOnKeyUp($(document));
    },
    bodyOnKeyUp:function(select){
             var that=this;
              select.keyup(function(event){ 
                 //按下了Ctrl+Alt+P键
                  if ((event.ctrlKey) && (event.altKey) && (event.keyCode === 80)){ 
                            $(that.errorFeedbackID).trigger("click");
                  } 
             }); 
    },
    //右边侧栏“截屏”点击
    errorFeedbackOnClick:function(){
           var that=this;
           $(this.errorFeedbackID).click(function(){
                    //tabpanel ID
                    var TabPanelId = window.top.TabPanelManager.getActiveTab().id;
                    //当前tabpanel所在的frame
                    var frame = window.frames[TabPanelId + "Frame"];
                    //frame存在时，iframe页面截屏
                    if(!!frame){
                        var screenShotInput=$($F(frame).getFrameDocument().body).find(that.screenShotInputID);
                        //确保隐藏button是存在的，再去截屏
                        if(screenShotInput.length > 0){
                                 //显示提示框
                                 that.screenShotPromptShow();
                                 //触发iframe内隐藏button的点击事件
                                 screenShotInput.trigger("click");
                        }
                    //frame不存在时，首页截屏
                    }else{
                          //显示提示框
                          that.screenShotPromptShow();
                          //截屏
                          that.screenShotSecond();
                    }
            })
            
    },
    //show modal提示语框
    screenShotPromptShow:function(){
         $(this.screenShotID).modal({
                show:true,
                backdrop:'static' //点击空白处不关闭
         });
    },
    //hide modal提示语框
    screenShotPromptHidden:function(){
        $(this.screenShotID).modal('hide');
    },
    //html2canvas截屏
    screenShot:function(select){
             var that=this;
             html2canvas(select, {
                        allowTaint: true,
                        onrendered: function(canvas) {
                             //var image=canvas.toDataURL("img/png");
                             //select.append(canvas);
                             that.appendCanvasToBody(canvas);
                             //父级截屏
                             that.screenShotSecond();
                        }
            });
    },
    //将iframe截图放并绝对定位（css）至body，以便第二次截屏
    appendCanvasToBody:function(canvas){
         //var imageHtml= "<img src="+image+" class='fake-html' id='fakeHtml' />"; 
         $('body .portal-content').append(canvas);
    },
    //二次截屏完毕，需remove掉
    removeCanvas:function(){
         $(".portal-content > canvas").remove();
    },
    //html2canvas截屏
    screenShotSecond:function(){
             var that=this;
             var select=$("body");
             html2canvas(select, {
                        allowTaint: true,
                        onrendered: function(canvas) {
                             var image=canvas.toDataURL("img/png");
                             //select.append(canvas);
                             that.removeCanvas();
                             that.postImage(image);
                        }
            });
    },
    postImage:function(image){
             var that=this;
             var imageStr=image.split(";")[1].split(",")[1];
             /*$.post("../error/Snapshot",{image:imageStr},function(result){
                      var tiket=result.tiket;
                      if(tiket){
                           window.open("../error/Download?tiket="+tiket);
                      }
                      that.screenShotPromptHidden();
              });*/
              $.post("../error/Snapshot",{image:imageStr},function(result){
                  var tiket=result.tiket;
                  $.post("../error/Download", { tiket: tiket }, function (data) {
                      if (data && data.Result == true) {
                          that.screenShotPromptHidden();
                          alert("截屏已完成，文件路径如下：\r\n"+data.Path)

                      }else{
                          that.screenShotPromptHidden();
                          alert("截屏异常，请联系管理员");
                      }
                  })
                  //"../error/Download?tiket=" + tiket);
             });
    }
}
ErrorFeedback.GetInstance = function() {
    return new ErrorFeedback();
}
//右边侧栏 截屏（又称错误反馈） end

 $(function() {
            IndexFaciesManager.GetInstance().init();
            AvatarFaciesManager.GetInstance().init(); //头像
            // $(".list-con-ul").sortable();
            //  $(".list-con-ul").disableSelection();
            ErrorFeedback.GetInstance().init()
            
 });


FullScreenBodyHelper = function(FullScreen) {
    this.cssZoom="zoomBig";
    this.cssZoomBig = "y-icon-fullScreen";
    this.cssZoomSmall = "y-icon-exit-fullScreen";
    this.FullScreen=FullScreen;
    this.isFullScreen=false;
    this.cssFullScreen="full-screen";
}
FullScreenBodyHelper.prototype = {
    init: function() {
              this.creatMaximized();
              this.cssMaximized();
    },
    creatMaximized:function(){
        if(!this.FullScreen){
                  if($("body").children("."+this.cssZoom).length ==0){
                      this.FullScreen=document.createElement("i");
                      $("body").prepend(this.FullScreen);
                      this.bindOnMaximizedClick();
                  }else{
                       this.FullScreen=$("body").children("."+this.cssZoom).get(0);
                  } 
         }
    },
     bindOnMaximizedClick:function(){
              var that=this;
              //放大
              $(this.FullScreen).on('click',function(){
                  //放大
                   if($(this).hasClass(that.cssZoomBig)){
                        that.onFullScreenClick();
                   //缩小
                   }else if($(this).hasClass(that.cssZoomSmall)){
                        that.onRemoveFullScreenClick();
                   }
              })   
    },
    //全屏
    onFullScreenClick:function(){
           if(this.FullScreen){
                     $("body").addClass(this.cssFullScreen);
                     this.isFullScreen=true;
                     this.cssMaximized();
           }
    },
    //移出全屏
    onRemoveFullScreenClick:function(){
         if(this.FullScreen){
                  $("body").removeClass(this.cssFullScreen);
                  this.isFullScreen=false;
                  this.cssMaximized();
         }
    },
    cssMaximized:function(){
             if(this.FullScreen){
                  //全屏
                  if(!!this.isFullScreen){
                         this.changeFullScreen({
                             Element:this.FullScreen,
                             title:"退出全屏模式",
                             className:this.cssZoomSmall
                         })
                  }else{
                        this.changeFullScreen({
                             Element:this.FullScreen,
                             title:"进入全屏模式",
                             className:this.cssZoomBig
                         })
                  }
             }
    },
    changeFullScreen:function(opinions){
         opinions.Element.className = this.cssZoom+" iconfont "+opinions.className;
         opinions.Element.title=opinions.title; 
    }
}
FullScreenBodyHelper.GetInstance = function(FullScreen) {
    return new FullScreenBodyHelper(FullScreen);
}
$(function(){
    FullScreenBodyHelper.GetInstance().init();
     window.top.GridInfo=[]; //全屏
     $("#myPrompt>b").click(function() {
         $("#myPrompt").hide();
     })
     $("#myErrorNotify>b").click(function(){
        $("#myErrorNotify").hide().find(".myPromptContent").html('');
     })
})

window.top.myNotify=function (text) {
    var timeOut=null;
    var selectMyPrompt=$("#myPrompt");
    
    selectMyPrompt.show().find(".myPromptContent").html('<li><i class="iconfont y-icon-warn"></i>'+text+'</li>');
    timeOut=setTimeout(function(){
        selectMyPrompt.hide();
    },3000)
}
