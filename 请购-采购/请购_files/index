                    }
                },
                getColorChannels: function(e) {
                    if (e = e.replace("#", ""),
                    /^[0-9a-fA-F]{3}$/.test(e)) {
                        e = e.split("");
                        for (var t = 2; t >= 0; t--)
                            e.splice(t, 0, e[t]);
                        e = e.join("")
                    }
