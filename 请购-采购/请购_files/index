        t.__esModule = !0,
        t.default = {
            name: "ElFooter",
            componentName: "ElFooter",
            props: {
                height: {
                    type: String,
                    default: "60px"
                }
            }
        }
    }
    , function(e, t, i) {
        "use strict";
        var n = function() {
            var e = this
              , t = e.$createElement;
            return (e._self._c || t)("footer", {
                staticClass: "el-footer",
                style: {
                    height: e.height
                }
            }, [e._t("default")], 2)
        }
          , s = []
          , r = {
            render: n,
            staticRenderFns: s
        };
        t.a = r
    }
    ])
});;
$(function () {
    window.diagnosticVue = new Vue({
        el: '#diagnosticDiv',
        data: {
            showConsole: false,
            showClearBox:false,
            isStart: false,
            tabpanel: null,
            activeIndex: 0,
            activeName:0,
            logTabs: [],
            disableList: [],
            round: null,
            disableConfigList:[]
        },
        mounted: function () {
            this.init();
        },
        watch: {
            
        },
        created: function () {
            var that = this;
            //window.diagnostic.$on('openConsole', function () {
            //    that.openOrCloseConsole(true);
            //});
            //window.diagnostic.$on('closeConsole', function () {
            //    that.openOrCloseConsole(false);
            //});
        },
        methods: {
            init: function () {
                this.tabpanel = this.$refs.tabs;

                var _this = this;
                $.post(this.getUrl('IsInDiagnosisModel'), {}, function (data) {
                    if (data.isIn) {
                        _this.goStart();
                        _this.disableList = data.list;
                        _this.openOrCloseConsole(false);
                    }
                    _this.disableConfigList = data.disableConfigList;
                });
                
                document.onkeydown = function(e) {
                    var e = event || window.event;
                    if (65 == e.keyCode && e.ctrlKey){
                        e.preventDefault();
                        _this.selectAll();
                    }
                }
                $(document).on('click mousedown',function(e){
                    if($(e.target).closest("#clearBox").length == 0 ){
                        _this.hideClearBox();
                    }
                })
            },
            enter: function (el, done) {
                $(el).addClass("run-enter-active");
                el.addEventListener("animationend", function() {  
                    $("#bell").draggable({
                        cursor:"move",
                        start:function(){
                           
                        },
                        stop:function(){
                       
                        }
                    });
                });
            },
            getUrl: function (action) {
                return "../diagnostic/" + action;
            },
            getActiveTab: function () {
                for (var i = this.logTabs.length - 1; i >= 0; i--) {
                    if (this.logTabs[i].name == this.activeIndex + '') {
                        return this.logTabs[i];
                    }
                }
            },
            openOrCloseConsole: function (isF) {
                this.showConsole = isF;
                if (isF) {
                    window.zlayer.openDiv({
                        content: $("#diagnosticConsole"),
                        width: 900,
                        height: 500,
                        title: "诊断模式"
                    });
                    $("#diagnosticConsole").closest(".layui-layer").find(".layui-layer-min").remove();
                }else{
                    this.showClearBox = false;
                }
            },
            startRound: function () {
                this.round = setInterval(function () {
                    if (this.isStart) {
                        var _this = this;
                        $.post(this.getUrl('GetQueue'), {}, function (data) {
                            _this.getActiveTab().messageQueue.push.apply(_this.getActiveTab().messageQueue, data.list);
                            if (data && data.list && data.list.length > 0) {
                                data.list.forEach(function(x, index, a){
                                   x.message = x.message.replace(/\r|\n|\\s/g,"<br>");
                                });
                                setTimeout(function () {
                                    var d = $("#consoleLog-" + _this.getActiveTab().name);
                                    d[0].scrollTop = d[0].scrollHeight;
                                }, 0);
                            }
                        });
                    }
                    else {
                        clearInterval(this.round);
                        this.round = null;
                    }
                }.bind(this), 1000);
            },
            doStart: function () {
                $.post(this.getUrl('Start'), { disableList: this.disableList }, (function (_this) {
                    return function (data) {
                        if (data.isOk) {
                            _this.goStart();
                        }
                        else {
                            alert("启动诊断模式报错，详情请看log日志");
                        }
                    }
                })(this));
                //this.activeIndex = newTabName;
            },
            goStart: function () {
                this.isStart = true;
                var newTabName = ++this.activeIndex + '';
                this.logTabs.push({
                    title: '诊断日志' + this.activeIndex,
                    name: newTabName,
                    messageQueue: []
                    //content: '<div id=\'log' + newTabName+'\'><textarea/></div>'
                });
                this.activeName = newTabName;
                this.startRound();
                $("#diagnosticConsole").closest(".layui-layer").find(".layui-layer-close").click();           
            },

            stop: function () {
                $.post(this.getUrl('End'), {}, (function (_this) {
                    return function (data) {
                        if (data.isOk) {
                            _this.isStart = false;
                            clearInterval(_this.round);
                            _this.round = null;
                        }
                        else {
                            alert("停用诊断模式报错，详情请看log日志");
                        }
                    }
                })(this));
            },
            selectAll:function(){
                var _this = this;
                 if (document.selection) {
            　　　　　//IE
                    var range = document.body.createTextRange();
                    range.moveToElementText(document.getElementById('consoleLog-'+_this.activeName));
                    range.select();
                } else if (window.getSelection) {
                    var selection = window.getSelection();
                    var range = document.createRange();
                    range.selectNode(document.getElementById('consoleLog-'+_this.activeName));
                    selection.removeAllRanges();  
                    selection.addRange(range);
                }
            },
            hideClearBox:function(tab, event) {
                this.showClearBox = false;
            },
            contextmenu:function(event){
                var e = event || window.event;
                var x = e.clientX;
                var y = e.clientY;
                this.showClearBox = true;
                $("#clearBox").css({"left":x+"px","top":y+"px"});
            },
            clear:function(e){
               var e = e || event;
               e.stopPropagation();
               for (var i = this.logTabs.length - 1; i >= 0; i--) {
                    if (this.logTabs[i].name == this.activeName + '') {
                        this.logTabs[i].messageQueue = [];
                    }
                }
               this.showClearBox = false;
            },
            getColorForType: function (message) {
                if (message.type == "SystemInfo")
                    return "black";
                else if (message.type == "Personalize")
                    return "blue";
                else if (message.type == "DataAuthority")
                    return "red";
                else if (message.type == "CustomerPluginBE")
                    return "green";
                else if (message.type == "CustomerPluginBP")
                    return "green";
                else if (message.type == "CustomerPluginUI")
                    return "green";
                else if (message.type == "RequestStart")
                    return "black";
                else if (message.type == "RequestEnd")
                    return "black";
            },
            getTitleForType: function (message) {
                var str = "";
                if (message.type == "SystemInfo")
                    str = "【系统信息】";
                else if (message.type == "Personalize")
                    str = "【个性化】";
                else if (message.type == "DataAuthority")
                    str = "【数据权限】";
                else if (message.type == "CustomerPluginBE")
                    str = "【BE插件】";
                else if (message.type == "CustomerPluginBP")
                    str = "【BP插件】";
                else if (message.type == "CustomerPluginUI")
                    str = "【UI插件】";
                else if (message.type == "RequestStart") {
                    message.begin = "===========================================================";
                    str = "【请求开始】";
                }
                else if (message.type == "RequestEnd") {
                    message.end = "===========================================================";
                    str = "\n【请求结束】";
                }
                str += "#" + message.threadId;
                return str;
            }
        }
    });
});
;
