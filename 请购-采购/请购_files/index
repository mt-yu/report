/*!
@Name：Defalut页面和Simple页面辅助类
@Author：MAHX
*/
var DefaultJsManager = function (_window) {
    this._window = _window;
    this.showDemoImg = function () {
        var _window = this._window;
        var pageUrl = _window.location.href;
        var demoImg
        if (pageUrl.indexOf("homenid") > -1) {
            demoImg = _window.document.getElementById("demoImg");
            demoImg.src = "../images/index.gif";
        }
        else if (pageUrl.indexOf("myu9nid") > -1) {
            demoImg = _window.document.getElementById("demoImg");
            demoImg.src = "../images/myu9.gif";
        }
        else if (pageUrl.indexOf("blank") > -1) {
            demoImg = _window.document.getElementById("demoImg");
            demoImg.src = "../images/index.gif";
        }
        else if (pageUrl.indexOf("Blank") > -1) {
            demoImg = _window.document.getElementById("demoImg");
            demoImg.src = "../images/index.gif";
        }
        _window.document.getElementById("demoDiv").style.display = "block";
    };

    this.fixedParent = function () {
        var _window = this._window;
        var editTd = _window.document.getElementById("editorTd");
        if (editTd.children.length > 0) {
            var inputs = editTd.getElementsByTagName("input");
            for (var i = 0; i < inputs.length; i++) {
                if (inputs[i].type == "checkbox" && inputs[i].disabled) {
                    inputs[i].parentElement.parentElement.style.display = "none";
                    break;
                }
            }
            var partTd = _window.document.getElementById("webpartTd");
            var tables = partTd.getElementsByTagName("table");
            for (var i = 0; i < tables.length; i++) {
                if (tables[i].className == "selectedPartChrome") {
                    var columnTable = tables[i].parentElement.parentElement.parentElement.parentElement;
                    if (columnTable) {
                        columnTable.style.tableLayout = "fixed";
                        break;
                    }
                }
            }
        }
    };
    this.isDefPWD = function (isDefPWD) {
        if (isDefPWD === "True") {
            this._window.location.href = '../erp/simple.aspx?lnk=bcfcb2f3-cbfd-444f-8ecf-ead98250a774';
        }
    };
    this.init = function () {
        var _window = this._window;
        $(_window.document).ready(function () {
            var winLoc = _window.location.href;
            var _revise = $("#__Revise_KEY");
            if (winLoc.indexOf("display.aspx") > 0) {
                if (_revise.length > 0)
                    _window.revise10Row();
                _window.reivseEditorContainer();
            }
            $(_window.document).click(function (event) { 
                // ：visible 判断是否显示  显示：true ，隐藏： false
                if (_window.top.menuManager && $(_window.top.menuManager.moduleBox).is(":visible")) _window.top.menuManager.closeBox();
            });
            if (_window.top.TabPanelManager) _window.top.TabPanelManager.checkTabTitle(_window.frameElement.id);

            _window.onunload = function () {
                if (_window.top.TabPanelManager) _window.top.TabPanelManager.frameBeforeUnLoad(_window.frameElement.id);
            }
        });
    };
}
DefaultJsManager.prototype = {
    sysInit: function () {
        var _window = this._window;
        if (_window.Sys && _window.Sys.WebForms) {
            _window._ClientPostBackManager.prm = _window.Sys.WebForms.PageRequestManager.getInstance();
            _window._ClientPostBackManager.prm.add_initializeRequest(_window._ClientPostBackManager.onInitRequest);
            _window._ClientPostBackManager.prm.add_beginRequest(_window._ClientPostBackManager.onBeginRequest);
            _window._ClientPostBackManager.prm.add_beginRequest(_window.reivseEditorContainer);
            _window._ClientPostBackManager.prm.add_endRequest(_window._ClientPostBackManager.onEndRequest);
            _window._ClientPostBackManager.prm.add_endRequest(_window.revise10Row);
            _window.Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(_window.afterOnLoad);
            _window.Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(function () { _window.setTimeout(_window.afterPageLoadComplete, 0) });
        }
    }
}

var SimpleJsManager = function (_window) {
    this._window = _window;
    this.divDebugger4AlertHref12Click = function () {
        var _window = this._window;
        if (_window.divDebugger4AlertHref1.clicked == '1') {
            if (_window.event.shiftKey == true) {
                if (_window.$open) {
                    $open();
                }
            } else if (_window.event.ctrlKey == true) {
                var lct = _window.top.location.href;
                var nlct = lct.replace(/\/erp\/simple.aspx/i, "/errors/pdisplay.aspx");
                if (nlct.length == lct.length) {
                    nlct = lct.replace(/\/ufsoft\/simple.aspx/i, "/errors/pdisplay.aspx");
                }
                _window.top.location.href = nlct;
            }
            else {
                //_window.clipboardData.setData('Text', _window.document.location.href);
                Clipboard.setData(_window.document.location.href);
                alert(_window.document.location.href);
            }
        }
    };
    this.myOnLoad = function () {
        var _window = this._window;
        var hideStatusBar = true;
        if (_window.dialogArguments) {
            hideStatusBar = _window.ToBoolean(_window.dialogArguments.hideStatusBar);
            if (_window.dialogArguments.ufTitle != null && _window.dialogArguments.ufTitle != "")
                _window.document.title = _window.dialogArguments.ufTitle;
        }
        if (hideStatusBar)
            $("#ufStatusBarRow").hide();
        else
            $("#ufStatusBarRow").show();
        var mpb = $(".PersonalizationBar");
        if (mpb.length > 0) {
            var mul = mpb.find("ul");
            var mpbw = mpb.width();
            var mulw = mul.width();
            if (mpbw < mulw) {
                mul.css("right", 0); //-2与页边分离
            }
            mpb.attr("widthFixed", true);
        }
    }
    this.init = function () {//===by zzq 老马让暂时注释
        //var _window = this._window;
        //var _this = this;
        //$(_window.document).ready(function () {
        //    _this.myOnLoad();
        //    if (_window.top.TabPanelManager) _window.top.TabPanelManager.checkTabTitle(_window.frameElement.id);
        //    if (_window.top.TabPanelManager) {
        //        _window.onunload = function () {
        //            _window.top.TabPanelManager.frameBeforeUnLoad(_window.frameElement.id);
        //        }
        //    }
        //});
    }
}
SimpleJsManager.prototype = DefaultJsManager.prototype;
;
//审批插件
;(function(window, jQuery, undefined) {
    var $=jQuery;
    //多语，调用红旭提供的接口
    var ApprovalLanguage=NewPortal.ResourceDataManager;
    //服务
    ApprovalStatusPanel.ServerTools=function(){
                     return {
                            getApproval: function(recoreIDAndentitityFullName,callback) {
                                var ary=[];
                                ary[0]=recoreIDAndentitityFullName;
                                $.ajax({
                                    url: "../main/GetWorkflowProcessLogInfo",
                                    data: { "entityInfo": ary },
                                    type: "POST",
                                    traditional: true,
                                    success:callback,
                                    error:callback //本地调试
                                    /*error: function () {
                                        console.log('fail');
                                    }*/
                                });
                            }                   
                     };
        };
    //审批
    function ApprovalStatusPanel() {
        //审批 $("#approvalStatusPanel")
        this.approval=null;
        //传进来的参数
        this.recoreIDAndentitityFullNames=null;
        //$("#approvalStatusPanelUl")
        this.approvalUl=null;
    }
    ApprovalStatusPanel.prototype = {//这里是重写了原型，这样构造函数指向就出现了问题，需要手动去修正constructor
        //修正constructor
        constructor:ApprovalStatusPanel,
        defaults :{
              //缓存的数据，不刷新一直存在，刷新则清空 
              cacheData:{},
              //#approvalStatusPanelUl的最大高度
              maxHeight:500
        },
        //html模板
        template:{
            //approval 外层 
            approval: '<div class="approval-div" id="approvalStatusPanel"></div>',
            //approval 外层 
            approvalUl: '<ul class="approval-ul" id="approvalStatusPanelUl"></ul>',
            //关闭
            close:'<div class="approvalHeader"><i class="fa fa-times close-ex" id="close-ex"></i></div>',
            //覆盖背景条
            cover:'<div class="cover-bar"></div>' 
        },
        //class
        classes:{
                cssListLeft : "list-left",
                //审批时间
                cssApproveTime : "approve-time",
                //审批人
                cssOperateUser : "operate-user",
                //节点名称
                cssListInfo : "list-info-con",
                //审批意见
                cssApproveOptions : "approve-options",
                //时间轴图标
                cssTimeLineIcon : "timeline-icon",
                //审批list
                cssApprovalList : "approval-list",
                //更多
                cssMoreEx : "more-ex",
                //关闭
                cssClose:"close-ex",
                //时间间隔
                cssTimeInterval:'time-interval',
                //印章
                cssStamp:'stamp',
                //同意
                cssStampPass:'stampPass',
                //拒绝
                cssStampRefulse:'stampRefulse',
                //括号
                cssBracket:'bracket-bar'
        },
        id:{
                idApproval : "approvalStatusPanel",
                idClose:"close-ex",
                approvalBtn:"approvalBtn",
                idApprovalUl:"approvalStatusPanelUl",
                lastList:"lastList"
        },
        //初始化
        init:function(recoreIDAndentitityFullName){
               var _this=this;
               //参数存进this.recoreIDAndentitityFullNames
               this.recoreIDAndentitityFullNames=recoreIDAndentitityFullName;

               //取缓存数据
               var cacheData=this.defaults.cacheData;
               //取缓存数据的value对应recoreIDAndentitityFullName（传进来的参数）的值，
               //即跟服务器取的数据再经过处理后一致
               var cacheDataValue=cacheData[recoreIDAndentitityFullName];
              //取到缓存数据，判断数据存在，则
              if(cacheDataValue){
                     this.renderDom(cacheDataValue);
              }
              //没有缓存数据,每次刷新走一次，缓存数据，
              else{
                     //get服务
                     var server=ApprovalStatusPanel.ServerTools();
                     server.getApproval(recoreIDAndentitityFullName,function(data){
                              //var data=_this.getData(data);    、
                              if(data &&　typeof(data)!="string" && data.length > 0){
                                    var dataHasProcess=ApprovalStatusPanel.prototype.dataProcess(data);
                                    //缓存数据
                                    if(recoreIDAndentitityFullName!="" || recoreIDAndentitityFullName!=null){
                                         //将data缓存,其中传进来的参数作为value值
                                         cacheData[recoreIDAndentitityFullName]=dataHasProcess;
                                    }
                                    //警告：此时的this不是ApprovalStatusPanel
                                    ApprovalStatusPanel.prototype.renderDom(dataHasProcess);
                              }else{
                                   $("#"+_this.id.idApproval).remove();
                              }
                          //释放内存
                          server=null;  
                     });
              }
              
        },
        getData:function(data){
              var _data;
              try {
                  _data=$.parseJSON(data);  
                  //_data=$.parseJSON(localdata); //本地数据 假数据 服务通后要注释掉
              } catch(err){
                  _data=null;
              } 
              return _data;
        },
        //处理数据
        dataProcess:function(data){
             //日期处理 DateProcess
              var dateProcess=DateProcess.GetInstance(); 
              var getTimeForText,//日期的文字显示，
                  approveTimeInterval;//时间间隔
                  //状态，节点名称,节点类型,节点唯一标识,实例状态code,处理动作标识
              var action,nodeName,nodeType,nodeID,stateCode,operationCode;
              var i;
              //for循环data
              for(i=0;i<data.length;i++){
                          //第i个data
                          var item=data[i];
                          //"Act":"提交,开始,Start,WFd287ed89fb0a42e5a608c38683db530b,0,4"  
                          // Act=0处理动作，1节点名称，2节点类型，3节点唯一标识，4实例状态，5处理动作标识  6 头像
                          action=item.Act.split(",");
                          nodeName=action[1];
                          nodeType=action[2];
                          nodeID=action[3];
                          stateCode=action[4];
                          operationCode=action[5];
                          photo=action[6];
                          //日期另外一种方式显示
                          getTimeForText=dateProcess.getTimeForText(item.ApproveTime);
                          //get审批开始到审批结束的时间间隔
                           if(i<data.length-1){
                               approveTimeInterval=dateProcess.getTimeInterval(item.StartTime,data[i+1].ApproveTime);
                           //最后一个li
                           }else{
                                approveTimeInterval="";
                           }
                          //operationCode为空，说明是待审核状态
                          //非多人审核
                          if(operationCode=="" && nodeType!="Counter-Sign" &&　nodeName==""){
                               //=待审核
                               nodeName=ApprovalLanguage.get("ActionIconTitle")[8];
                          }
                          //追加以下value及值
                          //日期文字显示
                          data[i].ApproveTimeForText=getTimeForText;
                          //状态图标的className
                          data[i].ActionIconInfo=this.getActionIconInfo(operationCode);
                          //时间间隔
                          data[i].ApproveTimeInterval=approveTimeInterval;
                          //nodeName
                          data[i].NodeName=nodeName;
                          //多人活动值Counter-Sign，其他单人活动
                          data[i].NodeType=nodeType;
                          //多人活动ID
                          data[i].NodeID=nodeID;
                          //实例状态
                          data[i].StateCode=stateCode;
                          //处理动作标识
                          data[i].operation=operationCode;
                          //头像
                          data[i].photo=photo;
               } 
              return data;
        },
        //显示
        show: function(recoreIDAndentitityFullName) {
            var approvalBtn=$("#"+this.id.approvalBtn);
            approvalBtn.attr("recoreIDAndentitityFullName",recoreIDAndentitityFullName);
            //参数不存在或者为空
            if(!recoreIDAndentitityFullName || recoreIDAndentitityFullName=="")return;
            //如果审批按钮isvisibe==none，表示前一个是隐藏显示，则状态保持一致，也是隐藏显示，return。
            if(approvalBtn.attr("isvisibe")!="no"){
                var approval=$("#"+this.id.idApproval);
                //非刷新状态下，当前审批流，如果dom已经 render 完毕
                //id为approval的长度不为0，
                //并且取this.recoreIDAndentitityFullNames跟传进来的参数完全一致时，
                //则直接显示
                if(approval.length!=0 &&  (this.recoreIDAndentitityFullNames==recoreIDAndentitityFullName)){
                     this.block();
                }else{
                   //初始化，将参数传入
                   this.init(recoreIDAndentitityFullName);
                }
                approval=null;
            }
        },
        //直接将#approvalStatusPanel  display:block  
        block:function(){
            var approval=$("#"+this.id.idApproval);
            if(approval.length!=0){
                approval.show();
            }
        },
       //直接将#approvalStatusPanel  display:none
        hide: function() {
            var approval=$("#"+this.id.idApproval);
            if(approval.length!=0){
                approval.hide();
            }
        },
        remove:function(){
             $("#"+this.id.approvalBtn).removeAttr("recoreidandentitityfullname");
             var approval=$("#"+this.id.idApproval);
             if(approval.length!=0){
                 approval.remove();
             }
        },
       
        // render Dom
        renderDom:function(data){
            //create 外皮
            this.renderWrap();
            //append 右上角的关闭
            this.renderClose();
            //renderUl
            this.renderUl();
            //create li
            this.renderLists(data);
            //create 最后一个li
            this.renderLastList(data);
            //事件
            this.bindEvent();
        },
        renderWrap:function(){
            //取id #approvalStatusPanel
            var idApproval=this.id.idApproval;
            //先清空
            if ($("#"+idApproval).length != 0) {
                 $("#"+idApproval).remove();
            }
            //给body render 一个id为approval的ul
            //this.template.approval  外皮模板
            var tabpanel=$("#tab").children(".tabpanel");
            if(tabpanel.length ==1 ){
                tabpanel.append(this.template.approval);
            }else{
                $("body").append(this.template.approval);
            }
            
            //给属性赋值
            this.approval=$("#"+idApproval);
          },
        //append 右上角的关闭
        renderClose:function(){
            //this.template.close  close模板
            this.approval.append(this.template.close);
            $("#"+this.id.idClose).attr("title",ApprovalLanguage.get("Close"));
        },
        //approvalUl
        renderUl:function(){
            //this.template.approvalUl  approvalUl
            this.approval.append(this.template.approvalUl);
            this.approvalUl=$("#"+this.id.idApprovalUl);
        },
        // render li
        renderLists:function(data){
              //className
              var classes=this.classes;
              var list="";
              //for循环data
              for(var i in data){
                    //第i个data
                   var item=data[i];
                   list+=this.getList(i,item);
               } 
               this.approvalUl.html(list);
        },
        getFirstLiCoverHtml:function(i){
                   //第一个li要追加一个白色覆盖div
                   var cover="";
                   if(i==0){
                        cover=this.template.cover;
                   } 
                   return cover;
        },
        gettimeIntervalHtml:function(approveTimeInterval){
                   var timeIntervalHtml;
                   //等于空,表示没有内容
                   if(approveTimeInterval=="" || !approveTimeInterval){
                         timeIntervalHtml=""; 
                   }else{
                         timeIntervalHtml='<span class="time-interval">'+approveTimeInterval+'</span>';
                   }
                   return timeIntervalHtml;
        },
        getTagLi:function(NodeType,NodeID){
           var tagLi='<li>';
           //值为Counter-Sign说明是多人活动
           if(NodeType=="Counter-Sign"){
                  tagLi='<li CounterSignID="'+NodeID+'">';
            }
            return  tagLi;
        },
        getAvatarHtml:function(imageID,OperateUser){
                   var avatarHtml;
                   if(imageID !="" && imageID){
                          //图片地址
                           var imageSrc='../../gridfs/id/'+imageID+'.jpg';
                           avatarHtml='<img class="approval-avatar" src='+imageSrc+' width="36px">';
                   }else{
                          //人名首字母
                          var operateUserFirstLetter=this.getFirstLetter(OperateUser);
                          avatarHtml=operateUserFirstLetter;
                   }
                   return avatarHtml;
        },
        getList:function(i,item){
                   //有头像显示头像，否则显示首字符
                   var avatarHtml=this.getAvatarHtml(item.photo,item.OperateUser);
                   //className
                   var classes=this.classes;
                   //审批时间
                   var approveTime=this.strSubString(item.ApproveTime,3);
                   //审批意见
                   var ApproveOptions=this.getApproveOptions(item.ApproveOptions,item.ActionIconInfo.title);
                   var timeIntervalHtml=this.gettimeIntervalHtml(item.ApproveTimeInterval);
                   //第一个li要追加一个白色覆盖div
                   var getFirstLiCoverHtml=this.getFirstLiCoverHtml(i);
                   //li标签
                   var tagLi=this.getTagLi(item.NodeType,item.NodeID);
                   var list=
                      tagLi
                      +'<div class="'+classes.cssApprovalList+'">'
                        //第一个li要追加一个白色覆盖div
                        +getFirstLiCoverHtml
                        +'<span class="triangle-left"></span>'
                        +timeIntervalHtml
                        +'<i class="fa  '+classes.cssTimeLineIcon+' '+item.ActionIconInfo.icon+'" data-toggle="tooltip" data-placement="bottom" title="'+item.ActionIconInfo.title+'"></i>'
                        +'<dl class="approval-dl">'
                            +'<dd class="approval-dd-1">'
                                //审批日期
                                +'<small class="'+classes.cssApproveTime+'" data-toggle="tooltip" data-placement="bottom" title="'+approveTime+'">'+item.ApproveTimeForText+'</small>'
                                //首字母
                                +'<div class="'+classes.cssListLeft+'">'+avatarHtml+'</div>'
                                +'<div class="list-right">'
                                     //审批人员
                                    +'<p class="'+classes.cssOperateUser+' text-ellipsis" ><span data-toggle="tooltip" data-placement="bottom" title="'+item.OperateUser+'">'+item.OperateUser+'</span></p>'
                                     //节点名称
                                    +'<p class="list-info"><small class="'+classes.cssListInfo+'"  title="'+item.NodeName+'">'+item.NodeName+'</small></p>'
                                +'</div>'
                            +'</dd>'
                            +'<dd class="approval-dd-2">'
                               //审批意见
                              +'<i class="fa fa-comment"></i><small class="'+classes.cssApproveOptions+'"  title="'+ApproveOptions+'">'+ApproveOptions+'</small>'
                            +'</dd>'
                        +'</dl>'
                    +'</div></li>';
                    return list;
        },
        getApproveOptions:function(Options,icon){
                   var ApproveOptions;
                   if(Options!="" && Options){
                       ApproveOptions=Options;
                   }else{
                       ApproveOptions=icon;
                   }
                   return ApproveOptions;
        },
        //去掉字符串后第n位
        strSubString:function(str,n){
             if(!n){n=1;}
             return  str.substring(0,str.length-3);   
        },
        // render 最后一个li
        renderLastList:function(data){
                 //最后一条数据
                var lastData=data[data.length -1];
                var lastList=
                            '<li class="stamp-li" id="lastList"><div class="'+this.classes.cssApprovalList+'">'
                                +this.getLastListConHtml(lastData)
                                //覆盖层
                                +this.template.cover
                            +'</div></li>';
                this.approvalUl.append(lastList);

        },
        getStampHtml:function(operation){
                 var stamp="";
                 //同意
                  if(operation=="9"){
                      stamp=this.classes.cssStampPass;
                  //拒绝
                  }else if(operation=="10"){
                      stamp=this.classes.cssStampRefulse;
                  } 
                  return stamp;
        },
        getLastListConHtml:function(lastData){
                  var stateCode=lastData.StateCode;
                  //0-活动  3完成 -2中止 7是待分配处理者  
                  var actionInfo=this.getActionInfoForLastList(stateCode,lastData.operation);
                  var stampHtml="";
                  if(stateCode==3){//完成
                        stampHtml='<i class="stamp '+this.getStampHtml(lastData.operation)+'"></i>';
                 }else if(stateCode==7){
                        //7待分配处理者
                        stampHtml='<div class="last-li-text">'+ApprovalLanguage.get("WaitingFor")+'('+lastData.OperateUser+')'+actionInfo.text+'</div>';
                 }else{
                       //0活动 2中止
                        stampHtml='<div class="last-li-text">'+actionInfo.text+'</div>';
                 }
                 //图标
                 var iconHtml='<i class="fa '+actionInfo.icon+' timeline-icon" title='+actionInfo.text+'  data-toggle="tooltip" data-placement="bottom"></i>';
                 return stampHtml+iconHtml;
        },
        getActionInfoForLastList:function(stateCode,operation){
               //0-活动  3完成 -2中止 7待分配处理者
               switch (stateCode)
                {
                     case "0":
                         //暂停图标
                         return  {"icon":"fa-pause-circle","text":ApprovalLanguage.get("Activity")};
                     case "2":
                         //拒绝图标
                         return  {"icon":"fa-times-circle-22","text":ApprovalLanguage.get("ProcessStop")};
                     case "3":
                           //同意
                          if(operation=="9"){
                               //完成 结束图标
                               return  {"icon":"fa-end-circle","text":"end"};
                          //拒绝
                          }else{
                               //完成 结束图标
                               return  {"icon":"fa-end-warning-circle","text":"end"};
                          } 
                         
                     //7待分配处理者
                     case "7":
                         //暂停图标
                        return  {"icon":"fa-pause-circle","text":ApprovalLanguage.get("PendingHandler")};
                     default:
                         //警告图标
                        return  {"icon":"fa-warning-circle","text":""};
                }
        },
        //获取名字的第一个字
        getFirstLetter:function(str){
              if(str){
                return str.charAt(0);
              }else{
                return "";
              }
        },
        //get状态图标的信息
        getActionIconInfo:function(action){ 
               var iconTitle=ApprovalLanguage.get("ActionIconTitle");
               //0 中止  1挂起 2重启 4提交 5转发 6退回 7撤回 8 加签  9 同意 10 拒绝  null=待审核
               switch (action)
                {
                case "0":
                      return {"icon":"fa-warning-circle","title":iconTitle[0]};
                case "1":
                       return {"icon":"fa-hang-warning-circle","title":iconTitle[1]};
                case "2":
                       return {"icon":"fa-restart-circle-color","title":iconTitle[2]};
                //case "start":
                case "4":
                      return {"icon":"fa-play-circle","title":iconTitle[4]};
                case "5":
                      return {"icon":"fa-forward-warning-circle","title":iconTitle[5]};
                case "6":
                       return {"icon":"fa-return-warning-circle","title":iconTitle[6]};
                case "7":
                       return {"icon":"fa-recall-warning-circle","title":iconTitle[7]};
                case "8":
                      return {"icon":"fa-addSign-warning-circle","title":iconTitle[8]};
                //case "agree":
                case "9":
                      return {"icon":"fa-ok-circle-color-22","title":iconTitle[9]};
                case "10":
                      return {"icon":"fa-times-circle-22","title":iconTitle[10]};
                /*case "end":
                      return {"icon":"fa-end-circle","title":"提交"};*/
                //待审核
                case "":
                      return {"icon":"fa-pause-circle","title":iconTitle[11]};     
                default:
                      return {"icon":"fa-warning-circle","title":""};
                }
        },
        
        //事件
         bindEvent: function() {
            var _this = this;  //this -> ApprovalStatusPanel
            //加载完毕
            $(document).ready(function(){
                    //时间间隔显示控制
                    //左时间间隔位置，垂直居中
                     _this.SetMarginTopForTimeInterval();
                    //显示更多
                     _this.ShowApproveMore();
                     //append批括号，多人活动
                     _this.AppendBracket();
                     //改变各个括号的高度
                     _this.ChangeEachBracketHeight();
                    //click"更多"
                     _this.OnApproveMoreClicked();
                     //click时间
                     _this.OnApproveTimeClicked();
                     //单击关闭按钮，整个审批隐藏
                     _this.OnApproveCloseClicked();
                     //设置最大高度
                     _this.setApprovalUlMaxHeight();
                     //限制ApprovalStatusPanelUl高度
                     _this.ChangeApprovalStatusPanelUlHeight();
                     //6.0提示init
                     _this.initTooltip();
                     //6.0拖拽
                     _this.draggableForApproveBox();
                  
            });
        },
         initTooltip:function()
        { //提示
            $('[data-toggle="tooltip"]').tooltip();
        },
        draggableForApproveBox:function(){
            var that=this;
                 //jquery-ui draggable 拖拽
            $("#"+this.id.idApproval).draggable({
                cursor:"move",
                cancel: "#"+that.id.idApprovalUl+" li"
            })
        },
        //#ApprovalStatusPanelUl的最大高度
        setApprovalUlMaxHeight:function(){
               var _this=this;
               var windowHeight=$(window).height();
               if(windowHeight==0){
                   windowHeight=window.screen.height-150;
               }
               //距离顶部的top值
               // $(#approvalStatusPanel)
               var idApprovalOffsetTop=$("#"+this.id.idApprovalUl).offset().top;
               //留出间隔
               var space=50;
               //最大高度
               var maxHeight=windowHeight-idApprovalOffsetTop-space;
               //最大高度存起来
               _this.defaults.maxHeight=maxHeight;
        },
        //限制ApprovalStatusPanel高度
        ChangeApprovalStatusPanelUlHeight:function(){
              //滚动条拖动到最低端的高度
             var scrollTop=$("#"+this.id.idApprovalUl).get(0).scrollHeight;
             this.setApprovalHeight(scrollTop);
        },
        setApprovalHeight:function(scrollTop){
                  var idApprovalUl=$("#"+this.id.idApprovalUl); // $(#approvalStatusPanel)
                  idApprovalUl.height("");
                  var idApprovalHeight=idApprovalUl.outerHeight(true);
                  var maxHeight=this.defaults.maxHeight;
                  //超过最大值则：赋值最大值
                  if(idApprovalHeight > maxHeight){
                       idApprovalUl.outerHeight(maxHeight);
                       idApprovalUl.scrollTop(scrollTop);
                  }  
        },
        //左时间间隔位置，垂直居中
        SetMarginTopForTimeInterval:function(){
                var _this = this; 
                // _this.id.idApproval == "approvalStatusPanel"
                 $("."+_this.classes.cssTimeInterval,"#"+_this.id.idApproval).each(function(index,item) {
                         var $item,itemLineHeight;
                         $item=$(item);
                         itemLineHeight=-1 * $item.height()/2;
                         $item.css("margin-top",itemLineHeight-8);
                  })
        },
        //显示更多
        ShowApproveMore:function(){
                 var _this = this,
                classes=_this.classes;
                // _this.id.idApproval == "approvalStatusPanel"
                $("."+classes.cssApproveOptions,"#"+_this.id.idApproval).each(function(index,item) {
                       var $item=$(item);
                       //超出一行，一行显示，出省略号，添加"更多"
                       if($item.height() > 35){
                          $item.addClass("text-ellipsis");
                          //"更多"片段
                          //ApprovalLanguage 多语
                          var more='<i class="fa '+classes.cssMoreEx+' fa-angle-circle-o-down" title="'+ApprovalLanguage.get("More")+'"></i>';
                           //没有"更多"按钮时，向前追加"更多"按钮
                           if($item.siblings("."+classes.cssMoreEx).length == 0){
                                $item.before(more); 
                           }
                       }         
                    })
        },
        //点击更多
        OnApproveMoreClicked:function(){
                   var _this = this,
                classes=_this.classes;
                // $(#approvalStatusPanel)
                $("#"+_this.id.idApproval).delegate('.'+classes.cssMoreEx, 'click', function () {
                         //初始滚动条位置
                         var scrollTop=$("#"+_this.id.idApprovalUl).scrollTop();
                         //向上向下图标切换
                         $(this).toggleClass("fa-angle-circle-o-down fa-angle-circle-o-up");
                         //省略号增加与否切换
                         $(this).siblings("."+classes.cssApproveOptions).toggleClass("text-ellipsis")
                         //批括号
                         if($(this).closest("li").attr("countersignid")){
                              _this.ChangeEachBracketHeight();
                         }
                         _this.setApprovalHeight(scrollTop);
                  })
                  
        },
        
         //点击时间，审批时间切换
        OnApproveTimeClicked:function(){
                 var classes=this.classes,
                  idApproval=$("#"+this.id.idApproval); // $(#approvalStatusPanel)
                 //审批时间切换
                  idApproval.delegate("."+classes.cssApproveTime,"click", function(e) {
                      $("."+classes.cssApproveTime,idApproval).each(function(index,item){
                             //title 内容互换
                             var _item=$(item);
                             var text=_item.text();
                             var title=_item.attr("title");
                             if(title==""){
                                title=_item.attr("data-original-title");
                                _item.attr("data-original-title",text);
                             }else{
                                 _item.attr("title",text);
                             }
                             _item.html(title);
                             
                       })
                  });
        },
        //html批括号
        AppendBracket:function(){
                    var _this=this;
                    var classes=this.classes; 
                    var countersignid;
                    var countersignidArr=[];
                    var i=0;
                    var bracketHtml='<div class="'+classes.cssBracket+'"></div>';
                    // _this.id.idApproval == "approvalStatusPanel"
                    $("li","#"+this.id.idApproval).each(function(index,item) {
                         countersignid=$(item).attr("countersignid");
                         if(countersignid){
                                 countersignidArr[i]=countersignid;
                                 i++;
                         }
                     })
                     countersignidArr=_this.ArrayUnique(countersignidArr);
                     for(j in countersignidArr){
                           var _countersign=$('[countersignid='+countersignidArr[j]+']')
                           if(_countersign.length > 1){
                                _countersign.each(function(index,item){
                                      var nextItemCountersignid=$(item).next().attr("countersignid");
                                      //如果下一个item也有一样的countersignid，则（中间被子行审批等隔断，没有括号）
                                      if(index !=_countersign.length-1 && nextItemCountersignid==countersignidArr[j]){
                                          $(this).find("."+classes.cssApprovalList).append(bracketHtml);
                                      }
                                })
                                
                           }
                     }
        },
        ArrayUnique :function(arr){
             var res = [];
             var json = {};
             for(var i = 0; i < arr.length; i++){
              if(!json[arr[i]]){
               res.push(arr[i]);
               json[arr[i]] = 1;
              }
             }
             return res;
        },

        //改变各个括号的高度
        ChangeEachBracketHeight:function () {
              var _this=this;
              //批括号
              // _this.id.idApproval == "approvalStatusPanel"
               $("."+_this.classes.cssBracket,"#"+_this.id.idApproval).each(function(index,item) {
                         var closestLi=$(item).closest("li");
                         var nextLi=closestLi.next("li");
                         var border=1;
                         var brackeHeight= (closestLi.height()+nextLi.height())/2 - border;
                         $(item).height(brackeHeight);
               })
        },
        //单击关闭按钮，整个审批隐藏
        OnApproveCloseClicked:function(){
              var _this = this; 
                // $(#approvalStatusPanel)
                $("#"+_this.id.idApproval).delegate("."+_this.classes.cssClose,"click", function(e) {
                      _this.hide();
                      //ApprovalBtn按钮置灰色
                      ApprovalBtn.GetInstance().greyen($("#"+_this.id.approvalBtn));
                });  
        },
        //destory
        destory: function() {
            var approval=$("#"+this.id.idApproval);
            if(approval.length!=0){
                approval.remove();
                delete window.approvalPanel;
            }
        }
    };

    //日期处理，基础类方法 
    function DateProcess(){
    }
    DateProcess.prototype = {
         //修正constructor  
          constructor:DateProcess,
          init:function(){
          },
          getDate:function(date){
              //parse，解析一个表示某个日期的字符串，并返回从1970-1-1 00:00:00 UTC 到该日期对象（该日期对象的UTC时间）的毫秒数，如果该字符串无法识别，或者一些情况下，包含了不合法的日期数值（如：2015-02-31），则返回值为NaN
               var regTime = /(\d{4})-(\d{1,2})-(\d{1,2})( \d{1,2}:\d{1,2})/g;
               var newFormat="$2-$3-$1$4";
               return Date.parse(date.replace(regTime,newFormat));
          },
          //得到星期几 
          getWeek:function(date){
                    var _dateLanguage=ApprovalLanguage;
                    var _date =new Date(this.getDate(date));
                    var weekday=[_dateLanguage.get("Sunday"),_dateLanguage.get("Monday"),_dateLanguage.get("Tuesday"),_dateLanguage.get("Wednesday"),_dateLanguage.get("Thursday"),_dateLanguage.get("Friday"),_dateLanguage.get("Saturday")];
                    return weekday[_date.getDay()];
          },
          //计算时间间隔 
          getTimeInterval:function(startDate,endDate){
             if(!startDate || !endDate || startDate=="" || endDate==""){
                return "";
             }
              //取多语中对应的语言
              var _dateLanguage=ApprovalLanguage;
              var interval,
                  days,
                  hours,
                  minutes,
                  hoursleave,
                  daysleave,
                   seconds;
               //时间差的秒数
               var interval = Math.abs(
                                    this.getDate(endDate)
                                  - this.getDate(startDate)
                            )/1000; 
              //计算出相差天数
              days=Math.floor(interval/(24*3600));
              //计算天数后剩余的毫秒数
              daysleave=interval%(24*3600);
              if(daysleave !=0){    
                      //计算相差小时数
                      hours=Math.floor(daysleave/3600);
                      //计算小时数后剩余的毫秒数
                      hoursleave=daysleave%3600; 
                  if(hoursleave !=0){
                      //计算相差分钟数
                      minutes=Math.floor(hoursleave/60);
                      seconds=hoursleave%60; //剩余的秒数
                  }     
              }
              var approveTimeInterval="";
              if(days>0) approveTimeInterval+= days+_dateLanguage.get("Days")+"<br/>";
              if(hours>0)approveTimeInterval+= hours+_dateLanguage.get("Hours")+"<br/>";
              if(minutes>0)approveTimeInterval+= minutes+_dateLanguage.get("Minutes")+"<br/>";
              if(days==0 && hours==0 && seconds>0)approveTimeInterval+= seconds+_dateLanguage.get("Seconds");
              return approveTimeInterval;
          },
          //审批日期文字显示 
          getTimeForText:function(date){
                 //date不存在或者为空的时候
                 if(date=="" || !date){
                    return "";
                  }
                  //取多语中对应的语言
                  var _dateLanguage=ApprovalLanguage;
                  //现在的时间 ,秒数
                  var nowTime=new Date().getTime();
                  //获取从今天0点开始到现在的时间 ,秒数
                  var todayTime=nowTime%(1000*60*60*(24-8));
                  //今天0点的时间
                  var  zeroTime=nowTime-todayTime;
                  //要判断的日期，秒数
                  var dateTime=this.getDate(date);
                  //时间差，获取要判断的日期和现在时间的偏差
                  var offset=nowTime-dateTime;
                  //今天0点到判断日期的时间差
                  var interval=zeroTime-dateTime;

                  var aday=1000*60*60*24;
                  var today=new Date();
                  var nowWeek=today.getDay();
                  var thisWeekTime=(nowWeek-1)*aday;
                   //现在的时间，日期
                  var nowMonth=today.getMonth();
                  var dateMonth=new Date(dateTime).getMonth();
                  //得到年份
                  var nowYear=today.getYear();
                  var  dateYear=new Date(dateTime).getYear();
                  //本月
                  if (nowYear==dateYear && nowMonth==dateMonth){
                        //本周
                        if(interval<=thisWeekTime){
                              //获取要判断日期距离今天0点有多久
                              if(interval<=0 && interval < aday){

                                 //return "今天";
                                 return _dateLanguage.get("Today");
                              }else if(interval>0 &&  interval<=aday)
                              {
                                 //return "昨天";
                                 return _dateLanguage.get("Yesterday");
                              }else if(interval> aday &&  interval<=2*aday){
                                 //return "前天"
                                 return _dateLanguage.get("BeforeYesterday");
                              }else{
                                 //按照星期显示
                                 return this.getWeek(date);
                              }
                        }
                        //上周
                        else if(interval> thisWeekTime && interval <= thisWeekTime + 7*aday){
                              //return "上周"
                              return _dateLanguage.get("LastWeek");
                        }
                        //两周以前
                        else if(interval> thisWeekTime + 7*aday){
                              //return "两周以前"
                              return _dateLanguage.get("TwoWeeksAgo");
                        } 
                  }
                  else if((nowYear-dateYear==0 && nowMonth-dateMonth==1) || (nowYear-dateYear==1 && nowMonth-dateMonth== -11)){
                       //return "上个月";
                       return _dateLanguage.get("LastMonth");
                  }else{
                       //return "更早";
                       return _dateLanguage.get("Earlier");

                  }
           }
    }
    DateProcess.GetInstance= function(){
          var aDateProcess=new DateProcess();
          aDateProcess.init();
          return aDateProcess;
    }
    
    function ApprovalBtn(){

    }
    ApprovalBtn.prototype={
          constructor:ApprovalBtn,
          id:{
                approvalBtn:"approvalBtn",
                idApproval:"approvalStatusPanel"
          },
          classes:{
              cssLighten:"lighten",
              cssGreyen:"greyen"
          },
          init:function(){
              this.bindEvent();
          },
          bindEvent:function(){
               var _this=this;
               var idApprovalBtn=$("#"+this.id.approvalBtn);
                //点亮按钮
                //v60审批流程记住状态
                 if(window.localStorage){
                         var storage=window.localStorage;
                         var isvisibeValue=storage.getItem("approvalIsvisibe");
                         if(isvisibeValue=="yes"){
                                 _this.lighten(idApprovalBtn);
                         }else{
                                 _this.greyen(idApprovalBtn);
                         }
                 }else{
                       _this.greyen(idApprovalBtn);
                 }
                //_this.greyen(idApprovalBtn);
                idApprovalBtn.click(function (e) {
                        var evt=e ? e:window.event;
                        evt.stopPropagation();
                        var that=$(this);
                        //给#approvalBtn增加标志
                        if(that.attr("isVisibe")=="yes"){
                               //置灰按钮
                                _this.greyen(that);
                                $("#"+_this.id.idApproval).hide();
                        }else{
                               //点亮按钮
                                _this.lighten(that);
                                var recoreIDAndentitityFullName=that.attr("recoreIDAndentitityFullName")
                                approvalPanel.show(recoreIDAndentitityFullName);  
                        }
                  })
          },
          //点亮按钮
          lighten:function(selector){
                 this.toggleBtnState(selector,this.classes.cssLighten,this.classes.cssGreyen,"yes");
          },
          //置灰按钮
          greyen:function(selector){
                 this.toggleBtnState(selector,this.classes.cssGreyen,this.classes.cssLighten,"no");
          },
          toggleBtnState:function(selector,_addClass,_removeClass,isvisibeValue){
                 selector.addClass(_addClass);
                 selector.removeClass(_removeClass);
                 selector.attr("isVisibe",isvisibeValue);
                 //v60审批流程记住状态
                 if(window.localStorage){
                         var storage=window.localStorage;
                         storage.setItem("approvalIsvisibe",isvisibeValue);
                 }
          }
    }
    ApprovalBtn.GetInstance=function(){
            return new ApprovalBtn();
    }
    //提供接口 
    window.approvalPanel =(function(){
        return new ApprovalStatusPanel();
    })();
    
    $(function(){
                //审批按钮
                ApprovalBtn.GetInstance().init();
    })
})(window, jQuery);

;
$(document).ready(function(){
     /****************************************duduService.js*****************************************/
    var tools = {
        ret: '',
        ajaxDuDuService: function (url, callback, isAsync, errorCallback) {
            if (isAsync === "undefined" || isAsync === true) {
                isAsync = true;
            } else {
                isAsync = false;
            }
            $.ajax({
                url: url,
                type: 'get',
                cache: false,
                async: isAsync
            }).done(function (data) {
                if (!(data == "TimeOut")) {
                    if (data.indexOf("由于目标计算机积极拒绝，无法连接") > -1) { tools.ret = "请检查网关是否仍在运行！"; return false; };
                    if (data === "" || data === "yes" || data === "no" || data == "undefined" || data.indexOf("<html") > -1) return false;
                    data = JSON.parse(data);
                    if (data.code == "0023") { tools.ret = "电话会议结束失败，请检查当前登陆人的联系对象中是否设置了电话号码，配置完成后刷新重试！"; return false; }
                    if (!data.session_id) {
                        if (data.isEnpty) { tools.ret = data.isEnpty + "！"; return false; }
                        if (data.result === "-1" && data.code === "-0007") { tools.ret = "请检查当前登陆人的联系对象中是否设置了电话号码！"; return false; }
                        if (data.result === "-1" && data.code === "0009") { tools.ret = "金额不足！"; return false; }
                        if (data.result === "-1" && data.code === "0007") { tools.ret = "请检查网关的嘟嘟配置是否正确！"; return false; }
                        if (data.result === "-1" && data.code === "0008") { tools.ret = "账号已停用！"; return false; }
                        if (data.result === "-1" && data.code === "0005") { tools.ret = "您的app账号已经停用！"; return false; }
                        if (data.result === "-1" && data.code === "0001") { tools.ret = "sign参数错误（可能由于企业帐号不存在导致）！"; return false; }
                        if (data.result === "-1" && data.code === "0020") { tools.ret = "您呼叫的为非法号码！"; return false; }
                        if (data.result === "-1" && data.code === "0004") { tools.ret = "电话会议不能超过12个人！"; return false; }
                        if (!(data.result === "0") && !(data.code === "0000")) return false;
                    }
                }
                tools.ret = '';
                callback(data);
            }).fail(function (data) {
                if (data === "" || data === "yes" || data.indexOf("<html") > -1) return false;
                if (data.status === 500) { tools.ret = "请检查服务器或网关是否仍在运行！"; return false; }
                errorCallback(data);
            });
            return this.ret;
        }
    };
    var url = {
        //   callUrl: 'http://139.217.0.90:90/duduservice/webapi/forwardprocess/Create?url=caller=[caller]|phones=[phones]|userId=1111',
        //   suspendUrl: 'http://139.217.0.90:90/duduservice/webapi/forwardprocess/Over?sessionid=[sessionid]',
        //   participateUrl: 'http://139.217.0.90:90/duduservice/webapi/forwardprocess/Add?url=sessionid=[sessionid]|calledNbr=[calledNbr]',
        //   quitUrl: 'http://139.217.0.90:90/duduservice/webapi/forwardprocess/Remove?url=sessionid=[sessionid]|calledNbr=[calledNbr]'
        callUrl: '../ForwardProcess/Create?url=caller=[caller]|phones=[phones]|userId=1111',
        suspendUrl: '../ForwardProcess/Over?sessionid=[sessionid]',
        participateUrl: '../ForwardProcess/Add?url=sessionid=[sessionid]|calledNbr=[calledNbr]',
        quitUrl: '../ForwardProcess/Remove?url=sessionid=[sessionid]|calledNbr=[calledNbr]',
        getDataUrl: '../AjaxMsg/GetData?sessionid=[sessionid]',
        clearListUrl: '../AjaxMsg/ClearList?sessionid=[sessionid]',
        //usersListUrl: '../AjaxMsg/GetDuDuUsers'
        usersListUrl: '../ForwardProcess/getPersonList'
    };
    var DuDuService = function () {
    };
    DuDuService.prototype.getUsers = function (typeData, data, callback) {
        $.post(url.usersListUrl, { type: typeData, users: data }, callback);
    };
    DuDuService.prototype.getData = function (sessionId, callback, isAsync) {
        var urlTmp = url.getDataUrl.replace("[sessionid]", sessionId);
        return tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    DuDuService.prototype.getData = function (sessionId, callback, isAsync) {
        var urlTmp = url.getDataUrl.replace("[sessionid]", sessionId);
        return tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    DuDuService.prototype.clearList = function (sessionId, callback, isAsync) {
        var urlTmp = url.clearListUrl.replace("[sessionid]", sessionId);
        return tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    DuDuService.prototype.start = function (caller, phones, callback, isAsync) {
        var urlTmp = url.callUrl.replace("[caller]", caller).replace("[phones]", phones);
        return tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    DuDuService.prototype.end = function (sessionId, callback, isAsync) {
        var urlTmp = url.suspendUrl.replace("[sessionid]", sessionId);
        tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    DuDuService.prototype.participate = function (sessionId, calledNbr, callback, isAsync) {
        var urlTmp = url.participateUrl.replace("[sessionid]", sessionId).replace("[calledNbr]", calledNbr);
        tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    DuDuService.prototype.quit = function (sessionId, calledNbr, callback, isAsync) {
        var urlTmp = url.quitUrl.replace("[sessionid]", sessionId).replace("[calledNbr]", calledNbr);
        tools.ajaxDuDuService(urlTmp, callback, isAsync);
    };
    window.DuDuService = DuDuService;

    /****************************************duduAble.js*****************************************/

    var globalTools = {
        url: {},
        ele: {},
        trDataList: [],
        trObjList: [],
        check: {},
        intervalList: [],
        callingTrDataList: [],
        personShowHid: new Array()
    };
    globalTools.deleteSelf = '';
    globalTools.pooling = '';
    globalTools.sessionId = '';
    globalTools.duduService = new DuDuService();
    globalTools.ele.$duduTableTbody = $("#table-dudu tbody");
    globalTools.ele.$toCall = $("#toCall");
    globalTools.ele.$overCall = $("#overCall");
    globalTools.ele.$dudu = $("#dudu");
    globalTools.ele.$addPhone = $("#addPhone");
    globalTools.ele.$addCall = $("#addCall");
    globalTools.ele.$duduSearch = $("#duduSearch");
    globalTools.ele.$inputDiv = $("#inputDiv");
    globalTools.ele.$closeDudu = $("#closeDudu");
    globalTools.ele.$originator = $("#originator");
    globalTools.ele.$picthNum = $("#picthNum");
    globalTools.ele.$chooseAll = $("#chooseAll");
    globalTools.ele.$showperson = $("#showperson");
    globalTools.ele.$hidperson = $("#hidperson");
    globalTools.check.isChecked = function () {
        if ($("#table-dudu tbody input[type=checkbox][id!=chooseAll]:checked").length == 0)
            return false;
        return true;
    };
    globalTools.check.getCheckedPhone = function () {
        var phoneStr = '';
        $("#table-dudu tbody input[type=checkbox][id!=chooseAll]:checked").each(function () {
            phoneStr += $(this).attr("data-phone") + ',';
        });
        return phoneStr.substr(0, phoneStr.length - 1);
    }
    globalTools.Regx = function (regx, target) {
        return target.match(new RegExp(regx, "g"));
    }
    globalTools.duduShowDialog = function (data, isAdd) {
        var trs = data.contactlist.length;
        var htm = '';
        var checked = '';
        for (var i = 0; i < trs; i++) {
            if (!isAdd) {
                globalTools.trDataList.push({ "addBtn": "addBtn" + data.contactlist[i].mobile, "removeBtn": "removeBtn" + data.contactlist[i].mobile, "mobile": data.contactlist[i].mobile, "stateTarget": "state" + data.contactlist[i].mobile });
                if (i >= trs - 1) {
                    globalTools.ele.$originator.text(data.contactlist[i].name);
                    //for (var mob = 0; mob < globalTools.trDataList.length - 1; mob++) {
                    //    if (globalTools.trDataList[mob].mobile == data.contactlist[i].mobile) {
                    //        globalTools.deleteTr = globalTools.trDataList[mob].mobile; //先记下Id，splice之后会改变数组长度和内容
                    //        globalTools.trDataList.splice(mob, 1);
                    //        break;
                    //    }
                    //}
                    break;
                };
            } else {
                globalTools.trDataList.unshift({ "addBtn": "addBtn" + data.contactlist[i].mobile, "removeBtn": "removeBtn" + data.contactlist[i].mobile, "mobile": data.contactlist[i].mobile, "stateTarget": "state" + data.contactlist[i].mobile });
                globalTools.callingTrDataList.push({ "addBtn": "addBtn" + data.contactlist[i].mobile, "removeBtn": "removeBtn" + data.contactlist[i].mobile, "mobile": data.contactlist[i].mobile, "stateTarget": "state" + data.contactlist[i].mobile });
            }
            var showChecked = globalTools.ele.$chooseAll.is(":checked") && 'checked="checked"' || '';
            htm += '<tr id="m' + data.contactlist[i].mobile + '" ><td><input type="checkbox" ' + showChecked + ' data-phone="' + data.contactlist[i].mobile + '" data-flag=0 class="singleSelect"></td>'
                    + '<td><img id="state' + data.contactlist[i].mobile + '" style="display:none;" class="dudu_state" src="../../Images/gray.png"/></td>'
                    + '<td>' + data.contactlist[i].name + '</td>'
                    + '<td class="mobile">' + data.contactlist[i].mobile + '</td>'
                    + '<td>' + data.contactlist[i].department + '</td>'
                    //把这个添人删人放到最后一列
                    + '<td><img src="../../Images/addUser.png"  id="addBtn' + data.contactlist[i].mobile + '" style ="display:none;cursor:pointer;"/><img src="../../Images/removeUser.png" id="removeBtn' + data.contactlist[i].mobile + '" style="display:none;cursor:pointer;"/></td>'
                    + '</tr>';
        }
        globalTools.ele.$duduTableTbody.append(htm);
        if (globalTools.deleteTr) { $("#m" + globalTools.deleteTr).remove(); globalTools.deleteTr = ''; }
        $("#dudu").modal("show");
    }
    globalTools.sumAllCheckB = function () {
        return $("#table-dudu tbody input[type = checkbox][id != chooseAll]").length;
    }
    //其它
    var other = {
        checkNullObject: function (obj) {
            if (typeof obj === "object" && !(obj instanceof Array)) {
                var hasProp = 0;
                for (var prop in obj) {
                    hasProp = 1;
                    break;
                }
                if (!!hasProp) return 0;
                else return 1;
            }
        },
        close: function () {
            call.overCall.endService();
        },
        placeholder: function () {
            globalTools.ele.$addPhone.focus(function () {
                if ($(this).val() === '请输入姓名或电话')//输入多个号码以逗号隔开，固定电话加区号
                    $(this).val('');
            }).blur(function () {
                if ($(this).val() === '请输入姓名或电话' || $(this).val() === '')
                    $(this).val('请输入姓名或电话');
            });
            globalTools.ele.$duduSearch.focus(function () {
                if (globalTools.sessionId.length > 8) {
                    globalTools.ele.$hidperson.click();
                }
                if ($(this).val() === '输入姓名 手机号 部门等')
                    $(this).val('');
            }).blur(function () {
                if ($(this).val() === '输入姓名 手机号 部门等' || $(this).val() === '')
                    $(this).val('输入姓名 手机号 部门等');
            });
        },
        search: function () {
            globalTools.ele.$duduSearch.on('input propertychange', function () {
                var inputVal = $(this).val();
                if (inputVal === '输入姓名 手机号 部门等') return false;
                globalTools.ele.$duduTableTbody.find("tr").each(function () {
                    if (!inputVal) $(this).fadeIn(300);
                    if ($(this).text().indexOf(inputVal) < 0) {
                        $(this).fadeOut(500);
                    } else {
                        $(this).fadeIn(300);
                    }
                });
            });
        },
        checkAll: function () {
            var sumCheckB = 0;
            globalTools.ele.$chooseAll.on('change', function () {
                sumCheckB = globalTools.sumAllCheckB();
                var cur$ = $(this);
                $("#table-dudu tbody input[type = checkbox][id != chooseAll]").each(function () {
                    if (cur$.is(":checked")) {
                        $(this).prop("checked", true);
                        globalTools.ele.$picthNum.text(sumCheckB);
                    } else {
                        $(this).prop("checked", false);
                        globalTools.ele.$picthNum.text(0);
                    }
                });
            });
            $("#table-dudu tbody").on('change', 'input[type = checkbox][id != chooseAll]', function () {
                sumCheckB = globalTools.sumAllCheckB();
                if ($(this).is(":checked")) {
                    if (sumCheckB === $("#table-dudu tbody input[type = checkbox][id != chooseAll]:checked").length) globalTools.ele.$chooseAll.prop("checked", true);
                    globalTools.ele.$picthNum.text(parseInt(globalTools.ele.$picthNum.text()) + 1);
                } else {
                    globalTools.ele.$chooseAll.prop("checked", false);
                    if (globalTools.ele.$picthNum.text() === '0') return false;
                    globalTools.ele.$picthNum.text(parseInt(globalTools.ele.$picthNum.text()) - 1);
                }
            });
        },
        addPerson: function () {
            var addPhone = globalTools.ele.$addPhone.val();
            var trData = {
                "contactlist": []
            };
            if (addPhone === '请输入姓名或电话' || addPhone === '') return false;
            if (other.checkNullObject(autoComplete.getData())) {
                if (globalTools.Regx("^[0-9]*$", addPhone) && globalTools.ele.$duduTableTbody.text().indexOf(addPhone) < 0) {
                    var tmpObj = { "name": "", "mobile": addPhone, "department": "" };
                    trData.contactlist.push(tmpObj);
                }
            } else {
                for (var attr in autoComplete.getData()) {//attr得到的就是key
                    if (globalTools.ele.$duduTableTbody.text().indexOf(attr) < 0) {
                        var tmpObj = { "name": autoComplete.getData()[attr].name, "mobile": attr, "department": autoComplete.getData()[attr].department };
                        trData.contactlist.push(tmpObj);
                    }
                }
            }
            globalTools.duduShowDialog(trData, true);
            if (globalTools.sessionId != "" && globalTools.sessionId != "start") {
                for (var c = 0; c < globalTools.callingTrDataList.length; c++) {
                    eval("(status.state.state" + globalTools.callingTrDataList[c].mobile + "='' )");
                };
                call.toCall.initTrObj(globalTools.callingTrDataList);
                globalTools.callingTrDataList = [];
            }
            globalTools.ele.$addPhone.val('请输入姓名或电话');
            autoComplete.data = {};//清空自动完成的缓存数据
            //if (addPhone.indexOf("，") > 0) { alert("请您使用英文半角逗号！"); return false; }
            //if (addPhone.indexOf("，") > 0) { alert("请您使用英文半角逗号！"); return false; }
            //var aryPhone = addPhone.split(','), aryPhoneValid = '';
            //for (var b = 0; b < aryPhone.length; b++) {
            //    if (globalTools.Regx("^[0-9]*$", aryPhone[b]) && globalTools.ele.$duduTableTbody.text().indexOf(aryPhone[b]) < 0) {
            //        aryPhoneValid += aryPhone[b] + ",";  //aryPhoneValid.push(aryPhone[b]);
            //    }
            //}
            //if (!aryPhoneValid) return false;
            //aryPhoneValid = aryPhoneValid.substr(0, aryPhoneValid.length - 1); //aryPhone = aryPhoneValid.join(',');
            //globalTools.duduService.getUsers(aryPhoneValid, function (data) {
            //if (!(data == "{ \"contactlist\": []}") && data.indexOf("<html x") < 0) {
            //    globalTools.duduShowDialog(JSON.parse(data), true);
            //}
            //else {
            //    var aryPhoneValidTmp = [];
            //    for (var f = 0; f < aryPhone.length; f++) {//获取不到人也应该将号码加入
            //        aryPhoneValidTmp.push({ "name": "", "mobile": "" + aryPhone[f] + "", "department": "" });
            //    }
            //    globalTools.duduShowDialog({ "contactlist": aryPhoneValidTmp }, true);
            //}
            //globalTools.ele.$addPhone.val('请输入姓名或电话');
            //});
        },
        hideCallWindow: function () {
            globalTools.trDataList = [];
            globalTools.ele.$duduTableTbody.empty();
            globalTools.ele.$picthNum.text(0);
            globalTools.ele.$chooseAll.prop('checked', false);
            $("#dudu").modal("hide");
            document.getElementById('contactDataList').parentNode.style.display = "none";//隐藏自动完成框
        }
    };
    other.init = function () {
        this.placeholder();
        this.search();
        this.checkAll();
        //globalTools.ele.$hidperson.on('click', function () {
        //    $(this).fadeOut(function () {
        //        globalTools.ele.$showperson.fadeIn(300);
        //        $("#table-dudu [id^='removeBtn']").each(function () {
        //            globalTools.personShowHid.push($(this).parent().parent());
        //            if (!$(this).attr("isRmv")) {
        //                $(this).parent().parent().fadeOut(100);
        //            }
        //        });
        //    });
        //});
        //globalTools.ele.$showperson.on('click', function () {
        //    $(this).fadeOut(function () {
        //        globalTools.ele.$hidperson.fadeIn(300);
        //        for (var g = 0; g < globalTools.personShowHid.length; g++) {
        //            globalTools.personShowHid[g].fadeIn(300);
        //        }
        //        globalTools.personShowHid = [];
        //    });
        //});
        globalTools.ele.$addCall.on('click', function () {
            other.addPerson();
        });
        globalTools.ele.$closeDudu.on('click', function () {
            if (globalTools.sessionId != "" && globalTools.sessionId != "start") {
                if (confirm("您是否退出电话会议？")) {// if (confirm("您是否挂断通话？")) {
                    other.close();
                } else {
                    return false;
                }
            }
            other.close();
            other.hideCallWindow();
        });
        globalTools.ele.$dudu.on("keydown", function () {
            if (window.event.keyCode === 116) {
                if (confirm("如果已经发起通话，刷新页面将会结束通话！")) {
                    other.close();
                    other.hideCallWindow();
                } else {
                    window.event.keyCode = 0;
                    return false;
                }
            }
        });
    }
    var status = {
        isAddRemo: false,
        //hubUrl: "/U9/signalr",
        state: {}
    };
    status.init = function () {
        for (var u = 0; u < globalTools.trDataList.length; u++) {
            eval("(status.state.state" + globalTools.trDataList[u].mobile + "='' )");
        };
        //$.connection.hub.url = this.hubUrl;
        //var pushMsg = $.connection.msg;
        //pushMsg.client.AddMessage = function (msg) {
        globalTools.pooling = setInterval(function () {
            globalTools.duduService.getData(globalTools.sessionId, function (data) {
                //if (!globalTools.sessionId === data.session_id) return false;
                var callee = '';
                if (globalTools.Regx("^[8]{1}[6]{1}[0-9]*", data.callee)) {
                    callee = '0' + data.callee.substr(2, data.callee.length - 1);
                }
                if (globalTools.Regx("^[0]{1}[1]{1}[0-9]*", data.callee)) {
                    callee = data.callee.substr(1, data.callee.length - 1);
                }
                if (callee === globalTools.trDataList[globalTools.trDataList.length - 1].mobile) {
                    data.call_state = data.call_state.toLocaleUpperCase();
                    if (data.call_state === "FAILED" || data.call_state === "DISCONNECTED" || data.call_state === "CANCEL") {
                        call.overCall.endService();
                        return false;
                    }
                    if (data.call_state === "ANSWERED") {
                        status.isAddRemo = true;
                        return false;
                    }
                }
                switch (data.call_state.toLocaleUpperCase()) {
                    //case "INVITE": case "RINGING": eval("(status.state.state" + callee + " = 'blue')"); break; //振铃为蓝色  
                    case "RINGING": case "ANSWERED": eval("(status.state.state" + callee + " = 'green')"); break; //接听为绿色
                    case "DISCONNECTED": case "FAILED": case "CANCEL": eval("(status.state.state" + callee + " = 'gray')"); break;  //没有在电话中
                }
            });
        }, 3100);
        //}
        //$.connection.hub.start().done(function () { });
    };
    status.uninit = function () { globalTools.duduService.clearList(globalTools.sessionId, function () { }, false); clearInterval(globalTools.pooling); }; //$.connection.hub.stop(); this.isAddRemo = false; this.state = {}; 
    //发起通话
    var call = {
        toCall: {},
        overCall: {}
    };
    call.init = function () {
        call.toCall.init(globalTools.ele.$toCall);
        call.overCall.init(globalTools.ele.$overCall);
    };
    call.toCall.init = function (obj) {
        obj.on("click", function () {
            if (!globalTools.check.isChecked()) { alert("您没有选择任何人！"); return false; }
            if (!confirm("您确定发起通话！")) return false;
            call.toCall.startService();
        });
    };
    call.toCall.startService = function () {
        globalTools.sessionId = 'start';
        var alarm = globalTools.duduService.start(globalTools.trDataList[globalTools.trDataList.length - 1].mobile, globalTools.check.getCheckedPhone(), function (msg) {
            globalTools.sessionId = msg.sessionId;
            status.init();
            //不隐藏checkbox了
            //$("#table-dudu  input[type=checkbox]").each(function () {
            //    $(this).fadeOut(100);
            //});
            //for (var j = 0; j < globalTools.trDataList.length; j++) {
            //    var eleKV = { 'key': globalTools.trDataList[j].mobile, 'val': new DuDuTr($("#" + globalTools.trDataList[j].addBtn), $("#" + globalTools.trDataList[j].removeBtn), $("#" + globalTools.trDataList[j].stateTarget), globalTools.trDataList[j].mobile).quit().participate().getState() };
            //    globalTools.trObjList.push(eleKV);
            //    //if ($("#" + globalTools.trDataList[j].addBtn).siblings("input[type=checkbox]").is(":checked")) {
            //    if ($("input[type=checkbox][data-phone=" + globalTools.trDataList[j].mobile + "]").is(":checked")) {
            //        //$("#" + globalTools.trDataList[j].removeBtn).attr("isRmv", "show").fadeIn(300);
            //        var currTrHtml = $("#m" + globalTools.trDataList[j].mobile).html()
            //        $("#table-dudu").prepend($("#m" + globalTools.trDataList[j].mobile));//将tr对象移到最上
            //        $("#" + globalTools.trDataList[j].removeBtn).attr("isRmv", "show").fadeIn(300);//状态要同步

            //    } else {
            //        $("#" + globalTools.trDataList[j].addBtn).fadeIn(300);
            //    };
            //};
            call.toCall.initTrObj(globalTools.trDataList);//批量实例化通话对象人员
            globalTools.ele.$toCall.fadeOut(function () {
                //输入框不要再隐藏
                //globalTools.ele.$inputDiv.fadeOut(function () {
                //    globalTools.ele.$hidperson.click();
                //});
                globalTools.ele.$overCall.fadeIn(300);
            });
            //*/将所有的通话状态图标显示出来
            $("img[id^=state]").fadeIn(90);
        }, false);
        if (alarm) alert(alarm);
    };
    call.toCall.initTrObj = function (list) {
        for (var j = 0; j < list.length; j++) {
            var eleKV = { 'key': list[j].mobile, 'val': new DuDuTr($("#" + list[j].addBtn), $("#" + list[j].removeBtn), $("#" + list[j].stateTarget), list[j].mobile).quit().participate().getState() };
            globalTools.trObjList.push(eleKV);
            $("#m" + list[j].mobile).find("img[id^=state]").css("display", "block");
            //if ($("#" + list[j].addBtn).siblings("input[type=checkbox]").is(":checked")) {
            if ($("input[type=checkbox][data-phone=" + list[j].mobile + "]").is(":checked")) {
                $("#table-dudu").prepend($("#m" + list[j].mobile));//将tr对象移到最上
                $("#" + list[j].removeBtn).attr("isRmv", "show").fadeIn(300);//状态要同步

            } else {
                $("#" + list[j].addBtn).fadeIn(300);
            };
        };
    };
    call.overCall.init = function (obj) {
        obj.on("click", function () {
            if (!confirm("您确定结束通话！")) return false;
            call.overCall.endService();
        });
    };
    call.overCall.endService = function () {
        globalTools.duduService.end(globalTools.sessionId, function () {
            status.uninit(); globalTools.sessionId = '';
            for (var n = 0; n < globalTools.intervalList.length; n++) {
                clearInterval(globalTools.intervalList[n]);
            }
            for (var p = 0; p < globalTools.trObjList.length; p++) {
                globalTools.trObjList[p].val.participateOff().quitOff().resetState();//电话状态恢复到灰色图标
                globalTools.trObjList[p].val.participateCtrlObj.fadeOut(100);
                globalTools.trObjList[p].val.quitCtrlObj.removeAttr("isRmv").fadeOut(100);
            }
            globalTools.intervalList = [];
            globalTools.trObjList = [];
            $("#table-dudu  input[type=checkbox]").each(function () {
                $(this).fadeIn(300);
            });
            globalTools.ele.$picthNum.text($("#table-dudu tbody input[type=checkbox][id!=chooseAll]:checked").length);
            //globalTools.ele.$showperson.click();
            globalTools.ele.$overCall.fadeOut(function () {
                //globalTools.ele.$hidperson.fadeOut();
                //globalTools.ele.$showperson.fadeOut();
                globalTools.ele.$toCall.fadeIn(300);
                //globalTools.ele.$inputDiv.fadeIn(300);
            });
            $("img[id^=state]").fadeOut(90);//隐藏通话图标
        });
    };
    //UI操作对象
    var DuDuTr = function ($participateCtrlObj, $quitCtrlObj, $stateCtrlObj, phone) {
        this.participateCtrlObj = $participateCtrlObj;
        this.quitCtrlObj = $quitCtrlObj;
        this.$stateCtrlObj = $stateCtrlObj;
        this.phone = phone;
    };
    DuDuTr.prototype.participateActionShow = function (participateCtrlObj, quitCtrlObj) {
        participateCtrlObj.fadeOut(100);
        quitCtrlObj.attr("isRmv", "show").fadeIn(300);
        globalTools.ele.$picthNum.text(parseInt(globalTools.ele.$picthNum.text()) + 1);
    };
    DuDuTr.prototype.quitActionShow = function (participateCtrlObj, quitCtrlObj) {
        quitCtrlObj.removeAttr("isRmv").fadeOut(100);
        participateCtrlObj.fadeIn(300);
        globalTools.ele.$picthNum.text(parseInt(globalTools.ele.$picthNum.text()) - 1);
    };
    DuDuTr.prototype.onCtrl = function (action, alarm) {
        var participateCtrlObjTmp = this.participateCtrlObj;
        var quitCtrlObjTmp = this.quitCtrlObj;
        var phoneTmp = this.phone;
        var participateActionShowTmp = this.participateActionShow;
        var quitActionShowTmp = this.quitActionShow;
        eval("(" + action + "CtrlObjTmp" + ")").on("click", function () {
            if (!status.isAddRemo) { alert("通话尚未开始无法进行此操作！"); return false; }
            if (!confirm(alarm)) return false;
            eval("(" + "globalTools.duduService." + action + ")")(globalTools.sessionId, phoneTmp, function (msg) {
                if (action === "participate") {
                    participateActionShowTmp(participateCtrlObjTmp, quitCtrlObjTmp);
                }
                if (action === "quit") {
                    quitActionShowTmp(participateCtrlObjTmp, quitCtrlObjTmp);
                };
            });
        });
    };
    DuDuTr.prototype.offCtrl = function (action) {
        if (action === "participateOff")
            this.participateCtrlObj.off("click");
        else if (action === "quitOff")
            this.quitCtrlObj.off("click");
    }
    DuDuTr.prototype.participate = function () {
        this.onCtrl("participate", "您确认将此人加入通话吗？");
        return this;
    };
    DuDuTr.prototype.participateOff = function () {
        this.offCtrl("participateOff");
        return this;
    };
    DuDuTr.prototype.quit = function () {
        this.onCtrl("quit", "您确认将此人移除通话吗？");
        return this;
    };
    DuDuTr.prototype.quitOff = function () {
        this.offCtrl("quitOff");
        return this;
    };
    DuDuTr.prototype.getState = function () {
        var phoneTmp = this.phone;
        var stateCtrlObjTmp = this.$stateCtrlObj;
        globalTools.intervalList.push(setInterval(function () {
            var retState = eval("(" + "status.state.state" + phoneTmp + ")");
            //stateCtrlObjTmp.css("background-color", retState);
            if (retState == "") return false;
            stateCtrlObjTmp.prop("src", "../../Images/" + retState + ".png");
        }, 1000));
        return this;
    };
    DuDuTr.prototype.resetState = function () {
        var phoneTmp = this.phone;
        var stateCtrlObjTmp = this.$stateCtrlObj;
        eval("(status.state.state" + phoneTmp + " = 'gray' )");
        //stateCtrlObjTmp.css("background-color", "gray");
        stateCtrlObjTmp.prop("src", "../../Images/gray.png");
        return this;
    };
    window.duduShowDialog = globalTools.duduShowDialog;
    $(function () {
        call.init();
        other.init();
        $("#dudu").mousedown(function () {
            document.getElementById('contactDataList').parentNode.style.display = "none";//隐藏自动完成框
        });
    });

    /****************************************autoCompleteDuDu.js*****************************************/

    var duduService = new DuDuService();
    window.autoComplete = {
        data: {},
        target: "",
        pop_len: 10,
        pop_cn: 'autoDis',
        hover_cn: 'cur',
        //sourceData: ['12341321@@@13612345564@@@324908798', '马宏徐@@@13612345564@@@部门1', '魏yin@@@13825646464@@@航天', '李四@@@13412236054@@@倒垃圾', '吼吼@@@13012348564@@@沙漠之鹰', '哈哈@@@13012345564@@@Ak47', '呵呵@@@13012365564@@@座山雕'],
        init: function () {
            this.setDom();
            return this;
        },
        bind: function (x) {
            target = x = document.getElementById(x);
            if (x.getAttribute('type') != 'text' || x.nodeName != 'INPUT')
                return null;
            var self = this;
            x.onkeyup = function (e) {
                e = e || window.event;
                var lis = self.pop.getElementsByTagName('li'), lens = self.pop.getElementsByTagName('li').length, n = lens, temp;
                if (e.keyCode == 38) { //键盘up键被按下
                    if (self.pop.style.display != 'none') {
                        for (var i = 0; i < lens; i++) { //遍历结果数据，判断是否被选中
                            if (lis[i].className)
                                temp = i;
                            else
                                n--;
                        }
                        if (n == 0) { //如果没有被选中的li元素，则选中最后一个
                            lis[lens - 1].className = self.hover_cn;
                            //this.value = lis[lens - 1].innerHTML;
                        } else { //如果有被选中的元素，则选择上一个元素并赋值给input
                            if (lis[temp] == lis[0]) { //如果选中的元素是第一个孩子节点则跳到最后一个选中
                                lis[lens - 1].className = self.hover_cn;
                                //this.value = lis[lens - 1].innerHTML;
                                lis[temp].className = '';
                            } else {
                                lis[temp - 1].className = self.hover_cn;
                                //this.value = lis[temp - 1].innerHTML;
                                lis[temp].className = '';
                            }
                        }
                    } else //如果弹出层没有显示则执行插入操作，并显示弹出层
                        self.insert(this);
                } else if (e.keyCode == 40) { //down键被按下，原理同up键
                    if (self.pop.style.display != 'none') {
                        for (var i = 0; i < lens; i++) {
                            if (lis[i].className)
                                temp = i;
                            else
                                n--;
                        }
                        if (n == 0) {
                            lis[0].className = self.hover_cn;
                            //this.value = lis[0].innerHTML;
                        } else {
                            if (lis[temp] == lis[lens - 1]) {
                                lis[0].className = self.hover_cn;
                                //this.value = lis[0].innerHTML;
                                lis[temp].className = '';
                            } else {
                                lis[temp + 1].className = self.hover_cn;
                                //this.value = lis[temp + 1].innerHTML;
                                lis[temp].className = '';
                            }
                        }
                    } else
                        self.insert(this);
                } else //如果按下的键既不是up又不是down那么直接去匹配数据并插入
                    self.insert(this);
            };
            //x.onblur = function () { //绑定目标文本框的失去焦点延迟隐藏处理
            //    if (!x.value)
            //        setTimeout(function () { self.pop.style.display = 'none'; }, 110);
            //};
            return this;
        },
        setDom: function () {
            var self = this;
            var dom = document.createElement('div'), frame = document.createElement('iframe'), ul = document.createElement('ul')

            frame.id = "contactDataList";
            document.body.appendChild(dom);
            with (frame) { //用来在ie6下遮住select元素
                setAttribute('frameborder', '0');
                setAttribute('scrolling', 'no');
                style.cssText = 'z-index:-1;position:absolute;left:0;top:0;'
            }
            with (dom) { //对弹出层li元素绑定onmouseover，onmouseover
                className = this.pop_cn;
                appendChild(frame);
                appendChild(ul);
                onmouseover = function (e) { //在li元素还没有加载的时候就绑定这个方法，通过判断target是否是li元素进行处理
                    e = e || window.event;
                    var target = e.srcElement || e.target;
                    //console.log(target.parentNode.tagName);
                    if (target.tagName == 'LI' || target.tagName == 'INPUT' || target.tagName == 'SPAN') { //（返回值始终是大写）添加样式前先把所有的li样式去掉，也可以单独写removeClass方法
                        for (var i = 0, lis = self.pop.getElementsByTagName('li') ; i < lis.length; i++)
                            lis[i].className = '';
                        if (target.tagName != 'INPUT' && target.tagName != 'SPAN') {
                            target.className = self.hover_cn; //也没有写addClass方法，直接赋值了
                        } else {
                            target.parentNode.className = self.hover_cn; //鼠标移到checkbox上时Li也要监听变色
                        }
                    }
                };
                onmouseout = function (e) {
                    e = e || window.event;
                    var target = e.srcElement || e.target;
                    if (target.tagName == 'LI')
                        target.className = '';
                };
            }
            this.pop = dom;
        },
        insert: function (self) {
            //bak = [], s, li = [], left = 0, top = 0,
            var val = self.value, regNum = new RegExp('^[0-9]*$'), regStr = new RegExp('[a-zA-Z\u4E00-\u9FA5\uF900-\uFA2D]');
            if (!!val && val.match(regNum) && val.length >= 4) {
                this.getPersonList(this, self, 1, val);
                //for (var i = 0; i < this.source.length; i++) { //判断input的数据是否与数据源里的数据一致
                //    bak.push(this.source[i]);
                //}
            } else if (!!val && val.match(regStr) && val.length >= 2) {
                this.getPersonList(this, self, 0, val);
            }
        },
        getPersonList: function (currThis, currSelf, type, users) {
            var currObj = currThis;
            var self = currSelf;
            duduService.getUsers(type, users, function (data) {
                var bak = [], s, li = [], left = 0, top = 0;
                if (data == "" || data.indexOf("<html ") > -1) {
                    currObj.pop.style.display = 'none'; //如果没有匹配的数据则隐藏弹出层
                    return false;
                }
                bak = data.split(',');

                //if (bak.length == 0) { //如果没有匹配的数据则隐藏弹出层
                //    currObj.pop.style.display = 'none';
                //    return false;
                //}
                //使用offsetParent，发现ie跟ff下差别很大（可能是自己的问题），之后改用getBoundingClientRect
                left = self.getBoundingClientRect().left + document.documentElement.scrollLeft;
                top = self.getBoundingClientRect().top + document.documentElement.scrollTop + self.offsetHeight;
                with (currObj.pop) {
                    style.cssText = 'width:' + self.offsetWidth + 'px;' + 'position:absolute;left:' + left + 'px;top:' + parseInt(top + 3) + 'px;display:none;';
                    getElementsByTagName('iframe')[0].setAttribute('width', self.offsetWidth);
                    getElementsByTagName('iframe')[0].setAttribute('height', self.offsetHeight);
                    onclick = function (e) {
                        /*单选处理
                                 //    e = e || window.event;
                                 //    var target = e.srcElement || e.target;
                                 //    if (target.tagName == 'LI')
                                 //        self.value = target.innerHTML;
                                 //    this.style.display = 'none';
                                 //*/
                        e = e || window.event;
                        var target = e.srcElement || e.target;
                        if (target.tagName == "LI") {
                            if (target.getElementsByTagName('input')[0].checked) {
                                target.getElementsByTagName('input')[0].checked = false;
                                delete currObj.data[target.getElementsByTagName("span")[1].innerText];
                            }
                            else {
                                target.getElementsByTagName('input')[0].checked = true;
                                currObj.data[target.getElementsByTagName("span")[1].innerText] = { "name": target.getElementsByTagName("span")[0].innerText, "department": target.getElementsByTagName("span")[2].innerText };
                            }
                        } else if (target.tagName == "SPAN") {
                            if (target.parentNode.getElementsByTagName('input')[0].checked) {
                                target.parentNode.getElementsByTagName('input')[0].checked = false;
                                delete currObj.data[target.parentNode.getElementsByTagName("span")[1].innerText];
                            }
                            else {
                                target.parentNode.getElementsByTagName('input')[0].checked = true;
                                currObj.data[target.parentNode.getElementsByTagName("span")[1].innerText] = { "name": target.parentNode.getElementsByTagName("span")[0].innerText, "department": target.parentNode.getElementsByTagName("span")[2].innerText };
                            }
                        } else {
                            if (target.checked) {
                                currObj.data[target.parentNode.getElementsByTagName("span")[1].innerText] = { "name": target.parentNode.getElementsByTagName("span")[0].innerText, "department": target.parentNode.getElementsByTagName("span")[2].innerText };
                            }
                            else {
                                delete currObj.data[target.parentNode.getElementsByTagName("span")[1].innerText];
                            }
                        }
                    }//单机li和checkbox后的事件处理
                }
                //}//注册span的点击事件
                s = bak.length > currObj.pop_len ? currObj.pop_len : bak.length;
                for (var i = 0; i < s; i++) {
                    var tmpVal = bak[i].split('|#@@@#|');
                    li.push('<li><input phone="' + tmpVal[1] + '" style="vertical-align:middle; margin-top:0;margin-right:4px;" type="checkbox" /><span id="name" style="margin-left:16px; width:90px;">' + tmpVal[0] + '</span><span id="phone" style="margin-left:5px;">' + tmpVal[1] + '</span><span  id="department" style="display:none;">' + tmpVal[2] + '</span></li>');
                }
                currObj.pop.getElementsByTagName('ul')[0].innerHTML = li.join('');
                currObj.pop.style.display = 'block';
            });
        },
        getData: function () {
            setTimeout(function () { document.getElementById('contactDataList').parentNode.style.display = "none"; }, 310);
            return this.data;
        }
    };
    autoComplete.init().bind('addPhone');
});
// lib/handlebars/base.js
var Handlebars = {};

Handlebars.VERSION = "1.0.beta.6";

Handlebars.helpers  = {};
Handlebars.partials = {};

Handlebars.registerHelper = function(name, fn, inverse) {
  if(inverse) { fn.not = inverse; }
  this.helpers[name] = fn;
};

Handlebars.registerPartial = function(name, str) {
  this.partials[name] = str;
};

Handlebars.registerHelper('helperMissing', function(arg) {
  if(arguments.length === 2) {
    return undefined;
  } else {
    throw new Error("Could not find property '" + arg + "'");
  }
});

var toString = Object.prototype.toString, functionType = "[object Function]";

Handlebars.registerHelper('blockHelperMissing', function(context, options) {
  var inverse = options.inverse || function() {}, fn = options.fn;


  var ret = "";
  var type = toString.call(context);

  if(type === functionType) { context = context.call(this); }

  if(context === true) {
    return fn(this);
  } else if(context === false || context == null) {
    return inverse(this);
  } else if(type === "[object Array]") {
    if(context.length > 0) {
      for(var i=0, j=context.length; i<j; i++) {
        ret = ret + fn(context[i]);
      }
    } else {
      ret = inverse(this);
    }
    return ret;
  } else {
    return fn(context);
  }
});

Handlebars.registerHelper('each', function(context, options) {
  var fn = options.fn, inverse = options.inverse;
  var ret = "";

  if(context && context.length > 0) {
    for(var i=0, j=context.length; i<j; i++) {
      ret = ret + fn(context[i]);
    }
  } else {
    ret = inverse(this);
  }
  return ret;
});

Handlebars.registerHelper('if', function(context, options) {
  var type = toString.call(context);
  if(type === functionType) { context = context.call(this); }

  if(!context || Handlebars.Utils.isEmpty(context)) {
    return options.inverse(this);
  } else {
    return options.fn(this);
  }
});

Handlebars.registerHelper('unless', function(context, options) {
  var fn = options.fn, inverse = options.inverse;
  options.fn = inverse;
  options.inverse = fn;

  return Handlebars.helpers['if'].call(this, context, options);
});

Handlebars.registerHelper('with', function(context, options) {
  return options.fn(context);
});

Handlebars.registerHelper('log', function(context) {
  Handlebars.log(context);
});
;
