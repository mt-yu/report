var ResourceDataManager = {};  


/**
* TabPanel 标签的操作集合
* Mahx
*/
var TabPanelManager = {
    _tabpanel: null,

    init: function () {
        this._tabpanel = new TabPanel({
            renderTo: 'tab',
            width: '100%',
            height: '100%',
            autoResizable: true,
            items: [{
                id: 'toolbarPlugin1', title: ResourceDataManager.get("Home"),
                icon: '../../images/home.png', 
                contentType: 'url',
                url: '../dashboards/index' , 
                closable: false
            }]
        });

        //初始化完成后，把首页内部内容所在的DIv清空掉，使用Tab页所显示的首页内容，不然会存在两部分一样的内容，一部分在Tab页下，一部分是原生的内容
        //$("#portalContent").remove();

        if ($("#SSOReturnUrl").val()) {
            setTimeout(function () {
                TabPanelManager.addUrlTab(encodeURI($("#SSOReturnUrl").val()), "");//===by zzq 增加uri编码，防止uri出现获取错误
            }, 2000);
        }
    },
    getActiveTab: function () {
        if (this._tabpanel != null) {
            return this._tabpanel.getActiveTab();
        }
    },
    getActiveIndex: function () {
        if (this._tabpanel != null) {
            return this._tabpanel.getActiveIndex();
        }
    },
    addUrlTab: function (url, name) {
        if (url.indexOf('../../erp/full:http://') > -1 || url.indexOf('../../erp/full:https://') > -1) {  //通过一个指定标识单独做的url处理 -- weiyina -- 预置清单新增
            url = url.replace(/..\/..\/erp\/full:/, '');
        }
        else if (url.indexOf('erp/full:') > -1) {  //通过一个指定标识单独做的url处理 -- weiyina -- 预置清单新增
            url = url.replace(/erp\/full:/, '') + "?__dbg=true&__curOId=" + $("#OrgId").val();
        }
        menuManager.navigation();
        var tabs = this._tabpanel.tabs;
        var tab = null;
        for (var i = 0; i < tabs.length; i++) {
            if (menuManager.urlCompare.linkEquels(tabs[i].url, url)) {
                tab = tabs[i];
                break;
            }
        }
        if (window.location.href.indexOf("__dbg=true") > -1 && url.indexOf("__dbg=true") == -1) {
            if (url.indexOf("?") != -1) {
                url += "&";
            }
            else{
                url += "?";
            }
            url += "__dbg=true";
        }
        if ($("#OrgId") && url.indexOf("__curOId") == -1) {
            if (url.indexOf("?") != -1) {
                url += "&__curOId=" + $("#OrgId").val();
            } else {
                url += "?__curOId=" + $("#OrgId").val();
            }
        }
        if (tab != null) {
            var frame = window.frames[tab.id + "Frame"];
            if (frame) {
                var _frameWindow = HtmlHelper.getFrameWindow(frame);
                this._tabpanel.kill(this._tabpanel.getTabIndexById(tab.id), true);
                if (_frameWindow.AfterZlayerJsQueue) {
                    var _this = this;
                    function _add() {
                        _this._tabpanel.addTabByMenu(url, name);
                    }
                    _frameWindow.ZlayerCallBackArray.unshift(_add);
                    return;
                }
            }
        }
        this._tabpanel.addTabByMenu(url, name);
    },
    checkTabTitle: function (id) {
        var i = this._tabpanel.getTabIndexById(id.replace("Frame", ""));
        var frame = window.frames[id];
        var tab = this._tabpanel.tabs[i];
        if (frame) {
            var _frameDocument = HtmlHelper.getFrameDocument(frame);
            var title = _frameDocument.title;
            if (!tab.title.text() || tab.title.text() != title) {
                if(title == ''){  //再加一层判断，当title为空时候，重新赋值为之前传参中的name
                    title = tab.title.text();
                }
                this._tabpanel.resetTitle(i, title);
                if (i == this._tabpanel.getActiveIndex()) {
                    window.document.title = tab.title.text();
                }
            }
            //加载完成后更新菜单的宽度 -- 20170426 -- weiyina
            this._tabpanel.update();  
            this._tabpanel.showScroll();
            this._tabpanel.useableScroll();
        }
    },
    getOnUrl: function () {
        //针对于报表中心的另外窗口打开的屏蔽操作 - WY
        var repeatNames = TabPanelManager.getActiveTab().title[0].innerHTML;   //获取title的值
        if (repeatNames != '报表中心') {
            this._tabpanel.getActiveUrl();
        }
    },
    closeAllTab: function () {
        this._tabpanel.killOtherTabs();
    },
    killThis: function () {
        this._tabpanel.killOnThis();
    },
    firseItem: function () {
        this._tabpanel.show(0, false);
        this._tabpanel.changeBg();
        this._tabpanel.moveToLeft();
    },
    selectOptions: function (position) {
        this._tabpanel.show(position, true);
        this._tabpanel.changeBg();
        this._tabpanel.moveSee(position);

    },
    refreshPage: function () {
        var id = this._tabpanel.getActiveTab().id;
        var frame = window.frames[id + "Frame"];
        if (frame) {
            //frame.location.reload();
            // frame.contentWindow.location.reload();
            $F(frame).getFrameWindow().location.href = $F(frame).getFrameWindow().location.href;  //跨浏览器兼容 -- wy
        }
        else {
            DocumentManager.refreshIndex();
        }
    },
    advance: function () {
        var id = this._tabpanel.getActiveTab().id;
        var frame = window.frames[id + "Frame"];
        if (frame) {
            var _frameWindow = HtmlHelper.getFrameWindow(frame);
            _frameWindow.history.forward();
        }
    },
    goback: function () {
        var id = this._tabpanel.getActiveTab().id;
        var frame = window.frames[id + "Frame"];
        if (frame) {
            var _frameWindow = HtmlHelper.getFrameWindow(frame);
            var position = _frameWindow.history.length;
            if (position > 2)
                _frameWindow.history.back();
        }
    },
    frameLoaded: function (id) {
        //    alert(id);
        /*var ProgressGetInstance=top.ProgressHelper.GetInstance();
       ProgressGetInstance.progressSetValue(100);*/
        $("#" + id + "LoadingDiv").hide();
        var frame = window.frames[id + "Frame"];
        if (frame) {
            var _frameWindow = HtmlHelper.getFrameWindow(frame);
            _frameWindow.status = _frameWindow.status;
            //遍历所有tab，为不是当前显示的frame的accesskey增加一些字符，使其不能被识别，只保留当前显示的frame中的accesskey。解决快捷键冲突问题 mahx
            var _frameDocument = HtmlHelper.getFrameDocument(frame);
            var activeId=this._tabpanel.getActiveTab().id;
            $(_frameDocument).find("[accessKey]").each(function (j, item) {
                if (activeId != id) {
                    if ($(item).attr("accessKey").indexOf("^") == -1)
                        $(item).attr("accessKey", "^" + $(item).attr("accessKey"));
                }
                else {
                    $(item).attr("accessKey", $(item).attr("accessKey").replace("^", ""));
                }
            });
            //weiyina为扫码功能新加判断--2016.08.04
            var iframeUrl = _frameWindow.location.href;
            if (iframeUrl.indexOf("scanCode=wya") > 0) {
                window.setTimeout($("#textCode")[0].focus, 1000);
            }
        }
    },
    frameBeforeUnLoad: function (id) {
        //        alert("dded");
        id = id.replace("Frame", "LoadingDiv");
        $("#" + id).show();
    },
    editPersonal: function () {
        var id = this._tabpanel.getActiveTab().id;
        var frame = window.frames[id + "Frame"];
        if (frame) {
            var _frameDocument = HtmlHelper.getFrameDocument(frame);
            $(_frameDocument).find("li[val='Edit:page']").click();
        }
    },
    deletePersonal: function () {
        var id = this._tabpanel.getActiveTab().id;
        var frame = window.frames[id + "Frame"];
        if (frame) {
            var _frameDocument = HtmlHelper.getFrameDocument(frame);
            $(_frameDocument).find("li[val='Reset:page']").click();
        }
    },
    shareToWorkspace: function () {
        var id = this._tabpanel.getActiveTab().id;
        var frame = window.frames[id + "Frame"];
        // 针对于报表中心不共享页面的屏蔽操作 - WY
        var src = TabPanelManager.getActiveTab().url;
        var repeatNames = TabPanelManager.getActiveTab().title[0].innerHTML;
        var reportFlag = true;
        if (repeatNames == '报表中心') {
            reportFlag = false;
        }
        //针对于报表中心不共享页面的屏蔽操作END

        if (frame && reportFlag) {
            //var ref=document.getElementById("RawUrl").value;
            // var ref = frame.GetRealUrl4Fav(src);
            var ref = frame.contentWindow.GetRealUrl4Fav(src);
            var _frameDocument = HtmlHelper.getFrameDocument(frame);
            var title = _frameDocument.title;
            var url = "../../simple.aspx?lnk=bed66a1d-6b11-4ae2-95fc-d91e00c46096" + "&WorkspaceRawUrl=" + escape(ref) + "&title=" + escape(title) + "&chromeType=4"
            ;
            var retv;
            var fea = {
                url: url,
                title: ResourceDataManager.get("Task"),
                width: 598,
                height: 340,
                isHideStatusbar: false,
                isModeless: false
            };
            retv = DocumentManager.showDialog(fea);

            if (retv) {
                retv = "../../" + retv.substring(2);
                TabPanelManager.addUrlTab(retv, "协同工具");
            }
            //return false;
        }
    },
    disponse: function (func) {
        this._tabpanel.checkAllFramesDirtyData(func);
    }
}

/**
* 获取数据的操作集合
* Mahx
*/
var ServiceManager = {
    _messageListForClick: null,
    _messageNotReadIndex: 1,
    _messageAllIndex: 1,
    _messageSize: 6,
    _taskAllIndex: 1,
    _taskNotFinishIndex: 1,
    _taskSize: 6,
    _taskListForClick: null,
    _newsSize: 6,
    setTaskAllIndex: function (index) {
        this._taskAllIndex = index;
    },
    setTaskNotFinishIndex: function (index) {
        this._taskNotFinishIndex = index;
    },
    setTaskSize: function (index) {
        this._taskSize = index;
    },
    setNewsSize: function (index) {
        this._newsSize = index;
    },
    setMessageNotReadIndex: function (index) {
        this._messageNotReadIndex = index;
    },
    setMessageAllIndex: function (index) {
        this._messageAllIndex = index;
    },
    setMessageSize: function (index) {
        this._messageSize = index;
    },
    init: function () {

        this._messageNotReadIndex = 1;
        this._messageAllIndex = 1;
        this._taskAllIndex = 1;
        this._taskNotFinishIndex = 1;
        this._themesList = $("#themesDl");
        this._enterpriseList = $("#enterpriseDl");
        this._orgsList = $("#orgsDl");
        this._languagesList = $("#languagesDl");
        this._boardsList = $("#boardsDiv");
        this._newsList = $("#newsDiv");
        this._messagesList = $("#messagesDiv");
        this._taskList = $("#tasksDiv");

        $("[ulType='messages'] > [litype='0']").attr("class", "select");
        $("[ulType='messages'] > [litype='-1']").removeClass("select")
        $("[ulType='tasks'] > [litype='0']").attr("class", "select");
        $("[ulType='tasks'] > [litype='-1']").removeClass("select")

        //获取各项信息列表 mahx 不再一初始化就加载，而是在半圆点击时才加载数据
        //this.getUnifyInfo();
        //获取消息
        //liyne注释掉this.getMessages(); this.getTasks();因为初始化时多执行了一次
        //this.getMessages(); 
        //获取任务
        //this.getTasks();
        //获取待审批消息 -- weiyina新增，原因是this.getUnifyInfo方法被注释后，里面涉及到的调用msgBox.init(message)的操作没法执行--20180417
        this.toReadMessage();
        //获取公告 -- weiyina放开，lqy测试要用，后续再注释上-20180301
        this.getBoards();
        //获取新闻 -- weiyina放开，lqy测试要用，后续再注释上-20180301
        this.getNews();
        //获取语言
        this.getLauguages();
        //获取是否显示企业空间
        this.getEspace();
        //获取云合同链接
        this.getYHT();
        //获取搜索类型
        this.getSearchCategorys();
        //获取组织
        this.getOrgs();
        this.bindEvent();
    },
    bindEvent: function () {
        var _this = this;
        //切换企业
        $("#enterpriseDl").on("click", "dd", function () {
            var id = $(this).attr("itemId");
            _this.changeEnterprise(id);
        })

        //切换语言
        $("#languagesDl").on("click", "dd", function () {
            var id = $(this).attr("itemId");
            _this.changeLanguages(id);
        })

        //切换样式
        $("#themesDl").on("click", "dd", function () {
            var id = $(this).attr("itemId");
            _this.changeThemes(id);
        })

        //切换组织
        $("#orgsDl").on("click", "dd", function () {
            var id = $(this).attr("itemId");
            // var name = $(this).text();
            var name = $(this).find("span").attr('title');  //组织name因为前一次的补丁中重新拼接了字符串导致这个地方name值获取错误引发的问题，此处name值放到title属性中通过这种方式来获取name -- 216.09.26 -- weiyina
            _this.changeOrg(id, name);
        })


        //历史库查询按钮点击事件
        $("#libraryStatus").click(function () {
            $.post("../main/EnableHistoryQuery", { mt: Math.uuid() }, function (data) {
                if (data == "True") {
                    $("#libraryStatus").html('<i class="fa fa-check fa-sm"></i><span>' + ResourceDataManager.get("Join_History") + '</span>');
                }
                else {
                    $("#libraryStatus").html('<span>' + ResourceDataManager.get("Join_History") + '</span>');
                }
            });
        })
        //知识弹窗
        $("#knowledgeBtn").on('click',function(){
            var id = TabPanelManager.getActiveTab().id;
            var frame = window.top.frames[id + "Frame"];
            if(frame){
                if(helpTool._status == 'off'){
                   helpTool.on(); 
                   $(".portal-header .logo-bar").after("<div class='controlBg1'></div>");
                }else{
                   helpTool.off();  
                }
            }else{
                alert("当前界面不支持知识弹窗");
            }
            
        })
    },
    getEspace: function () {
        $.get("../main/GetThirdInfo", { mt: Math.uuid() }, function (data) {
            if (data) {
                if (data.StatusCode == 200) {  //状态200
                    if (data.IsShowESpace) {
                        $(".logo1").removeClass("hidden");
                    }
                    if (data.U9PLMUrl) {
                        $(".logo2").removeClass("hidden");
                        $(".logo2").on('click', function () {
                            window.open(data.U9PLMUrl, "_blank");
                        })
                    }
                    if (data.U9BlogUrl) {
                        $(".community").removeClass("hidden");
                        $(".community").on('click', function () {
                            window.open(data.U9BlogUrl, "_blank");
                        })
                    }
                }
            }

        });

    },
    getYHT: function () {
        $.get("../yht/IsShowYHTLink", { mt: Math.uuid() }, function (data) {
            if (data) {
                if (data.IsShowYHT) {
                    $(".yht").removeClass("hidden");
                    if (data.IsNeedPwd) {
                        $("#isNeedPwd").val("True");
                    } else {
                        $("#isNeedPwd").val("False");
                    }
                }
            }
        });
    },
    _themesList: $("#themesDl"),
    getThemes: function () {
        var _this = this;
        //_themesList.empty();
        if (!_this._themesList.attr("isShow") || _this._themesList.attr("isShow") != "true") {
            $.get("../Main/Themes", { mt: Math.uuid() }, function (data) {
                _this._themesList.html("");
                if (data && data.Themes) {
                    $(data.Themes).each(function (i, item) {
                        var li = "<dd itemId='" + item.Value + "'><span>" + item.Text + "</span></dd>";
                        if (item.Value == data.SelectedID) {
                            $(this).children("span").html(item.Text);
                            li = "<dd itemId='" + item.Value + "' isSelected='true'><i class='fa fa-check fa-sm'></i><span>" + item.Text + "</span></dd>";
                        }
                        _this._themesList.append(li);
                        _this._themesList.attr("isShow", "true");
                    });
                }
            });
        }
    },
    _enterpriseList: $("#enterpriseDl"),
    getEnterprise: function () {
        var _this = this;
        if (!_this._enterpriseList.attr("isShow") || _this._enterpriseList.attr("isShow") != "true") {
            $.get("../Main/Enterprises", { mt: Math.uuid() }, function (data) {
                _this._enterpriseList.html("");
                if (data && data.Enterprises) {
                    $(data.Enterprises).each(function (i, item) {
                        var li = "<dd itemId='" + item.ID + "'><span>" + item.Name + "</span></dd>";
                        if (item.ID == data.SelectedID) {
                            $(this).children("span").html(item.Name);
                            li = "<dd itemId='" + item.ID + "'><i class='fa fa-check fa-sm'></i><span>" + item.Name + "</span></dd>";
                        }
                        _this._enterpriseList.append(li);
                        _this._enterpriseList.attr("isShow", "true");
                    });
                }
            });
        }
    },
   _orgsList: $('#orgsDl'),
        getOrgs: function() {
            var htm = '';
            var _this = this;
            if (!_this._orgsList.attr("isShow") || _this._orgsList.attr("isShow") != "true") {
                if ($("#userNameLi").text().toLowerCase() == "enterprise administrator"  || $("#userNameLi").text().toLowerCase() == "vp" || $("#userNameLi").text().toLowerCase() == "cp") {
                    return;
                }
                _this._orgsList.html('<dd class="loading">加载中...</dd>');
                var longestItem;
                $.get("../Main/Organizations", {
                    mt: Math.uuid()
                }, function(data) {
                    //组织字数过长问题lyn
                    if (data && data.Organizations) {
                         //小于15条不需要搜索框
                        if($(data.Organizations).length < 15){
                                $("#orgsSearch").remove();
                        }else{
                               //需要搜索框，给搜索框留个空隙
                               $(".listmMenu-zz").css("padding-top","30px")
                        }
                        $(data.Organizations).each(function(i, item) {
                            var codeStr = '';
                            if (item.ID == data.SelectedID) {
                                codeStr = item.Code + "&nbsp;&nbsp;&nbsp;" + item.Name;   //weiyin -- 跨组织部分添加组织名称和组织编码 -- 2016.08.30
                                $(this).children("span").html(codeStr);
                                htm += "<dd itemId='" + item.ID + "'><i class='fa fa-check fa-sm'></i><span title=" + item.Name + ">"+ codeStr +"</span></dd>";
                            } else {
                                codeStr = item.Code + "&nbsp;&nbsp;&nbsp;" + item.Name;   //weiyin -- 跨组织部分添加组织名称和组织编码 -- 2016.08.30
                                htm += "<dd itemId='" + item.ID + "'><span title=" + item.Name + ">"+ codeStr + "</span></dd>";
                            }
                            // 根据数据量来动态分割多列显示 
                            //求字数最长的item
                            longestItem = _this.longestItem(longestItem,item,codeStr);
                        });
                        
                        _this._orgsList.html(htm);
                        _this._orgsList.attr("isShow", "true");
                        //添加临时文件,求宽度用
                        _this.appendTemp(longestItem);
                        //设置组织高度和宽度
                        _this.setOrgsSize(longestItem, data.Organizations);
                        //鼠标放上hover的效果
                        _this.orgsEvent();
                    }
                });
            }

        },
        _tempId: "tempSpan",
        //添加临时文件,求宽度用
        appendTemp: function(longestItem) {
            if(longestItem){
            // weiyin -- 跨组织部分添加组织名称和组织编码 -- 2016.08.30
            var codeStr = longestItem.Code + "&nbsp;&nbsp;&nbsp;" + longestItem.Name;
            if ($("#" + this._tempId).length == 0) {
                $("body").append('<span style="display:none" id=' + this._tempId + '>' + codeStr + '</span>');
            } else {
                $("#" + this._tempId).html(codeStr);
                }
            }
        },
        //汉字两个字节，英文一个字节处理
        strLength: function(str) {
            var len = 0;
            for (var i = 0; i < str.length; i++) {
                if (str.charCodeAt(i) > 127 || str.charCodeAt(i) == 94) {
                    len += 2;
                } else {
                    len++;
                }
            }
            return len;
        },
        //求字数最长的item
        longestItem: function(longestItem,item,codeStr) {
            var itemNameLength=this.strLength(codeStr);
            if (!longestItem) {
                longestItem = item;
            } else if (itemNameLength > this.strLength(longestItem.Code + '&nbsp;&nbsp;&nbsp;' + longestItem.Name)) {
                longestItem = item;
            }
            return longestItem;
        },
        //设置组织高度和宽度
        setOrgsSize: function(longestItem, Organizations) {
            if (longestItem) {
            var _this = this;
            var boxWidth=0;
            //计算宽度
            if (longestItem.ID) {
                var $itemIdLongest = $('[itemid="' + longestItem.ID + '"]');
                boxWidth = $("#" + this._tempId).width() + $itemIdLongest.outerWidth() - $itemIdLongest.width() + 10;
            }
            //最大高度
            if (Organizations.length >= 20 || _this._orgsList.height() > 450) {
                boxWidth = boxWidth + 17;
                    _this._orgsList.css({
                        "height": 450
                    });
                }
                //计算box的总长度
                _this._orgsList.css({
                    "width": boxWidth
                });
            }
        },
        //鼠标放上hover的效果
        orgsEvent: function() {
            this._orgsList.delegate("dd", {
                'mouseover': function() {
                    $(this).addClass("hover");
                },
                'mouseleave': function() {
                    $(this).removeClass("hover");
                }
            });
        },
    
    _languagesList: $("#languagesDl"),
    getLauguages: function () {
        var _this = this;
        if (!_this._languagesList.attr("isShow") || _this._languagesList.attr("isShow") != "true") {
            $.get("../Main/Languages", { mt: Math.uuid() }, function (data) {
                _this._languagesList.html("");
                if (data && data.Languages) {
                    $(data.Languages).each(function (i, item) {
                        var li = "<dd itemId='" + item.Key + "'><span>" + item.Value + "</span></dd>";
                        if (item.Key == data.SelectedID) {
                            $(this).children("span").html(item.Value);
                            li = "<dd itemId='" + item.Key + "' isSelected='true'><i class='fa fa-check fa-sm'></i><span>" + item.Value + "</span></dd>";
                        }
                        _this._languagesList.append(li);
                        _this._languagesList.attr("isShow", "true");
                    });
                }
            });
        }
    },
    _boardsList: $("#boardsDiv"),
    getBoards: function () {
        var _this = this;
        _this._boardsList.html("");
        if (parseInt(this._newsSize) <= 0) return;
        //$.get("../Main/Boards?MaxAmount=" + this._newsSize, { mt: Math.uuid() }, function (data) {
